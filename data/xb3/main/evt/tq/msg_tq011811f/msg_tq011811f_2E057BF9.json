    "label" : "tq011811_msg0015",
    "talker" : "kY1",
    "talkattr" : 33024,
    "talkattr2" : 16513,
    "talkattr3" : 1072,
    "style" : 126,
    "name" : "그래, 그렇다…… \"녀석\"과 같지"
