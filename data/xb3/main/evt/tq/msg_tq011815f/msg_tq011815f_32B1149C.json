    "$id" : 19,
    "label" : "tq011815_msg0019",
    "talker" : "J",
    "talkattr" : 33024,
    "talkattr2" : 16513,
    "talkattr3" : 1072,
    "style" : 126,
    "name" : "아뇨, 그런 건 아닙니다\n이건…… \"약속\"입니다"
