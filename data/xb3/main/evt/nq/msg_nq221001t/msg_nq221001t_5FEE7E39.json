    "$id" : 4,
    "label" : "nq221001t_msg0004",
    "talker" : "_c1",
    "talkattr" : 33024,
    "talkattr2" : 16513,
    "talkattr3" : 1072,
    "style" : 126,
    "name" : "맞아, 의식은 필요하니까"
