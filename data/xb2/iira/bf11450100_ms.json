    },
    {
      "$id": 6,
      "label": "bf11450100_0060",
      "style": 82,
      "name": "하지만 이게 정말로 \"신\"의 말씀인가?"
    },
    {
      "$id": 7,
      "label": "bf11450100_0070",
      "style": 82,
      "name": "\"신\"에 의한 구원이라고?"
    },
    {
      "$id": 8,
      "label": "bf11450100_0080",
      "style": 82,
      "name": "이런 게 어딜 봐서 구원인가"
