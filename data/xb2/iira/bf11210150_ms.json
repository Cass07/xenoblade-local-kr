  "rows": [
    {
      "$id": 1,
      "label": "bf11210150_0010",
      "style": 82,
      "name": "오랜만입니다, 아델 왕자\n부하의 무례를 부디 용서해 주시길"
    },
    {
      "$id": 2,
      "label": "bf11210150_0020",
      "style": 82,
      "name": "뭘, 괜찮아"
    },
    {
      "$id": 3,
      "label": "bf11210150_0022",
      "style": 82,
      "name": "건강해 보여서 다행이야, 유고 황제 폐하"
    },
    {
      "$id": 4,
      "label": "bf11210150_0030",
      "style": 82,
      "name": "유고… 황제 폐하"
    },
    {
      "$id": 5,
      "label": "bf11210150_0040",
      "style": 82,
      "name": "소개하지…"
    },
    {
      "$id": 6,
      "label": "bf11210150_0050",
      "style": 82,
      "name": "수페르비아 제국 황제, 유고 엘 수페르비아 폐하다"
    },
    {
      "$id": 7,
      "label": "bf11210150_0060",
      "style": 82,
      "name": "수페르비아의 황제…"
    },
    {
      "$id": 8,
      "label": "bf11210150_0070",
      "style": 82,
      "name": "그런 높은 분이 왜 이런 곳에?"
    },
    {
      "$id": 9,
      "label": "bf11210150_0080",
      "style": 82,
      "name": "나도 입장만 놓고 보면 비슷한데…"
    },
    {
      "$id": 10,
      "label": "bf11210150_0090",
      "style": 82,
      "name": "그렇게 생각도 안 하면서"
    },
    {
      "$id": 11,
      "label": "bf11210150_0100",
      "style": 82,
      "name": "역시 그래 보이나?"
    },
    {
      "$id": 12,
      "label": "bf11210150_0110",
      "style": 82,
      "name": "하늘의 성배입니다"
    },
    {
      "$id": 13,
      "label": "bf11210150_0120",
      "style": 82,
      "name": "수페르비아에서도 피해가 나오기 시작해서요\n그걸 쫓고 있던 중이었습니다"
    },
    {
      "$id": 14,
      "label": "bf11210150_0130",
      "style": 82,
      "name": "우리랑 같네"
    },
    {
      "$id": 15,
      "label": "bf11210150_0140",
      "style": 82,
      "name": "하지만 네가 직접 나오다니…"
    },
    {
      "$id": 16,
      "label": "bf11210150_0150",
      "style": 82,
      "name": "본국은 형님께 맡겨 두었습니다"
    },
    {
      "$id": 17,
      "label": "bf11210150_0160",
      "style": 82,
      "name": "그렇군…\n그래서 카구츠치와 와다츠미를…"
    },
    {
      "$id": 18,
      "label": "bf11210150_0170",
      "style": 82,
      "name": "[ML:undisp ]"
    },
    {
      "$id": 19,
      "label": "bf11210150_0180",
      "style": 82,
      "name": "카구츠치와 와다츠미는 제국의 보배…\n황가의 상징이야"
    },
    {
      "$id": 20,
      "label": "bf11210150_0190",
      "style": 82,
      "name": "그들을 거느리고 최전선에서 싸우는 것이\n황위를 이은 자의 책임이지"
    },
    {
      "$id": 21,
      "label": "bf11210150_0200",
      "style": 82,
      "name": "원래는 형님께서 황위를 이을 예정이었죠"
    },
    {
      "$id": 22,
      "label": "bf11210150_0210",
      "style": 82,
      "name": "하지만 네 형님에겐 드라이버 적성이 없었어"
    },
    {
      "$id": 23,
      "label": "bf11210150_0220",
      "style": 82,
      "name": "그리고 하필이면 네가"
    },
    {
      "$id": 24,
      "label": "bf11210150_0222",
      "style": 82,
      "name": "그들과 동조해 버렸지…"
    },
    {
      "$id": 25,
      "label": "bf11210150_0230",
      "style": 82,
      "name": "특별 집권관… 형님의 직속 부하입니다"
    },
    {
      "$id": 26,
      "label": "bf11210150_0240",
      "style": 82,
      "name": "오호"
    },
    {
      "$id": 27,
      "label": "bf11210150_0250",
      "style": 82,
      "name": "제가 전장에서 죽으면\n두 블레이드의 코어를 회수하라는 명령을 받았죠"
    },
    {
      "$id": 28,
      "label": "bf11210150_0260",
      "style": 82,
      "name": "가족인데도?"
    },
    {
      "$id": 29,
      "label": "bf11210150_0270",
      "style": 82,
      "name": "위정자라는 건 그런 자리야"
    },
    {
      "$id": 30,
      "label": "bf11210150_0280",
      "style": 82,
      "name": "하지만 말입니다"
    },
    {
      "$id": 31,
      "label": "bf11210150_0282",
      "style": 82,
      "name": "차라리 잘됐다고 생각합니다"
    },
    {
      "$id": 32,
      "label": "bf11210150_0290",
      "style": 82,
      "name": "국민을 지키기 위한 싸움에 참여할 수 있다면 족해요"
    },
    {
      "$id": 33,
      "label": "bf11210150_0300",
      "style": 82,
      "name": "애초에 권력을 휘두르는 건 좋아하지도 않고요"
    },
    {
      "$id": 34,
      "label": "bf11210150_0310",
      "style": 82,
      "name": "정말 동감이야\n옥좌에 앉으려는 녀석들의 마음을 모르겠어"
    },
    {
      "$id": 35,
      "label": "bf11210150_0320",
      "style": 82,
      "name": "그거, 제타 전하가 들으면\n그냥 넘어가진 않을걸요?"
    },
    {
      "$id": 36,
      "label": "bf11210150_0330",
      "style": 82,
      "name": "이런, 방금 건 못 들은 걸로 해 줘"
    },
    {
      "$id": 37,
      "label": "bf11210150_0340",
      "style": 82,
      "name": "서로 뜻대로 되지 않네요\n가만히 놔두기만 해도 좋을 텐데"
    },
    {
      "$id": 38,
      "label": "bf11210150_0350",
      "style": 82,
      "name": "사람이라는 존재를 두려워하는 거야"
    },
    {
      "$id": 39,
      "label": "bf11210150_0360",
      "style": 82,
      "name": "정체 모를 괴물 같은 걸로 보이나 봐"
    },
    {
      "$id": 40,
      "label": "bf11210150_0370",
      "style": 82,
      "name": "그게 자기를 비추는 거울이라는 것도 모르고 말이야…"
    },
    {
      "$id": 41,
      "label": "bf11210150_0380",
      "style": 82,
      "name": "아델 왕자"
    },
    {
      "$id": 42,
      "label": "bf11210150_0382",
      "style": 82,
      "name": "괜찮다면 옛날처럼 유고라고 불러 주시죠"
    },
    {
      "$id": 43,
      "label": "bf11210150_0390",
      "style": 82,
      "name": "알았어, 유고\n그럼 나도 아델이라고 불러"
    },
    {
      "$id": 44,
      "label": "bf11210150_0400",
      "style": 82,
      "name": "이제 서로 정보를 교환하고 싶은데\n장소를 바꾸는 게 좋을 것 같군"
    },
    {
      "$id": 45,
      "label": "bf11210150_0410",
      "style": 82,
      "name": "폐하의 배인 제왕선은 어떠신지요"
    },
    {
      "$id": 46,
      "label": "bf11210150_0415",
      "style": 82,
      "name": "그게 좋겠어"
    },
    {
      "$id": 47,
      "label": "bf11210150_0420",
      "style": 82,
      "name": "제왕선?"
    },
    {
      "$id": 48,
      "label": "bf11210150_0430",
      "style": 82,
      "name": "왜 그러세요? 라우라 님?"
    },
    {
      "$id": 49,
      "label": "bf11210150_0440",
      "style": 82,
      "name": "나… 그런 딱딱한 장소\n별로 안 좋아하는데"
    },
    {
      "$id": 50,
      "label": "bf11210150_0450",
      "style": 82,
      "name": "아~ 알 거 같아, 나도 그렇거든"
    },
    {
      "$id": 51,
      "label": "bf11210150_0460",
      "style": 82,
      "name": "그렇지?"
    },
    {
      "$id": 52,
      "label": "bf11210150_0462",
      "style": 82,
      "name": "그러니까\n나는 거절할게"
    },
    {
      "$id": 53,
      "label": "bf11210150_0470",
      "style": 82,
      "name": "그럼 나도…"
    },
    {
      "$id": 54,
      "label": "bf11210150_0480",
      "style": 82,
      "name": "너는 안 돼"
    },
    {
      "$id": 55,
      "label": "bf11210150_0490",
      "style": 82,
      "name": "혹시 이분이 소문의?"
    },
    {
      "$id": 56,
      "label": "bf11210150_0500",
      "style": 82,
      "name": "그래, 또 다른 하늘의 성배\n히카리야"
    },
    {
      "$id": 57,
      "label": "bf11210150_0510",
      "style": 82,
      "name": "[ML:undisp ]"
    },
    {
      "$id": 58,
      "label": "bf11210150_0520",
      "style": 82,
      "name": "원래는 라우라의 이야기도 들려주고 싶었는데…"
    },
    {
      "$id": 59,
      "label": "bf11210150_0530",
      "style": 82,
      "name": "내가 보고 들은 건\n신이 모두 알고 있어"
    },
    {
      "$id": 60,
      "label": "bf11210150_0540",
      "style": 82,
      "name": "아, 그래, 그렇긴 하지…"
    },
    {
      "$id": 61,
      "label": "bf11210150_0550",
      "style": 82,
      "name": "부탁해, 나 대신 다녀와 줘"
    },
    {
      "$id": 62,
      "label": "bf11210150_0560",
      "style": 82,
      "name": "알겠습니다, 제가 동행하죠"
    },
    {
      "$id": 63,
      "label": "bf11210150_0570",
      "style": 82,
      "name": "잘 부탁합니다"
    },
    {
      "$id": 64,
      "label": "bf11210150_0572",
      "style": 82,
      "name": "그럼 가죠"
    }
  ]
}