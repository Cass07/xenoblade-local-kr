  "rows": [
    {
      "$id": 1,
      "label": "campfev11_000_000_msg0001",
      "style": 84,
      "name": "라우라, 조금 전의 전투 말인데…"
    },
    {
      "$id": 2,
      "label": "campfev11_000_000_msg0002",
      "style": 84,
      "name": "나 잘 싸웠지?"
    },
    {
      "$id": 3,
      "label": "campfev11_000_000_msg0003",
      "style": 84,
      "name": "너무 무모하게 굴지 마"
    },
    {
      "$id": 4,
      "label": "campfev11_000_000_msg0004",
      "style": 84,
      "name": "에이~ 칭찬 안 해 주네…"
    },
    {
      "$id": 5,
      "label": "campfev11_000_000_msg0005",
      "style": 84,
      "name": "신의 힘을\n잘 끌어냈다고 생각했는데"
    },
    {
      "$id": 6,
      "label": "campfev11_000_000_msg0006",
      "style": 84,
      "name": "그건 문제없지만\n라우라가 위험에 처하는 상황을\n피하고 싶다는 뜻이야"
    },
    {
      "$id": 7,
      "label": "campfev11_000_000_msg0007",
      "style": 84,
      "name": "나랑 신은 오랫동안\n함께 지내 왔잖아?"
    },
    {
      "$id": 8,
      "label": "campfev11_000_000_msg0008",
      "style": 84,
      "name": "어릴 때, 코어 크리스털을 만져서\n신과 동조하고\n드라이버가 된 후로 지금까지"
    },
    {
      "$id": 9,
      "label": "campfev11_000_000_msg0009",
      "style": 84,
      "name": "그래…"
    },
    {
      "$id": 10,
      "label": "campfev11_000_000_msg0010",
      "style": 84,
      "name": "내게 드라이버와 블레이드는\n가족 그 이상의 관계야"
    },
    {
      "$id": 11,
      "label": "campfev11_000_000_msg0011",
      "style": 84,
      "name": "그러니까 나도 도와주고 싶어…\n신의 힘이 되고 싶어"
    },
    {
      "$id": 12,
      "label": "campfev11_000_000_msg0012",
      "style": 84,
      "name": "하지만 라우라는 드라이버고\n나는 블레이드야"
    },
    {
      "$id": 13,
      "label": "campfev11_000_000_msg0013",
      "style": 84,
      "name": "블레이드는 당연히 드라이버를\n지키면서 싸워야 해"
    },
    {
      "$id": 14,
      "label": "campfev11_000_000_msg0014",
      "style": 84,
      "name": "상처를 입는 건 아무렇지도 않아\n코어 크리스털이 부서지지 않는 한"
    },
    {
      "$id": 15,
      "label": "campfev11_000_000_msg0015",
      "style": 84,
      "name": "그에 비해 나는 평범한 인간이지"
    },
    {
      "$id": 16,
      "label": "campfev11_000_000_msg0016",
      "style": 84,
      "name": "드라이버가 죽으면\n블레이드도 코어 크리스털로 돌아가고…"
    },
    {
      "$id": 17,
      "label": "campfev11_000_000_msg0017",
      "style": 84,
      "name": "그러니까 신중하게 싸우라는 말이지?"
    },
    {
      "$id": 18,
      "label": "campfev11_000_000_msg0018",
      "style": 84,
      "name": "맞아"
    },
    {
      "$id": 19,
      "label": "campfev11_000_000_msg0019",
      "style": 84,
      "name": "음~ 그렇구나…"
    },
    {
      "$id": 20,
      "label": "campfev11_000_000_msg0020",
      "style": 84,
      "name": "나도 딱히 신을\n불안하게 하고 싶진 않으니까…"
    },
    {
      "$id": 21,
      "label": "campfev11_000_000_msg0021",
      "style": 84,
      "name": "너무 무리하지 않겠다고 약속할게\n위험해지면 신하고 교대할 테니까"
    },
    {
      "$id": 22,
      "label": "campfev11_000_000_msg0022",
      "style": 84,
      "name": "그래, 그렇게 하자"
    },
    {
      "$id": 23,
      "label": "campfev11_000_000_msg0023",
      "style": 84,
      "name": "너만 믿을게"
    },
    {
      "$id": 24,
      "label": "campfev11_000_000_msg0024",
      "style": 84,
      "name": "알았어\n맡겨 줘"
    }
  ]
}