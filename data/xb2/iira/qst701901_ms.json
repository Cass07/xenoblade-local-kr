    {
      "name": "name",
      "type": 7
    }
  ],
  "rows": [
    {
      "$id": 1,
      "label": "qst701901_msg0001",
      "style": 84,
      "name": "하아…"
    },
    {
      "$id": 2,
      "label": "qst701901_msg0002",
      "style": 84,
      "name": "대체 어떻게 해야…"
    },
    {
      "$id": 3,
      "label": "qst701901_msg0003",
      "style": 84,
      "name": "왜 그래?"
    },
    {
      "$id": 4,
      "label": "qst701901_msg0004",
      "style": 84,
      "name": "라우라 님, 아델 님, 유고 님…"
    },
    {
      "$id": 5,
      "label": "qst701901_msg0005",
      "style": 84,
      "name": "괜찮으시면 들어 주시겠어요?"
    },
    {
      "$id": 6,
      "label": "qst701901_msg0006",
      "style": 84,
      "name": "물론이지"
    },
    {
      "$id": 7,
      "label": "qst701901_msg0007",
      "style": 84,
      "name": "제 딸인 플리아가\n정말 심각한 병에 걸렸어요"
    },
    {
      "$id": 8,
      "label": "qst701901_msg0008",
      "style": 84,
      "name": "워타 씨가 마련해 주신\n아르스선에도 초대해 주셨는데\n배 여행은 버틸 수 없을 거예요"
    },
    {
      "$id": 9,
      "label": "qst701901_msg0009",
      "style": 84,
      "name": "항구로 갈 체력도 없을 정도라…"
    },
    {
      "$id": 10,
      "label": "qst701901_msg0010",
      "style": 84,
      "name": "그리고 프리드 씨의 여인숙 지하에도\n초대를 받았는데요…"
    },
    {
      "$id": 11,
      "label": "qst701901_msg0011",
      "style": 84,
      "name": "사람이 많은 곳이나 공기가 안 좋은 곳도\n무리거든요…"
    },
    {
      "$id": 12,
      "label": "qst701901_msg0012",
      "style": 84,
      "name": "그래서 어떻게 그 아이를 지킬 수 있을지\n모르겠어서…"
    },
    {
      "$id": 13,
      "label": "qst701901_msg0013",
      "style": 84,
      "name": "그렇군요…"
    },
    {
      "$id": 14,
      "label": "qst701901_msg0014",
      "style": 84,
      "name": "우리는 반드시 이길 거니까 안심해!\n…라고 말하고 싶지만"
    },
    {
      "$id": 15,
      "label": "qst701901_msg0015",
      "style": 84,
      "name": "만약을 위해 대책을 세우고 싶은 건\n부모로서 당연한 일이겠지"
    },
    {
      "$id": 16,
      "label": "qst701901_msg0016",
      "style": 84,
      "name": "플리아!\n드디어 완성됐어!!"
    },
    {
      "$id": 17,
      "label": "qst701901_msg0017",
      "style": 84,
      "name": "뭐가…?"
    },
    {
      "$id": 18,
      "label": "qst701901_msg0018",
      "style": 84,
      "name": "약 말이야!\n플리아의 병이 낫는 약!"
    },
    {
      "$id": 19,
      "label": "qst701901_msg0019",
      "style": 84,
      "name": "내가 엄청 노력해서\n만든 거야"
    },
    {
      "$id": 20,
      "label": "qst701901_msg0020",
      "style": 84,
      "name": "다나스컬러를 100마리 모은 다음\n그 진액을 한 방울씩 추출해서 만든\n특효약이지!"
    },
    {
      "$id": 21,
      "label": "qst701901_msg0021",
      "style": 84,
      "name": "다나스컬러가 뭐야?"
    },
    {
      "$id": 22,
      "label": "qst701901_msg0022",
      "style": 84,
      "name": "다나 사막에 있는 귀중한 곤충이야"
    },
    {
      "$id": 23,
      "label": "qst701901_msg0023",
      "style": 84,
      "name": "벌레는 싫어!!"
    },
    {
      "$id": 24,
      "label": "qst701901_msg0024",
      "style": 84,
      "name": "하지만 마르코가 만든 약을 먹으면\n함께 바깥에서 놀 수 있어"
    },
    {
      "$id": 25,
      "label": "qst701901_msg0025",
      "style": 84,
      "name": "부탁이야, 조금만이라도 좋으니까 먹어 봐!"
    },
    {
      "$id": 26,
      "label": "qst701901_msg0026",
      "style": 84,
      "name": "……"
    },
    {
      "$id": 27,
      "label": "qst701901_msg0027",
      "style": 84,
      "name": "알았어…"
    },
    {
      "$id": 28,
      "label": "qst701901_msg0028",
      "style": 84,
      "name": "꿀꺽…"
    },
    {
      "$id": 29,
      "label": "qst701901_msg0029",
      "style": 84,
      "name": "[ML:Feeling kind=Anger ]콜록!!"
    },
    {
      "$id": 30,
      "label": "qst701901_msg0030",
      "style": 84,
      "name": "[ML:Feeling kind=Anger ]으아악!"
    },
    {
      "$id": 31,
      "label": "qst701901_msg0031",
      "style": 84,
      "name": "[ML:Feeling kind=Anger ]이런~ 플리아가 토했어!!"
    },
    {
      "$id": 32,
      "label": "qst701901_msg0032",
      "style": 84,
      "name": "[ML:Feeling kind=Anger ]아차~ 너무 맛이 없었나 봐!!"
    },
    {
      "$id": 33,
      "label": "qst701901_msg0033",
      "style": 84,
      "name": "……"
    },
    {
      "$id": 34,
      "label": "qst701901_msg0034",
      "style": 84,
      "name": "저 아이들은 정말 착한 애들이에요\n늘 플리아를 신경 써 주죠"
    },
    {
      "$id": 35,
      "label": "qst701901_msg0035",
      "style": 84,
      "name": "무슨 방법이 없을까?"
    },
    {
      "$id": 36,
      "label": "qst701901_msg0036",
      "style": 84,
      "name": "…제 일기에 의하면"
    },
    {
      "$id": 37,
      "label": "qst701901_msg0037",
      "style": 84,
      "name": "인비디아에는 용진향이라고 불리는\n만능약이 존재한다는군요"
    },
    {
      "$id": 38,
      "label": "qst701901_msg0038",
      "style": 84,
      "name": "이걸로 난치병에 걸린 왕비를\n치료한 적이 있다네요"
    },
    {
      "$id": 39,
      "label": "qst701901_msg0039",
      "style": 84,
      "name": "나도 들어 본 것 같군"
    },
    {
      "$id": 40,
      "label": "qst701901_msg0040",
      "style": 84,
      "name": "그것에 희망을 걸어 보는 건 어때?"
    },
    {
      "$id": 41,
      "label": "qst701901_msg0041",
      "style": 84,
      "name": "지금은 그게 최선인 것 같군요"
    },
    {
      "$id": 42,
      "label": "qst701901_msg0042",
      "style": 84,
      "name": "여러분…"
    },
    {
      "$id": 43,
      "label": "qst701901_msg0043",
      "style": 84,
      "name": "하지만 인비디아까지는\n거리가 멀지 않나요…?"
    },
    {
      "$id": 44,
      "label": "qst701901_msg0044",
      "style": 84,
      "name": "그렇긴 하지"
    },
    {
      "$id": 45,
      "label": "qst701901_msg0045",
      "style": 84,
      "name": "지금 우리가 메츠를 방치할 수는 없어"
    },
    {
      "$id": 46,
      "label": "qst701901_msg0046",
      "style": 84,
      "name": "역시 그렇겠죠…"
    },
    {
      "$id": 47,
      "label": "qst701901_msg0047",
      "style": 84,
      "name": "하지만 다행히도\n우리에게는 믿음직한 아군이 있지"
    },
    {
      "$id": 48,
      "label": "qst701901_msg0048",
      "style": 84,
      "name": "아레타로 돌아가서\n베로니카에게 상담해 보자"
    },
    {
      "$id": 49,
      "label": "qst701901_msg0049",
      "style": 84,
      "name": "역시 아델 님\n명안이군요"
    },
    {
      "$id": 50,
      "label": "qst701901_msg0050",
      "style": 84,
      "name": "저항군 중에는 힘을 빌려줄 사람이\n많이 있지"
    },
    {
      "$id": 51,
      "label": "qst701901_msg0051",
      "style": 84,
      "name": "그럼 갈까?"
    },
    {
      "$id": 52,
      "label": "qst701901_msg0052",
      "style": 84,
      "name": "마레나 씨\n조금 시간이 걸리겠지만\n안심하고 기다려 줘"
    },
    {
      "$id": 53,
      "label": "qst701901_msg0053",
      "style": 84,
      "name": "네…!\n감사합니다!"
    }
  ]
}