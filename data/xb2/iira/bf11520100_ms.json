  "rows": [
    {
      "$id": 1,
      "label": "bf11520100_0010",
      "style": 82,
      "name": "아델과 함께 다닌 지 얼마나 됐어?"
    },
    {
      "$id": 2,
      "label": "bf11520100_0020",
      "style": 82,
      "name": "음~ 3년 정도려나\n히카리는 1년 정도"
    },
    {
      "$id": 3,
      "label": "bf11520100_0030",
      "style": 82,
      "name": "마을이 홍수로 떠내려갔을 때\n아델 님이 구해 주셨어"
    },
    {
      "$id": 4,
      "label": "bf11520100_0040",
      "style": 82,
      "name": "그렇구나, 전쟁이 아니었네"
    },
    {
      "$id": 5,
      "label": "bf11520100_0050",
      "style": 82,
      "name": "전쟁이나 마찬가지야"
    },
    {
      "$id": 6,
      "label": "bf11520100_0060",
      "style": 82,
      "name": "물자가 모자라 산을 벌거숭이로 만들고\n그것 때문에 홍수가 일어났으니까"
    },
    {
      "$id": 7,
      "label": "bf11520100_0070",
      "style": 82,
      "name": "누나"
    },
    {
      "$id": 8,
      "label": "bf11520100_0080",
      "style": 82,
      "name": "[ML:undisp ]"
    },
    {
      "$id": 9,
      "label": "bf11520100_0090",
      "style": 82,
      "name": "어머니를 찾고 있었지?\n역시 전쟁 때문에 헤어진 거야?"
    },
    {
      "$id": 10,
      "label": "bf11520100_0100",
      "style": 82,
      "name": "그랬으면 좀 나았을까…"
    },
    {
      "$id": 11,
      "label": "bf11520100_0110",
      "style": 82,
      "name": "[ML:undisp ]"
    },
    {
      "$id": 12,
      "label": "bf11520100_0120",
      "style": 82,
      "name": "집에 사정이 좀 있었거든"
    },
    {
      "$id": 13,
      "label": "bf11520100_0130",
      "style": 82,
      "name": "사정…?"
    },
    {
      "$id": 14,
      "label": "bf11520100_0140",
      "style": 82,
      "name": "응…"
    },
    {
      "$id": 15,
      "label": "bf11520100_0150",
      "style": 82,
      "name": "그렇구나…"
    },
    {
      "$id": 16,
      "label": "bf11520100_0160",
      "style": 82,
      "name": "엄마는 하녀였어"
    },
    {
      "$id": 17,
      "label": "bf11520100_0170",
      "style": 82,
      "name": "나는 10살까지 몸종으로 일했고\n그런 시기에 신을 만났어"
    },
    {
      "$id": 18,
      "label": "bf11520100_0180",
      "style": 82,
      "name": "10살에 드라이버…"
    },
    {
      "$id": 19,
      "label": "bf11520100_0190",
      "style": 82,
      "name": "응\n칼부림 사건이 일어나서"
    },
    {
      "$id": 20,
      "label": "bf11520100_0200",
      "style": 82,
      "name": "도망치듯 집을 빠져나왔고…\n어느새 17년이 흘렀지"
    },
    {
      "$id": 21,
      "label": "bf11520100_0210",
      "style": 82,
      "name": "27살로는 안 보이는데?\n스무 살 정도일 거라 생각했어"
    },
    {
      "$id": 22,
      "label": "bf11520100_0220",
      "style": 82,
      "name": "어머, 말도 잘하네"
    },
    {
      "$id": 23,
      "label": "bf11520100_0230",
      "style": 82,
      "name": "…헤헤헤"
    },
    {
      "$id": 24,
      "label": "bf11520100_0240",
      "style": 82,
      "name": "그럼, 나랑 사타하고 똑같네"
    },
    {
      "$id": 25,
      "label": "bf11520100_0250",
      "style": 82,
      "name": "그래\n요즘엔 그런 아이들이 너무 많아"
    },
    {
      "$id": 26,
      "label": "bf11520100_0260",
      "style": 82,
      "name": "그래서 같이 있어 주고 싶었어\n신이 내 곁에 있어 줬던 것처럼"
    },
    {
      "$id": 27,
      "label": "bf11520100_0270",
      "style": 82,
      "name": "비록 진짜 가족이 아닐지라도"
    },
    {
      "$id": 28,
      "label": "bf11520100_0275",
      "style": 82,
      "name": "그렇지? 사타히코"
    },
    {
      "$id": 29,
      "label": "bf11520100_0290",
      "style": 82,
      "name": "어머~ 대꾸도 안 해 주는 거야?\n나 상처 입었어"
    },
    {
      "$id": 30,
      "label": "bf11520100_0300",
      "style": 82,
      "name": "괜찮아\n이 녀석도 기뻐하고 있어"
    },
    {
      "$id": 31,
      "label": "bf11520100_0310",
      "style": 82,
      "name": "어머, 그런 거야?"
    },
    {
      "$id": 32,
      "label": "bf11520100_0320",
      "style": 82,
      "name": "그렇지"
    },
    {
      "$id": 33,
      "label": "bf11520100_0330",
      "style": 82,
      "name": "그렇구나\n좋아, 힘이 났어"
    },
    {
      "$id": 34,
      "label": "bf11520100_0340",
      "style": 82,
      "name": "하루빨리 평화로운 세계를 만들어서 다 함께 살자"
    },
    {
      "$id": 35,
      "label": "bf11520100_0350",
      "style": 82,
      "name": "응!"
    },
    {
      "$id": 36,
      "label": "bf11520100_0360",
      "style": 82,
      "name": "너도 함께하면 좋을 텐데"
    },
    {
      "$id": 37,
      "label": "bf11520100_0370",
      "style": 82,
      "name": "나 말이야?"
    },
    {
      "$id": 38,
      "label": "bf11520100_0380",
      "style": 82,
      "name": "하늘의 성배라고 무리할 필요는 없어"
    },
    {
      "$id": 39,
      "label": "bf11520100_0390",
      "style": 82,
      "name": "딱히 무리한 적은…"
    },
    {
      "$id": 40,
      "label": "bf11520100_0400",
      "style": 82,
      "name": "있는 그대로의 너로 있으면 된다고 생각해, 나는"
    },
    {
      "$id": 41,
      "label": "bf11520100_0410",
      "style": 82,
      "name": "[ML:undisp ]"
    }
