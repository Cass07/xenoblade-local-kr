    {
      "$id": 5,
      "label": "bf11620100_0100",
      "style": 82,
      "name": "어떻게 할까? 그거…"
    },
    {
      "$id": 6,
      "label": "bf11620100_0110",
      "style": 82,
      "name": "어떡하기는, 앞으로도 계속 쓸 거야"
    },
    {
      "$id": 7,
      "label": "bf11620100_0120",
      "style": 82,
      "name": "라우라가 나를 위해 만들어 준 거니까"
    },
    {
      "$id": 8,
      "label": "bf11620100_0130",
      "style": 82,
      "name": "그렇구나…"
    },
    {
      "$id": 9,
      "label": "bf11620100_0140",
      "style": 82,
      "name": "언젠가 말이야…"
    },
    {
      "$id": 10,
      "label": "bf11620100_0150",
      "style": 82,
      "name": "[ML:undisp ]"
    },
    {
      "$id": 11,
      "label": "bf11620100_0160",
      "style": 82,
      "name": "언젠가 내가 죽고\n신이 코어로 돌아가는 날이 올 거잖아"
    },
    {
      "$id": 12,
      "label": "bf11620100_0170",
      "style": 82,
      "name": "전부 잊게 된다 해도"
    },
    {
      "$id": 13,
      "label": "bf11620100_0172",
      "style": 82,
      "name": "그 가면만이라도\n간직해 주면 좋을 텐데…"
    },
    {
      "$id": 14,
      "label": "bf11620100_0180",
      "style": 82,
      "name": "라우라…"
    },
    {
      "$id": 15,
      "label": "bf11620100_0190",
      "style": 82,
      "name": "언젠가, 먼 훗날의 이야기야\n너무 신경 쓰지 마"
    },
    {
      "$id": 16,
      "label": "bf11620100_0200",
      "style": 82,
      "name": "…아무쪼록\n이 일기가 \"네\" 손에 전해지길 바란다"
    },
    {
      "$id": 17,
      "label": "bf11620100_0210",
      "style": 82,
      "name": "그걸 믿고 마지막으로\n내가 나로 있기 위한 방법을"
    },
    {
      "$id": 18,
      "label": "bf11620100_0220",
      "style": 82,
      "name": "내 반려와의 인연을\n이 몸에 새길 방법을 남긴다"
    },
    {
      "$id": 19,
      "label": "bf11620100_0230",
      "style": 82,
      "name": "그 방법은…"
    },
    {
      "$id": 20,
      "label": "bf11620100_0240",
      "style": 82,
      "name": "그 방법은"
    },
    {
      "$id": 21,
      "label": "bf11620100_0250",
      "style": 82,
      "name": "사랑하는 이의 마음을 내 육신의 일부로 만드는 것이다"
    },
    {
      "$id": 22,
      "label": "bf11620100_0260",
      "style": 82,
      "name": "그 방법을 통해 두 사람의 관계는 영원해진다"
    },
    {
      "$id": 23,
      "label": "bf11620100_0270",
      "style": 82,
      "name": "하지만 과연 그게 최선의 방법일까"
