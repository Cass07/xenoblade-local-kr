    {
      "$id": 22,
      "label": "qst702201_msg0022",
      "style": 84,
      "name": "너 말이야…"
    },
    {
      "$id": 23,
      "label": "qst702201_msg0023",
      "style": 84,
      "name": "하, 하지만 중요한 물건이라\n몸에 늘 지니고 다녔단 말임다~~"
    },
    {
      "$id": 24,
      "label": "qst702201_msg0024",
      "style": 84,
      "name": "아무튼 열쇠는 우리가 주워 올게"
