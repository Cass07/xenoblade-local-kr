  "rows": [
    {
      "$id": 1,
      "label": "campfev11_610_150_msg0001",
      "style": 84,
      "name": "세이류우 씨에게\n흥미로운 이야기를 들었는데…"
    },
    {
      "$id": 2,
      "label": "campfev11_610_150_msg0002",
      "style": 84,
      "name": "이다음에 어떻게 할 예정이었지?"
    },
    {
      "$id": 3,
      "label": "campfev11_610_150_msg0003",
      "style": 84,
      "name": "라우라…"
    },
    {
      "$id": 4,
      "label": "campfev11_610_150_msg0004",
      "style": 84,
      "name": "의도적으로 피하는 건가?"
    },
    {
      "$id": 5,
      "label": "campfev11_610_150_msg0005",
      "style": 84,
      "name": "왕도의 숙소에 머물면서\n서훈식을 준비하기로 했어요"
    },
    {
      "$id": 6,
      "label": "campfev11_610_150_msg0006",
      "style": 84,
      "name": "…그랬지"
    },
    {
      "$id": 7,
      "label": "campfev11_610_150_msg0007",
      "style": 84,
      "name": "아아, 긴장되네"
    },
    {
      "$id": 8,
      "label": "campfev11_610_150_msg0008",
      "style": 84,
      "name": "이라의 기사가 된다고 해도\n지금이랑 똑같아"
    },
    {
      "$id": 9,
      "label": "campfev11_610_150_msg0009",
      "style": 84,
      "name": "그래요\n긴장하지 않아도 괜찮아요"
    },
    {
      "$id": 10,
      "label": "campfev11_610_150_msg0010",
      "style": 84,
      "name": "그렇겠지만…\n왕자님이랑 황제 폐하가 그렇게 말해 봤자\n설득력이 떨어진다고 해야 하나…"
    },
    {
      "$id": 11,
      "label": "campfev11_610_150_msg0011",
      "style": 84,
      "name": "아까도 말했지만\n이렇게 생각해 보는 건 어떨까요?"
    },
    {
      "$id": 12,
      "label": "campfev11_610_150_msg0012",
      "style": 84,
      "name": "이라의 기사가 됨으로써\n이라의 비보를 가질 권리를 얻는 거죠"
    },
    {
      "$id": 13,
      "label": "campfev11_610_150_msg0013",
      "style": 84,
      "name": "지금까지 이라에게 쫓길지도 모른다고\n불안에 떨었겠지만\n앞으로는 걱정 없을 거야"
    },
    {
      "$id": 14,
      "label": "campfev11_610_150_msg0014",
      "style": 84,
      "name": "으음, 그렇긴 하네…"
    },
    {
      "$id": 15,
      "label": "campfev11_610_150_msg0015",
      "style": 84,
      "name": "그렇게 생각하니 조금 마음이 가벼워졌어…"
    },
    {
      "$id": 16,
      "label": "campfev11_610_150_msg0016",
      "style": 84,
      "name": "좋아!\n숙소로 돌아가서 서훈식을 준비하자"
    }
  ]
}