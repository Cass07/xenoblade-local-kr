  "rows": [
    {
      "$id": 1,
      "label": "bf07530100_0010",
      "style": 82,
      "name": "[ML:undisp ]"
    },
    {
      "$id": 2,
      "label": "bf07530100_0015",
      "style": 82,
      "name": "호무라!!"
    },
    {
      "$id": 3,
      "label": "bf07530100_0020",
      "style": 82,
      "name": "호무라"
    },
    {
      "$id": 4,
      "label": "bf07530100_0030",
      "style": 82,
      "name": "한발 늦었군\n필요한 건 전부 받았다"
    },
    {
      "$id": 5,
      "label": "bf07530100_0040",
      "style": 82,
      "name": "신, 호무라한테 무슨 짓을 했지?"
    },
    {
      "$id": 6,
      "label": "bf07530100_0050",
      "style": 82,
      "name": "아까 얘기했을 텐데"
    },
    {
      "$id": 7,
      "label": "bf07530100_0060",
      "style": 82,
      "name": "500년 전에 잃어버린 것을\n되찾은 것뿐이라고"
    },
    {
      "$id": 8,
      "label": "bf07530100_0070",
      "style": 82,
      "name": "메츠… 어떻게…"
    },
    {
      "$id": 9,
      "label": "bf07530100_0080",
      "style": 82,
      "name": "내가 그 정도로 죽을 줄 알았나?"
    },
    {
      "$id": 10,
      "label": "bf07530100_0082",
      "style": 82,
      "name": "나는 하늘의 성배라고"
    },
    {
      "$id": 11,
      "label": "bf07530100_0084",
      "style": 82,
      "name": "얕잡아 보면 곤란하지"
    },
    {
      "$id": 12,
      "label": "bf07530100_0086",
      "style": 82,
      "name": "[ML:undisp ]"
    },
    {
      "$id": 13,
      "label": "bf07530100_0088",
      "style": 82,
      "name": "제법 그럴싸한 연기였지?"
    },
    {
      "$id": 14,
      "label": "bf07530100_0090",
      "style": 82,
      "name": "[ML:undisp ]"
    },
    {
      "$id": 15,
      "label": "bf07530100_0095",
      "style": 82,
      "name": "네가 한 짓이냐?"
    },
    {
      "$id": 16,
      "label": "bf07530100_0100",
      "style": 82,
      "name": "하늘의 성배씩이나 돼서\n인간 따위에게 정을 주다니"
    },
    {
      "$id": 17,
      "label": "bf07530100_0110",
      "style": 82,
      "name": "너와의 추억을 지키려고\n발버둥 치더군"
    },
    {
      "$id": 18,
      "label": "bf07530100_0120",
      "style": 82,
      "name": "뭐, 몽땅 지워 버렸지만"
    },
    {
      "$id": 19,
      "label": "bf07530100_0130",
      "style": 82,
      "name": "[ML:undisp ]"
    },
    {
      "$id": 20,
      "label": "bf07530100_0140",
      "style": 82,
      "name": "열심히 애를 쓰더라고"
    },
    {
      "$id": 21,
      "label": "bf07530100_0150",
      "style": 82,
      "name": "자기 추억을 빼앗지 말라고\n애원하더라니까"
    },
    {
      "$id": 22,
      "label": "bf07530100_0160",
      "style": 82,
      "name": "미안하게 됐군\n이런 구석진 곳까지 오게 해서"
    },
    {
      "$id": 23,
      "label": "bf07530100_0170",
      "style": 82,
      "name": "빈껍데기라도 좋다면 넘겨주지"
    },
    {
      "$id": 24,
      "label": "bf07530100_0175",
      "style": 82,
      "name": "가져가라"
    },
    {
      "$id": 25,
      "label": "bf07530100_0180",
      "style": 82,
      "name": "호무라, 호무라!"
    },
    {
      "$id": 26,
      "label": "bf07530100_0190",
      "style": 82,
      "name": "미안해, 호무라, 히카리…"
    },
    {
      "$id": 27,
      "label": "bf07530100_0200",
      "style": 82,
      "name": "나 같은 사람이랑 함께하게 돼서…"
    },
    {
      "$id": 28,
      "label": "bf07530100_0210",
      "style": 82,
      "name": "렉스…"
    },
    {
      "$id": 29,
      "label": "bf07530100_0220",
      "style": 82,
      "name": "형님…"
    },
    {
      "$id": 30,
      "label": "bf07530100_0230",
      "style": 82,
      "name": "니아"
    },
    {
      "$id": 31,
      "label": "bf07530100_0240",
      "style": 82,
      "name": "알았어"
    },
    {
      "$id": 32,
      "label": "bf07530100_0250",
      "style": 82,
      "name": "뱌코, 호무라를 부탁할게"
    },
    {
      "$id": 33,
      "label": "bf07530100_0260",
      "style": 82,
      "name": "알겠습니다"
    },
    {
      "$id": 34,
      "label": "bf07530100_0270",
      "style": 82,
      "name": "뭐야\n울부짖으면서 달려들 줄 알았는데"
    },
    {
      "$id": 35,
      "label": "bf07530100_0280",
      "style": 82,
      "name": "이제 좀 어른이 된 거냐?"
    },
    {
      "$id": 36,
      "label": "bf07530100_0290",
      "style": 82,
      "name": "바보야…"
    },
    {
      "$id": 37,
      "label": "bf07530100_0300",
      "style": 82,
      "name": "응?"
    },
    {
      "$id": 38,
      "label": "bf07530100_0310",
      "style": 82,
      "name": "나는 바보야"
    },
    {
      "$id": 39,
      "label": "bf07530100_0315",
      "style": 82,
      "name": "두 사람의 마음을 조금도 이해하지 못했어"
    },
    {
      "$id": 40,
      "label": "bf07530100_0320",
      "style": 82,
      "name": "완전히 다른 길을 보며 걷고 있었는데\n난 같은 길을 걷고 있다고 믿었어"
    },
    {
      "$id": 41,
      "label": "bf07530100_0330",
      "style": 82,
      "name": "하늘의 성배라는 광채가 만들어 낸 어두운 그림자"
    },
    {
      "$id": 42,
      "label": "bf07530100_0340",
      "style": 82,
      "name": "그런 어두운 길을 지금까지 쭉"
    },
    {
      "$id": 43,
      "label": "bf07530100_0345",
      "style": 82,
      "name": "호무라와 히카리는 홀로 걸어왔어"
    },
    {
      "$id": 44,
      "label": "bf07530100_0350",
      "style": 82,
      "name": "무슨 소리냐, 애송이?"
    },
    {
      "$id": 45,
      "label": "bf07530100_0360",
      "style": 82,
      "name": "그래서…\n바보인 나도 결심했어"
    },
    {
      "$id": 46,
      "label": "bf07530100_0370",
      "style": 82,
      "name": "호오? 뭘 결심했지?"
    },
    {
      "$id": 47,
      "label": "bf07530100_0380",
      "style": 82,
      "name": "이렇게 된 거, 내가 대신 들어나 주마"
    },
    {
      "$id": 48,
      "label": "bf07530100_0390",
      "style": 82,
      "name": "뻔한 거 아냐?"
    },
    {
      "$id": 49,
      "label": "bf07530100_0400",
      "style": 82,
      "name": "너희들을 쳐부수고\n두 사람과 함께 낙원에 가겠어!"
    },
    {
      "$id": 50,
      "label": "bf07530100_0410",
      "style": 82,
      "name": "하! 쳐부순다고?\n신에게 꼼짝도 못 했던 네가?"
    },
    {
      "$id": 51,
      "label": "bf07530100_0420",
      "style": 82,
      "name": "게다가 이 몸은\n하늘의 성배의 힘을 되찾았고"
    },
    {
      "$id": 52,
      "label": "bf07530100_0430",
      "style": 82,
      "name": "디바이스도 마음껏 쓸 수 있지"
    },
    {
      "$id": 53,
      "label": "bf07530100_0440",
      "style": 82,
      "name": "무슨 수로 이긴다는 거냐"
    },
    {
      "$id": 54,
      "label": "bf07530100_0450",
      "style": 82,
      "name": "이길 수 있어"
    },
    {
      "$id": 55,
      "label": "bf07530100_0460",
      "style": 82,
      "name": "그건 이제 빈껍데기일 뿐이다\n힘을 빌리려 해 봤자 헛수고야"
    },
    {
      "$id": 56,
      "label": "bf07530100_0470",
      "style": 82,
      "name": "이제야 알았어"
    },
    {
      "$id": 57,
      "label": "bf07530100_0475",
      "style": 82,
      "name": "두 사람이 바라는 게 무엇인지를"
    },
    {
      "$id": 58,
      "label": "bf07530100_0480",
      "style": 82,
      "name": "그걸 위해\n내가 반드시 해야 할 일을"
    },
    {
      "$id": 59,
      "label": "bf07530100_0490",
      "style": 82,
      "name": "드라이버와 블레이드가 맺는 인연의 의미를!"
    },
    {
      "$id": 60,
      "label": "bf07530100_0500",
      "style": 82,
      "name": "[ML:undisp ]"
    },
    {
      "$id": 61,
      "label": "bf07530100_0510",
      "style": 82,
      "name": "얘들아\n내게 힘을 빌려줘!"
    },
    {
      "$id": 62,
      "label": "bf07530100_0520",
      "style": 82,
      "name": "호무라를 되찾고 낙원에 가는 거야!"
    }
  ]
}