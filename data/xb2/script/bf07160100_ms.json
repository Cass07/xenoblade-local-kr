  "rows": [
    {
      "$id": 1,
      "label": "bf07160100_0010",
      "style": 82,
      "name": "수호자잖아"
    },
    {
      "$id": 2,
      "label": "bf07160100_0020",
      "style": 82,
      "name": "수호자라는 게 뭔데?"
    },
    {
      "$id": 3,
      "label": "bf07160100_0030",
      "style": 82,
      "name": "마을의 수호신을 모시는 돌기둥이야"
    },
    {
      "$id": 4,
      "label": "bf07160100_0040",
      "style": 82,
      "name": "할아버지, 여기 뭐가 있는데?"
    },
    {
      "$id": 5,
      "label": "bf07160100_0045",
      "style": 82,
      "name": "렉스, 대좌의 석판을\n시계 방향으로 돌리거라"
    },
    {
      "$id": 6,
      "label": "bf07160100_0050",
      "style": 82,
      "name": "이거?\n영차"
    },
    {
      "$id": 7,
      "label": "bf07160100_0055",
      "style": 82,
      "name": "어? 이 문장은…"
    },
    {
      "$id": 8,
      "label": "bf07160100_0060",
      "style": 82,
      "name": "그때랑 똑같은 거네"
    },
    {
      "$id": 9,
      "label": "bf07160100_0065",
      "style": 82,
      "name": "문장에 손을 올려 보거라"
    },
    {
      "$id": 10,
      "label": "bf07160100_0070",
      "style": 82,
      "name": "응"
    },
    {
      "$id": 11,
      "label": "bf07160100_0075",
      "style": 82,
      "name": "이, 이건!"
    },
    {
      "$id": 12,
      "label": "bf07160100_0080",
      "style": 82,
      "name": "이 길은 군도의 중심부와 연결돼 있단다"
    },
    {
      "$id": 13,
      "label": "bf07160100_0085",
      "style": 82,
      "name": "엘피스 영동으로\n불리는 곳이 거기에 있지"
    },
    {
      "$id": 14,
      "label": "bf07160100_0090",
      "style": 82,
      "name": "엘피스 영동…"
    },
    {
      "$id": 15,
      "label": "bf07160100_0095",
      "style": 82,
      "name": "이곳과 영동의 문은"
    },
    {
      "$id": 16,
      "label": "bf07160100_0100",
      "style": 82,
      "name": "리베라리타스 사람이 아니면\n열 수 없게 되어 있단다"
    },
    {
      "$id": 17,
      "label": "bf07160100_0105",
      "style": 82,
      "name": "그렇구나\n그래서 신은 그때 날…"
    },
    {
      "$id": 18,
      "label": "bf07160100_0110",
      "style": 82,
      "name": "아델은 이 마을을 세웠을 때"
    },
    {
      "$id": 19,
      "label": "bf07160100_0115",
      "style": 82,
      "name": "군도의 중심인 아르스의 몸속 깊은 곳에\n어떤 것을 숨겼다"
    },
    {
      "$id": 20,
      "label": "bf07160100_0120",
      "style": 82,
      "name": "설마, 그게 그 검이야?"
    },
    {
      "$id": 21,
      "label": "bf07160100_0130",
      "style": 82,
      "name": "어디서 새어 나간 건지"
    },
    {
      "$id": 22,
      "label": "bf07160100_0140",
      "style": 82,
      "name": "하늘의 성배의 검이\n여기 있다는 얘기를 들은 자들이"
    },
    {
      "$id": 23,
      "label": "bf07160100_0145",
      "style": 82,
      "name": "지금껏 수도 없이 영동에 들어갔지"
    },
    {
      "$id": 24,
      "label": "bf07160100_0150",
      "style": 82,
      "name": "하지만 다시 돌아온 자는 없었다"
    },
    {
      "$id": 25,
      "label": "bf07160100_0160",
      "style": 82,
      "name": "그렇게 무시무시한 곳이야?"
    },
    {
      "$id": 26,
      "label": "bf07160100_0170",
      "style": 82,
      "name": "호무라와 나눠서 봉인해야 할 정도니까"
    },
    {
      "$id": 27,
      "label": "bf07160100_0180",
      "style": 82,
      "name": "누구도 건드릴 수 없도록\n장치를 해 둔 게지"
    },
    {
      "$id": 28,
      "label": "bf07160100_0190",
      "style": 82,
      "name": "하지만 아델은 이렇게도 말했다"
    },
    {
      "$id": 29,
      "label": "bf07160100_0200",
      "style": 82,
      "name": "이 검을 얻은 자가 바로\n진정한 하늘의 성배의 드라이버라고"
    },
    {
      "$id": 30,
      "label": "bf07160100_0210",
      "style": 82,
      "name": "진정한 드라이버…"
    },
    {
      "$id": 31,
      "label": "bf07160100_0220",
      "style": 82,
      "name": "어쩔 거냐, 렉스?"
    },
    {
      "$id": 32,
      "label": "bf07160100_0230",
      "style": 82,
      "name": "난 갈 거야\n아니, 가야만 해"
    },
    {
      "$id": 33,
      "label": "bf07160100_0240",
      "style": 82,
      "name": "지금 이 순간에도 호무라와 히카리는…"
    },
    {
      "$id": 34,
      "label": "bf07160100_0250",
      "style": 82,
      "name": "각오는 된 모양이구나"
    }
  ]
}