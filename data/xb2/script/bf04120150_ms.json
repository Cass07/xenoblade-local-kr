  "schema": [
    {
      "name": "label",
      "type": 7
    },
    {
      "name": "style",
      "type": 2
    },
    {
      "name": "name",
      "type": 7
    }
  ],
  "rows": [
    {
      "$id": 1,
      "label": "bf04120150_0010",
      "style": 82,
      "name": "자, 네 말대로 힘으로 되찾아 왔어"
    },
    {
      "$id": 2,
      "label": "bf04120150_0020",
      "style": 82,
      "name": "[ML:undisp ]"
    },
    {
      "$id": 3,
      "label": "bf04120150_0030",
      "style": 82,
      "name": "[ML:undisp ]"
    },
    {
      "$id": 4,
      "label": "bf04120150_0050",
      "style": 82,
      "name": "나 참…\n다들 열 살 안팎이잖아?"
    },
    {
      "$id": 5,
      "label": "bf04120150_0060",
      "style": 82,
      "name": "왜 이런 짓을 한 거야?"
    },
    {
      "$id": 6,
      "label": "bf04120150_0070",
      "style": 82,
      "name": "[ML:undisp ]"
    },
    {
      "$id": 7,
      "label": "bf04120150_0080",
      "style": 82,
      "name": "[ML:undisp ]"
    },
    {
      "$id": 8,
      "label": "bf04120150_0090",
      "style": 82,
      "name": "이봐, 입이 있으면 말 좀 해 봐"
    },
    {
      "$id": 9,
      "label": "bf04120150_0100",
      "style": 82,
      "name": "아가씨, 좀 더 상냥하게…"
    },
    {
      "$id": 10,
      "label": "bf04120150_0110",
      "style": 82,
      "name": "왜? 얘들은 도둑인데?"
    },
    {
      "$id": 11,
      "label": "bf04120150_0120",
      "style": 82,
      "name": "하지만 상대는 어린아이들이고…"
    },
    {
      "$id": 12,
      "label": "bf04120150_0130",
      "style": 82,
      "name": "이봐, 무슨 사정이 있는 거지?\n혹시 먹고살 돈이 없어서 그래?"
    },
    {
      "$id": 13,
      "label": "bf04120150_0140",
      "style": 82,
      "name": "깔보지 마\n누가 돈 때문에 이러는 줄 알아?"
    },
    {
      "$id": 14,
      "label": "bf04120150_0150",
      "style": 82,
      "name": "음, 돈 때문은 아니라는 거네"
    },
    {
      "$id": 15,
      "label": "bf04120150_0160",
      "style": 82,
      "name": "돈 때문에 그런 게 아니면 뭔데?"
    },
    {
      "$id": 16,
      "label": "bf04120150_0170",
      "style": 82,
      "name": "솔직하게 말하는 게 좋아모\n우리 말 들어서 나쁠 거 없어모"
    },
    {
      "$id": 17,
      "label": "bf04120150_0180",
      "style": 82,
      "name": "[ML:undisp ]"
    },
    {
      "$id": 18,
      "label": "bf04120150_0190",
      "style": 82,
      "name": "말해 봐"
    },
    {
      "$id": 19,
      "label": "bf04120150_0200",
      "style": 82,
      "name": "…복수야"
    },
    {
      "$id": 20,
      "label": "bf04120150_0210",
      "style": 82,
      "name": "복수?"
    },
    {
      "$id": 21,
      "label": "bf04120150_0220",
      "style": 82,
      "name": "마을 사람들을 대신해 복수를 하고 싶었어"
    },
    {
      "$id": 22,
      "label": "bf04120150_0230",
      "style": 82,
      "name": "설마 블레이드와 동조할 생각이었어?"
    },
    {
      "$id": 23,
      "label": "bf04120150_0240",
      "style": 82,
      "name": "설명해 줄래?\n우리가 도울 수 있을지도 몰라"
    },
    {
      "$id": 24,
      "label": "bf04120150_0250",
      "style": 82,
      "name": "도적들이 마을을…"
    },
    {
      "$id": 25,
      "label": "bf04120150_0260",
      "style": 82,
      "name": "작지만 평화로운 마을이었는데\n물이 나온다는 이유만으로…"
    },
    {
      "$id": 26,
      "label": "bf04120150_0270",
      "style": 82,
      "name": "생존자는?\n혹시 너희들뿐이야?"
    },
    {
      "$id": 27,
      "label": "bf04120150_0280",
      "style": 82,
      "name": "이름이 뭐야?"
    },
    {
      "$id": 28,
      "label": "bf04120150_0290",
      "style": 82,
      "name": "내 이름? 아니면 마을 이름?"
    },
    {
      "$id": 29,
      "label": "bf04120150_0300",
      "style": 82,
      "name": "둘 다"
    },
    {
      "$id": 30,
      "label": "bf04120150_0310",
      "style": 82,
      "name": "…리리오\n마을 이름은 이라다야"
    },
    {
      "$id": 31,
      "label": "bf04120150_0320",
      "style": 82,
      "name": "이라다 마을…\n들어 본 적 없어"
    },
    {
      "$id": 32,
      "label": "bf04120150_0330",
      "style": 82,
      "name": "굴라 외곽의 작은 마을이니까\n아는 사람은 별로 없어"
    },
    {
      "$id": 33,
      "label": "bf04120150_0340",
      "style": 82,
      "name": "리리오, 사정은 알았어\n하지만 남의 물건을 훔치는 건 잘못이야"
    },
    {
      "$id": 34,
      "label": "bf04120150_0350",
      "style": 82,
      "name": "그리고 코어와 동조하는 건 위험해"
    },
    {
      "$id": 35,
      "label": "bf04120150_0360",
      "style": 82,
      "name": "실패하면 어쩌려고 그래?"
    },
    {
      "$id": 36,
      "label": "bf04120150_0370",
      "style": 82,
      "name": "해 보지 않으면 모르잖아"
    },
    {
      "$id": 37,
      "label": "bf04120150_0380",
      "style": 82,
      "name": "우리 중 한 명이라도 성공하면 되니까"
    },
    {
      "$id": 38,
      "label": "bf04120150_0390",
      "style": 82,
      "name": "아무래도 꽤 절박한 상황인가 보구먼"
    },
    {
      "$id": 39,
      "label": "bf04120150_0400",
      "style": 82,
      "name": "그자들이 어디에 있는지 알고 있느냐?"
    },
    {
      "$id": 40,
      "label": "bf04120150_0410",
      "style": 82,
      "name": "쌍둥이 나무 언덕에 큰 동굴이 있어\n거기가 녀석들의 아지트야"
    },
    {
      "$id": 41,
      "label": "bf04120150_0420",
      "style": 82,
      "name": "리리오, 큰일 났어!\n녀석들이 거리에…"
    },
    {
      "$id": 42,
      "label": "bf04120150_0430",
      "style": 82,
      "name": "…누구야?"
    },
    {
      "$id": 43,
      "label": "bf04120150_0440",
      "style": 82,
      "name": "이 사람들은 신경 쓰지 마\n그나저나 큰일이라니, 뭐가?"
    },
    {
      "$id": 44,
      "label": "bf04120150_0450",
      "style": 82,
      "name": "아, 응, 녀석들이 조만간\n트리고 거리를 습격할 거래"
    },
    {
      "$id": 45,
      "label": "bf04120150_0460",
      "style": 82,
      "name": "그게 정말이야?"
    },
    {
      "$id": 46,
      "label": "bf04120150_0470",
      "style": 82,
      "name": "응, 아까 같은 패거리가\n거리에서 얘기하는 걸 들었어"
    },
    {
      "$id": 47,
      "label": "bf04120150_0480",
      "style": 82,
      "name": "한패가 틀림없는 거지?"
    },
    {
      "$id": 48,
      "label": "bf04120150_0490",
      "style": 82,
      "name": "아무래도 미리 정찰하러 온 것 같아"
    },
    {
      "$id": 49,
      "label": "bf04120150_0500",
      "style": 82,
      "name": "영사가 없어져서\n통솔이 흐트러진 지금이 찬스라고 했어"
    },
    {
      "$id": 50,
      "label": "bf04120150_0510",
      "style": 82,
      "name": "그렇군요\n듣고 보니 습격하려면 지금이 적기겠네요"
    },
    {
      "$id": 51,
      "label": "bf04120150_0520",
      "style": 82,
      "name": "그 영사한테 통솔력이 있었는지는\n의심스럽지만 말이야"
    },
    {
      "$id": 52,
      "label": "bf04120150_0530",
      "style": 82,
      "name": "혹시 우리 때문일까모?"
    },
    {
      "$id": 53,
      "label": "bf04120150_0540",
      "style": 82,
      "name": "으음…\n뭐, 계기를 제공했다고 볼 순 있겠지"
    },
    {
      "$id": 54,
      "label": "bf04120150_0550",
      "style": 82,
      "name": "못 본 척할 수는 없겠네요"
    },
    {
      "$id": 55,
      "label": "bf04120150_0560",
      "style": 82,
      "name": "그렇지"
    },
    {
      "$id": 56,
      "label": "bf04120150_0570",
      "style": 82,
      "name": "알았어\n그 녀석들은 우리에게 맡겨 줘"
    },
    {
      "$id": 57,
      "label": "bf04120150_0580",
      "style": 82,
      "name": "맡겨 달라니…"
    },
    {
      "$id": 58,
      "label": "bf04120150_0590",
      "style": 82,
      "name": "복수해 줄게"
    },
    {
      "$id": 59,
      "label": "bf04120150_0600",
      "style": 82,
      "name": "너…"
    },
    {
      "$id": 60,
      "label": "bf04120150_0610",
      "style": 82,
      "name": "난 렉스야"
    },
    {
      "$id": 61,
      "label": "bf04120150_0620",
      "style": 82,
      "name": "그럼 다녀올게\n너희는 거리의 위병에게 알리러 가"
    },
    {
      "$id": 62,
      "label": "bf04120150_0630",
      "style": 82,
      "name": "도적이 쳐들어온다는 걸 알리라고?"
    },
    {
      "$id": 63,
      "label": "bf04120150_0640",
      "style": 82,
      "name": "도적 얘기를 해 봤자\n안 믿어 줄 거야"
    },
    {
      "$id": 64,
      "label": "bf04120150_0650",
      "style": 82,
      "name": "그러니까 우리 이야기를 해"
    },
    {
      "$id": 65,
      "label": "bf04120150_0660",
      "style": 82,
      "name": "급수탑을 망가뜨린 자들이\n쌍둥이 나무 언덕에 숨어 있다고 하면 돼"
    },
    {
      "$id": 66,
      "label": "bf04120150_0670",
      "style": 82,
      "name": "상관은 없지만, 대체 왜?"
    },
    {
      "$id": 67,
      "label": "bf04120150_0680",
      "style": 82,
      "name": "그건 곧 알게 될 거야\n아무튼 부탁한다"
    },
    {
      "$id": 68,
      "label": "bf04120150_0690",
      "style": 82,
      "name": "응…"
    }
  ]
}