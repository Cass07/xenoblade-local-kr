  "rows": [
    {
      "$id": 1,
      "label": "kizunatalk057_msg0001",
      "style": 84,
      "name": "흐~~~읍!!!"
    },
    {
      "$id": 2,
      "label": "kizunatalk057_msg0002",
      "style": 84,
      "name": "무슨 일이야모\n지카라오?"
    },
    {
      "$id": 3,
      "label": "kizunatalk057_msg0003",
      "style": 84,
      "name": "기합 잔뜩 넣고\n뭘 하려고모?"
    },
    {
      "$id": 4,
      "label": "kizunatalk057_msg0004",
      "style": 84,
      "name": "나는\n사람들과 이야기하고 싶다"
    },
    {
      "$id": 5,
      "label": "kizunatalk057_msg0005",
      "style": 84,
      "name": "하지만 다들 날 무서워한다"
    },
    {
      "$id": 6,
      "label": "kizunatalk057_msg0006",
      "style": 84,
      "name": "사실 나는 무섭지 않다\n모두가 그걸 알아주었으면 한다"
    },
    {
      "$id": 7,
      "label": "kizunatalk057_msg0007",
      "style": 84,
      "name": "하긴 지카라오는 길을 걸을 때도\n개미조차 피해 밟으려는\n마음씨 고운 블레이드인걸모"
    },
    {
      "$id": 8,
      "label": "kizunatalk057_msg0008",
      "style": 84,
      "name": "지카라오가 어떤 블레이드인지\n알리려면 이야기를 나눠 보는 편이\n좋겠다고 봐"
    },
    {
      "$id": 9,
      "label": "kizunatalk057_msg0009",
      "style": 84,
      "name": "이야기를 나누다 보면\n지카라오의 착한 마음이 전해질걸?"
    },
    {
      "$id": 10,
      "label": "kizunatalk057_msg0010",
      "style": 84,
      "name": "이야기를 나눠서 지카라오의\n마음이 잘 전달됐으면 좋겠군"
    },
    {
      "$id": 11,
      "label": "kizunatalk057_msg0011",
      "style": 84,
      "name": "지카라오하고 같이 얘기해 보면\n어떤 아인지 바로 알 기다"
    },
    {
      "$id": 12,
      "label": "kizunatalk057_msg0012",
      "style": 84,
      "name": "그냥 평범하게 얘기하는 게 어렵겠어모\n아까처럼 기합까지 한다면 더 어려워모!"
    },
    {
      "$id": 13,
      "label": "kizunatalk057_msg0013",
      "style": 84,
      "name": "그래, 모두 도망간다\n그게 고민이다"
    },
    {
      "$id": 14,
      "label": "kizunatalk057_msg0014",
      "style": 84,
      "name": "뭔가 좋은 방법이 없을까?"
    },
    {
      "$id": 15,
      "label": "kizunatalk057_msg0015",
      "style": 165,
      "name": "음…"
    },
    {
      "$id": 16,
      "label": "kizunatalk057_msg0016",
      "style": 164,
      "name": "저기 울고 있는 아이를\n도와주면 어떨까?"
    },
    {
      "$id": 17,
      "label": "kizunatalk057_msg0017",
      "style": 164,
      "name": "저 곤경에 처한 어르신을\n도와드리면 어떨까?"
    },
    {
      "$id": 18,
      "label": "kizunatalk057_msg0018",
      "style": 165,
      "name": "그러게…"
    },
    {
      "$id": 19,
      "label": "kizunatalk057_msg0019",
      "style": 164,
      "name": "저기 울고 있는 아이를 도와주는 건 어때?"
    },
    {
      "$id": 20,
      "label": "kizunatalk057_msg0020",
      "style": 164,
      "name": "저 곤경에 처한 어르신을 도와드리는 건 어때?"
    },
    {
      "$id": 21,
      "label": "kizunatalk057_msg0021",
      "style": 165,
      "name": "흠…"
    },
    {
      "$id": 22,
      "label": "kizunatalk057_msg0022",
      "style": 164,
      "name": "저기 울고 있는 아이를 도와줘 보는 건 어떻지?"
    },
    {
      "$id": 23,
      "label": "kizunatalk057_msg0023",
      "style": 164,
      "name": "저 곤경에 처한 어르신을 도와드리는 건 어떻지?"
    },
    {
      "$id": 24,
      "label": "kizunatalk057_msg0024",
      "style": 165,
      "name": "글쎄다…"
    },
    {
      "$id": 25,
      "label": "kizunatalk057_msg0025",
      "style": 164,
      "name": "저기 울고 있는 아를 도와줘 볼래?"
    },
    {
      "$id": 26,
      "label": "kizunatalk057_msg0026",
      "style": 164,
      "name": "저 곤경에 처한 어르신을 도와드리 볼래?"
    },
    {
      "$id": 27,
      "label": "kizunatalk057_msg0101",
      "style": 84,
      "name": "좋은 생각이야모!"
    },
    {
      "$id": 28,
      "label": "kizunatalk057_msg0102",
      "style": 84,
      "name": "어려움에 처한 사람을 도우면\n지카라오의 상냥함이 꼭 전해질 거야모!"
    },
    {
      "$id": 29,
      "label": "kizunatalk057_msg0103",
      "style": 84,
      "name": "알겠다\n내가 도와주지"
    },
    {
      "$id": 30,
      "label": "kizunatalk057_msg0104",
      "style": 84,
      "name": "토라도 같이 가자"
    },
    {
      "$id": 31,
      "label": "kizunatalk057_msg0105",
      "style": 84,
      "name": "알았어모!"
    },
    {
      "$id": 32,
      "label": "kizunatalk057_msg0106",
      "style": 84,
      "name": "아아~앙!\n으앙~!!"
    },
    {
      "$id": 33,
      "label": "kizunatalk057_msg0107",
      "style": 84,
      "name": "무슨 일 있어모?\n왜 울고 있니모?"
    },
    {
      "$id": 34,
      "label": "kizunatalk057_msg0108",
      "style": 84,
      "name": "흑…\n엄마가 없어졌어"
    },
    {
      "$id": 35,
      "label": "kizunatalk057_msg0109",
      "style": 84,
      "name": "미아인가?"
    },
    {
      "$id": 36,
      "label": "kizunatalk057_msg0110",
      "style": 84,
      "name": "히익!\n몬스터?!"
    },
    {
      "$id": 37,
      "label": "kizunatalk057_msg0111",
      "style": 84,
      "name": "아냐모! 아냐모!"
    },
    {
      "$id": 38,
      "label": "kizunatalk057_msg0112",
      "style": 84,
      "name": "몬스터처럼 생겼어도\n몬스터 아니야모!"
    },
    {
      "$id": 39,
      "label": "kizunatalk057_msg0113",
      "style": 84,
      "name": "나는 사람을 잡아먹지 않는다\n안심해라!"
    },
    {
      "$id": 40,
      "label": "kizunatalk057_msg0114",
      "style": 84,
      "name": "히익!"
    },
    {
      "$id": 41,
      "label": "kizunatalk057_msg0115",
      "style": 84,
      "name": "아~ 그런 괜한 말했다간\n또 무서워할 거야모!"
    },
    {
      "$id": 42,
      "label": "kizunatalk057_msg0116",
      "style": 84,
      "name": "미… 미안하다\n토라, 아이를 부탁한다"
    },
    {
      "$id": 43,
      "label": "kizunatalk057_msg0117",
      "style": 84,
      "name": "나는 아이의 부모를 찾아보겠다"
    },
    {
      "$id": 44,
      "label": "kizunatalk057_msg0118",
      "style": 84,
      "name": "거기!\n우리 애한테 무슨 짓이야?!"
    },
    {
      "$id": 45,
      "label": "kizunatalk057_msg0119",
      "style": 84,
      "name": "어머니인가?"
    },
    {
      "$id": 46,
      "label": "kizunatalk057_msg0120",
      "style": 84,
      "name": "몬스터?!\n누가 좀 도와줘요!!"
    },
    {
      "$id": 47,
      "label": "kizunatalk057_msg0121",
      "style": 84,
      "name": "윽?\n아니다"
    },
    {
      "$id": 48,
      "label": "kizunatalk057_msg0122",
      "style": 84,
      "name": "누가 좀 도와주세요!!\n몬스터가 우리 아이를\n잡아먹으려 해요!"
    },
    {
      "$id": 49,
      "label": "kizunatalk057_msg0123",
      "style": 84,
      "name": "아니다… 아니다!"
    },
    {
      "$id": 50,
      "label": "kizunatalk057_msg0124",
      "style": 84,
      "name": "나는 몬스터가 아니다\n아니다!!"
    },
    {
      "$id": 51,
      "label": "kizunatalk057_msg0125",
      "style": 84,
      "name": "지카라오\n침착해모!"
    },
    {
      "$id": 52,
      "label": "kizunatalk057_msg0126",
      "style": 84,
      "name": "꺄악! 이쪽으로 오고 있어!\n누가 빨리 사람 좀 불러 줘요!"
    },
    {
      "$id": 53,
      "label": "kizunatalk057_msg0127",
      "style": 84,
      "name": "으으으\n으가아아아!"
    },
    {
      "$id": 54,
      "label": "kizunatalk057_msg0128",
      "style": 84,
      "name": "꺄아아아!"
    },
    {
      "$id": 55,
      "label": "kizunatalk057_msg0129",
      "style": 84,
      "name": "멈춰모! 지카라오!"
    },
    {
      "$id": 56,
      "label": "kizunatalk057_msg0130",
      "style": 84,
      "name": "소란 피웠다간 더 무서워한다고모!"
    },
    {
      "$id": 57,
      "label": "kizunatalk057_msg0131",
      "style": 84,
      "name": "엄마!"
    },
    {
      "$id": 58,
      "label": "kizunatalk057_msg0132",
      "style": 84,
      "name": "빨리 도망가자!"
    },
    {
      "$id": 59,
      "label": "kizunatalk057_msg0133",
      "style": 84,
      "name": "아… 실패했어모"
    },
    {
      "$id": 60,
      "label": "kizunatalk057_msg0134",
      "style": 84,
      "name": "으으…\n또 실패했다"
    },
    {
      "$id": 61,
      "label": "kizunatalk057_msg0135",
      "style": 84,
      "name": "그렇게 낙담할 것 없어\n오늘은 어쩌다 잘 안됐을 뿐이야"
    },
    {
      "$id": 62,
      "label": "kizunatalk057_msg0136",
      "style": 84,
      "name": "차근차근 사람들을 이해시켜 가면 돼"
    },
    {
      "$id": 63,
      "label": "kizunatalk057_msg0137",
      "style": 84,
      "name": "뭘 그렇게 낙담하고 그래\n오늘은 어쩌다 잘 안됐을 뿐이야"
    },
    {
      "$id": 64,
      "label": "kizunatalk057_msg0138",
      "style": 84,
      "name": "차근차근 사람들을 이해시켜 가면 돼"
    },
    {
      "$id": 65,
      "label": "kizunatalk057_msg0139",
      "style": 84,
      "name": "그렇게 낙담할 것 없다\n오늘은 어쩌다 잘 안됐을 뿐이니"
    },
    {
      "$id": 66,
      "label": "kizunatalk057_msg0140",
      "style": 84,
      "name": "차근차근 사람들을 이해시켜 보자"
    },
    {
      "$id": 67,
      "label": "kizunatalk057_msg0141",
      "style": 84,
      "name": "너무 침울해하지 마라\n오늘은 날이 아니었을 뿐인 기다"
    },
    {
      "$id": 68,
      "label": "kizunatalk057_msg0142",
      "style": 84,
      "name": "천천히 사람들을\n이해시키면 된다"
    },
    {
      "$id": 69,
      "label": "kizunatalk057_msg0143",
      "style": 84,
      "name": "고맙다"
    },
    {
      "$id": 70,
      "label": "kizunatalk057_msg0144",
      "style": 84,
      "name": "난 포기하지 않는다"
    },
    {
      "$id": 71,
      "label": "kizunatalk057_msg0145",
      "style": 84,
      "name": "반드시 사람들과 이야기하겠다!"
    },
    {
      "$id": 72,
      "label": "kizunatalk057_msg0201",
      "style": 84,
      "name": "좋은 생각이야모!"
    },
    {
      "$id": 73,
      "label": "kizunatalk057_msg0202",
      "style": 84,
      "name": "어려움에 처한 사람을 도우면\n지카라오의 상냥함이 꼭 전해질 거야모!"
    },
    {
      "$id": 74,
      "label": "kizunatalk057_msg0203",
      "style": 84,
      "name": "지카라오\n토라도 따라갈게모"
    },
    {
      "$id": 75,
      "label": "kizunatalk057_msg0204",
      "style": 84,
      "name": "좋다!"
    },
    {
      "$id": 76,
      "label": "kizunatalk057_msg0205",
      "style": 84,
      "name": "하아…"
    },
    {
      "$id": 77,
      "label": "kizunatalk057_msg0206",
      "style": 84,
      "name": "할머니, 왜 그래모?"
    },
    {
      "$id": 78,
      "label": "kizunatalk057_msg0207",
      "style": 84,
      "name": "아, 집에 돌아가려는데\n갑자기 몸이 좀 불편하네요…"
    },
    {
      "$id": 79,
      "label": "kizunatalk057_msg0208",
      "style": 84,
      "name": "제가 원래 눈이 나쁜데\n이럴 때면 앞이 영\n제대로 보이지가 않으니…"
    },
    {
      "$id": 80,
      "label": "kizunatalk057_msg0209",
      "style": 84,
      "name": "그건 위험하다\n내가 안내하지"
    },
    {
      "$id": 81,
      "label": "kizunatalk057_msg0210",
      "style": 84,
      "name": "아이고 친절하기도 하셔라\n고마워요"
    },
    {
      "$id": 82,
      "label": "kizunatalk057_msg0211",
      "style": 84,
      "name": "걸을 수 있어모?"
    },
    {
      "$id": 83,
      "label": "kizunatalk057_msg0212",
      "style": 84,
      "name": "그럼요, 걸을 수는 있죠"
    },
    {
      "$id": 84,
      "label": "kizunatalk057_msg0213",
      "style": 84,
      "name": "아이고\n이게 뭐람?"
    },
    {
      "$id": 85,
      "label": "kizunatalk057_msg0214",
      "style": 84,
      "name": "이건 무슨 바위인가요?"
    },
    {
      "$id": 86,
      "label": "kizunatalk057_msg0215",
      "style": 84,
      "name": "그건 내 몸이다"
    },
    {
      "$id": 87,
      "label": "kizunatalk057_msg0216",
      "style": 84,
      "name": "어머나, 대단하네요\n몸이 아주 튼튼하시네요"
    },
    {
      "$id": 88,
      "label": "kizunatalk057_msg0217",
      "style": 84,
      "name": "그래, 꼭 붙잡아라"
    },
    {
      "$id": 89,
      "label": "kizunatalk057_msg0218",
      "style": 84,
      "name": "뭐야 저거?!"
    },
    {
      "$id": 90,
      "label": "kizunatalk057_msg0219",
      "style": 84,
      "name": "몬스터가 있다!"
    },
    {
      "$id": 91,
      "label": "kizunatalk057_msg0220",
      "style": 84,
      "name": "주위가 왠지 시끌시끌하네요?"
    },
    {
      "$id": 92,
      "label": "kizunatalk057_msg0221",
      "style": 84,
      "name": "아무것도 아니다\n몬스터가 나왔나 보군"
    },
    {
      "$id": 93,
      "label": "kizunatalk057_msg0222",
      "style": 84,
      "name": "아이고 큰일 났네\n빨리 도망가야겠네요"
    },
    {
      "$id": 94,
      "label": "kizunatalk057_msg0223",
      "style": 84,
      "name": "괜찮다\n내가 지키겠다!"
    },
    {
      "$id": 95,
      "label": "kizunatalk057_msg0224",
      "style": 84,
      "name": "어머나\n듬직하기도 하시지"
    },
    {
      "$id": 96,
      "label": "kizunatalk057_msg0225",
      "style": 84,
      "name": "여기가 집인가?"
    },
    {
      "$id": 97,
      "label": "kizunatalk057_msg0226",
      "style": 84,
      "name": "네~ 네~ 맞아요\n덕분에 무사히 잘 왔습니다"
    },
    {
      "$id": 98,
      "label": "kizunatalk057_msg0227",
      "style": 84,
      "name": "저기, 이름이 어떻게 되시는지요?"
    },
    {
      "$id": 99,
      "label": "kizunatalk057_msg0228",
      "style": 84,
      "name": "나는 지카라오다"
    },
    {
      "$id": 100,
      "label": "kizunatalk057_msg0229",
      "style": 84,
      "name": "지카라오 씨\n정말 감사합니다"
    },
    {
      "$id": 101,
      "label": "kizunatalk057_msg0230",
      "style": 84,
      "name": "그래, 잘 지내라!"
    },
    {
      "$id": 102,
      "label": "kizunatalk057_msg0231",
      "style": 84,
      "name": "고생했어, 지카라오"
    },
    {
      "$id": 103,
      "label": "kizunatalk057_msg0232",
      "style": 84,
      "name": "계속 이렇게 하면 반드시\n모두에게 지카라오의 다정함이 전해질 거야"
    },
    {
      "$id": 104,
      "label": "kizunatalk057_msg0233",
      "style": 84,
      "name": "잘했어, 지카라오"
    },
    {
