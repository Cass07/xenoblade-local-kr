      "name": "style",
      "type": 2
    },
    {
      "name": "name",
      "type": 7
    }
  ],
  "rows": [
    {
      "$id": 1,
      "label": "qst201107_msg0036",
      "style": 84,
      "name": "…………"
    },
    {
      "$id": 2,
      "label": "qst201107_msg0037",
      "style": 84,
      "name": "저기, 렉스, 넌 알고 있지?"
    },
    {
      "$id": 3,
      "label": "qst201107_msg0038",
      "style": 84,
      "name": "…어쩌면 미쿠마리는\n모르는 게 좋을지도 몰라"
    },
    {
      "$id": 4,
      "label": "qst201107_msg0039",
      "style": 84,
      "name": "진실을 알면 분명…"
    },
    {
      "$id": 5,
      "label": "qst201107_msg0040",
      "style": 84,
      "name": "하지만 신경 쓰여!"
    },
    {
      "$id": 6,
      "label": "qst201107_msg0041",
      "style": 84,
      "name": "내 성격 알지?\n절대 우울해하지 않고\n긍정적으로 금세 기운을 되찾잖아"
    },
    {
      "$id": 7,
      "label": "qst201107_msg0042",
      "style": 84,
      "name": "렉스…\n미쿠마리를 믿고 말해 주자"
    },
    {
      "$id": 8,
      "label": "qst201107_msg0043",
      "style": 84,
      "name": "응…\n그래"
    },
    {
      "$id": 9,
      "label": "qst201107_msg0044",
      "style": 84,
      "name": "고마워"
    },
    {
      "$id": 10,
      "label": "qst201107_msg0001",
      "style": 84,
      "name": "저 애의 이름은 세오리야"
    },
    {
      "$id": 11,
      "label": "qst201107_msg0002",
      "style": 84,
      "name": "세오리…"
    },
    {
      "$id": 12,
      "label": "qst201107_msg0003",
      "style": 84,
      "name": "그래, 세오리와 미쿠마리는\n동료였어"
    },
    {
      "$id": 13,
      "label": "qst201107_msg0004",
      "style": 84,
      "name": "…동료라면 지금의 우리처럼\n함께 있었다는 거구나"
    },
    {
      "$id": 14,
      "label": "qst201107_msg0005",
      "style": 84,
      "name": "맞아…\n하지만 좋은 일을 하지는 않았어"
    },
    {
      "$id": 15,
      "label": "qst201107_msg0006",
      "style": 84,
      "name": "둘은 한때 코어 크리스털 사냥이라고 해서\n여기저기서 사람을 습격하고\n코어 크리스털을 빼앗았지"
    },
    {
      "$id": 16,
      "label": "qst201107_msg0007",
      "style": 84,
      "name": "내가 그런 나쁜 짓을…?"
    },
    {
      "$id": 17,
      "label": "qst201107_msg0008",
      "style": 84,
      "name": "그럼 나는 모두의 적이었어?"
    },
    {
      "$id": 18,
      "label": "qst201107_msg0009",
      "style": 84,
      "name": "응, 그랬지"
    },
    {
      "$id": 19,
      "label": "qst201107_msg0010",
      "style": 84,
      "name": "코어 크리스털 사냥을 막기 위해\n우리는 수페르비아 제국군과 협력해서\n미쿠마리 일행들과 싸웠어…"
    },
    {
      "$id": 20,
      "label": "qst201107_msg0011",
      "style": 84,
      "name": "…그리고\n미쿠마리의 드라이버가 죽었지"
    },
    {
      "$id": 21,
      "label": "qst201107_msg0012",
      "style": 84,
      "name": "그럼 그때 나는 다시\n코어 크리스털로 되돌아갔구나…"
    },
    {
      "$id": 22,
      "label": "qst201107_msg0013",
      "style": 84,
      "name": "그래, 그리고 우리가 그걸 회수했어"
    },
    {
      "$id": 23,
      "label": "qst201107_msg0014",
      "style": 84,
      "name": "하지만 세오리와 그녀의 드라이버는\n놓치고 말았지"
    },
    {
      "$id": 24,
      "label": "qst201107_msg0015",
      "style": 84,
      "name": "그 뒤로 계속 행방을\n알지 못했는데 말이야"
    },
    {
      "$id": 25,
      "label": "qst201107_msg0016",
      "style": 84,
      "name": "내가 범죄자의 동료였다니…"
    },
    {
      "$id": 26,
      "label": "qst201107_msg0045",
      "style": 84,
      "name": "…………"
    },
    {
      "$id": 27,
      "label": "qst201107_msg0017",
      "style": 84,
      "name": "미쿠마리…\n예전 일은 너무 신경 쓰지 마…"
    },
    {
      "$id": 28,
      "label": "qst201107_msg0018",
      "style": 84,
      "name": "지금의 미쿠마리는 우리의 동료니까"
    },
    {
      "$id": 29,
      "label": "qst201107_msg0019",
      "style": 84,
      "name": "…하지만 어쩌면\n그 세오리라는 아이는\n그렇게 생각하지 않는 거 아니야?"
    },
    {
      "$id": 30,
      "label": "qst201107_msg0020",
      "style": 84,
      "name": "분명 그 애가 보기에\n나는 배신자야, 그러니까…"
    },
    {
      "$id": 31,
      "label": "qst201107_msg0021",
      "style": 84,
      "name": "진정해라, 미쿠마리"
    },
    {
      "$id": 32,
      "label": "qst201107_msg0022",
      "style": 84,
      "name": "세오리가 너를 어떻게 생각하는지는\n그녀를 만나 보지 않으면 몰라"
    },
    {
      "$id": 33,
      "label": "qst201107_msg0023",
      "style": 84,
      "name": "하지만 그녀를 만나면\n너는 괴로워하겠지"
    },
    {
      "$id": 34,
      "label": "qst201107_msg0024",
      "style": 84,
      "name": "그래도 만나러 갈 건가?"
    },
    {
      "$id": 35,
      "label": "qst201107_msg0026",
      "style": 84,
      "name": "…………"
    },
    {
      "$id": 36,
      "label": "qst201107_msg0027",
      "style": 84,
      "name": "내가 예전에 악당이었다는 게\n너무 충격적이었어…"
    },
    {
      "$id": 37,
      "label": "qst201107_msg0028",
      "style": 84,
      "name": "하지만…\n우울해지면 안 되겠지"
    },
    {
      "$id": 38,
      "label": "qst201107_msg0029",
      "style": 84,
      "name": "왜냐면 모두가 날 도와줬던 것처럼\n나도 그 애를 돕고 싶은\n마음이 샘솟고 있거든…"
    },
    {
      "$id": 39,
      "label": "qst201107_msg0030",
      "style": 84,
      "name": "그러니까 난 그 애를 만나러 가고 싶어!"
    },
    {
      "$id": 40,
      "label": "qst201107_msg0031",
      "style": 84,
      "name": "…알았어, 미쿠마리\n다 같이 그녀를 도우러 가자!"
    },
    {
      "$id": 41,
      "label": "qst201107_msg0032",
      "style": 84,
      "name": "렉스, 괜찮겠나?"
    },
    {
      "$id": 42,
      "label": "qst201107_msg0033",
      "style": 84,
      "name": "여기서 고민해 봤자잖아\n…그리고 나도 세오리를 돕고 싶어"
    },
    {
      "$id": 43,
      "label": "qst201107_msg0034",
      "style": 84,
      "name": "가자, 미쿠마리!"
    },
    {
      "$id": 44,
      "label": "qst201107_msg0035",
      "style": 84,
      "name": "응!"
    }
  ]
}