  "rows": [
    {
      "$id": 1,
      "label": "kizunatalk076_msg0001",
      "style": 84,
      "name": "여기가 이라의 아르스 안쪽인가…"
    },
    {
      "$id": 2,
      "label": "kizunatalk076_msg0002",
      "style": 84,
      "name": "이곳에서 500년 전에\n장렬한 전투가 있었다지?"
    },
    {
      "$id": 3,
      "label": "kizunatalk076_msg0003",
      "style": 84,
      "name": "응, 그랬지"
    },
    {
      "$id": 4,
      "label": "kizunatalk076_msg0004",
      "style": 84,
      "name": "아~ 나도\n그 싸움에 참가했으면 좋았을 텐데~"
    },
    {
      "$id": 5,
      "label": "kizunatalk076_msg0005",
      "style": 84,
      "name": "아니, 기억이 없을 뿐이지\n어쩌면 거기 있었을 수도 있잖아?"
    },
    {
      "$id": 6,
      "label": "kizunatalk076_msg0006",
      "style": 84,
      "name": "아니, 기억이 없을 뿐이지\n어쩌면 거기 있었을지도 모르지?"
    },
    {
      "$id": 7,
      "label": "kizunatalk076_msg0007",
      "style": 84,
      "name": "아니, 기억을 잃어버렸을 뿐이지\n참여했을 수도 있을 텐데?"
    },
    {
      "$id": 8,
      "label": "kizunatalk076_msg0008",
      "style": 84,
      "name": "아이다, 기억 못 하는 것뿐이지\n어쩌면 거기 있었을지도 모른다 아이가?"
    },
    {
      "$id": 9,
      "label": "kizunatalk076_msg0009",
      "style": 84,
      "name": "그러고 보니\n그런 마음 설레는 가능성도 있네!"
    },
    {
      "$id": 10,
      "label": "kizunatalk076_msg0010",
      "style": 84,
      "name": "히카리는 기억해?"
    },
    {
      "$id": 11,
      "label": "kizunatalk076_msg0012",
      "style": 84,
      "name": "있었던 것 같기도 하고\n없었던 것 같기도 하고"
    },
    {
      "$id": 12,
      "label": "kizunatalk076_msg0013",
      "style": 84,
      "name": "애매하게 그게 뭐야~\n어느 쪽인 건데?"
    },
    {
      "$id": 13,
      "label": "kizunatalk076_msg0015",
      "style": 84,
      "name": "500년도 더 전에 누가 있었는지를\n일일이 기억하고 있을 리가 없잖아"
    },
    {
      "$id": 14,
      "label": "kizunatalk076_msg0016",
      "style": 84,
      "name": "쳇, 아쉽게 됐네"
    },
    {
      "$id": 15,
      "label": "kizunatalk076_msg0017",
      "style": 84,
      "name": "너무 실망하지 마\n그런데 야에기리가 있었다면…"
    },
    {
      "$id": 16,
      "label": "kizunatalk076_msg0018",
      "style": 165,
      "name": "뭐라고 말할까?"
    },
    {
      "$id": 17,
      "label": "kizunatalk076_msg0019",
      "style": 164,
      "name": "히카리와 싸웠을까?"
    },
    {
      "$id": 18,
      "label": "kizunatalk076_msg0020",
      "style": 164,
      "name": "히카리와 동료였을까?"
    },
    {
      "$id": 19,
      "label": "kizunatalk076_msg0021",
      "style": 84,
      "name": "너무 실망하지 마\n그런데 야에기리가 있었다면…"
    },
    {
      "$id": 20,
      "label": "kizunatalk076_msg0022",
      "style": 165,
      "name": "뭐라고 말할까?"
    },
    {
      "$id": 21,
      "label": "kizunatalk076_msg0023",
      "style": 164,
      "name": "히카리와 싸웠을까?"
    },
    {
      "$id": 22,
      "label": "kizunatalk076_msg0024",
      "style": 164,
      "name": "히카리와 동료였을까?"
    },
    {
      "$id": 23,
      "label": "kizunatalk076_msg0025",
      "style": 84,
      "name": "너무 낙담할 것 없어\n하지만 야에기리가 참가했다면…"
    },
    {
      "$id": 24,
      "label": "kizunatalk076_msg0026",
      "style": 165,
      "name": "뭐라고 말하지?"
    },
    {
      "$id": 25,
      "label": "kizunatalk076_msg0027",
      "style": 164,
      "name": "히카리와 적이었을까?"
    },
    {
      "$id": 26,
      "label": "kizunatalk076_msg0028",
      "style": 164,
      "name": "히카리와 동료였을까?"
    },
    {
      "$id": 27,
      "label": "kizunatalk076_msg0029",
      "style": 84,
      "name": "그래 실망할 거 없다\n그런데 야에기리가 있었다면…"
    },
    {
      "$id": 28,
      "label": "kizunatalk076_msg0030",
      "style": 165,
      "name": "뭐라 말하지?"
    },
    {
      "$id": 29,
      "label": "kizunatalk076_msg0031",
      "style": 164,
      "name": "히카리랑 싸웠으까?"
    },
    {
      "$id": 30,
      "label": "kizunatalk076_msg0032",
      "style": 164,
      "name": "히카리랑 동료였을지도 모르겠네"
    },
    {
      "$id": 31,
      "label": "kizunatalk076_msg0101",
      "style": 84,
      "name": "그래!\n그것 말고는 있을 수 없지!"
    },
    {
      "$id": 32,
      "label": "kizunatalk076_msg0102",
      "style": 84,
      "name": "그야말로 서로 사력을 다해서\n싸움을 벌였을 거야"
    },
    {
      "$id": 33,
      "label": "kizunatalk076_msg0103",
      "style": 84,
      "name": "나는 언제나\n최강의 블레이드가 되려 했을 테고"
    },
    {
      "$id": 34,
      "label": "kizunatalk076_msg0104",
      "style": 84,
      "name": "그렇다면 히카리는\n최고의 호적수니까 말이야!"
    },
    {
      "$id": 35,
      "label": "kizunatalk076_msg0105",
      "style": 84,
      "name": "어떻게 생각해, 히카리?"
    },
    {
      "$id": 36,
      "label": "kizunatalk076_msg0106",
      "style": 84,
      "name": "히카리는 어떻게 생각해?"
    },
    {
      "$id": 37,
      "label": "kizunatalk076_msg0107",
      "style": 84,
      "name": "히카리, 저 말이 맞나?"
    },
    {
      "$id": 38,
      "label": "kizunatalk076_msg0108",
      "style": 84,
      "name": "맞나, 히카리?"
    },
    {
      "$id": 39,
      "label": "kizunatalk076_msg0109",
      "style": 84,
      "name": "글쎄…"
    },
    {
      "$id": 40,
      "label": "kizunatalk076_msg0110",
      "style": 84,
      "name": "야에기리 같은 블레이드랑 싸웠다면\n아무래도 기억에 남았겠지"
    },
    {
      "$id": 41,
      "label": "kizunatalk076_msg0111",
      "style": 84,
      "name": "아마 당시에는 없지 않았을까?"
    },
    {
      "$id": 42,
      "label": "kizunatalk076_msg0112",
      "style": 84,
      "name": "뭐, 있었다고 해도 내가 상대하는 대신\n누군가에게 맡기거나 했겠지?"
    },
    {
      "$id": 43,
      "label": "kizunatalk076_msg0201",
      "style": 84,
      "name": "동료? 히카리의?"
    },
    {
      "$id": 44,
      "label": "kizunatalk076_msg0202",
      "style": 84,
      "name": "흠…\n하긴 있을 법하긴 하네"
    },
    {
      "$id": 45,
      "label": "kizunatalk076_msg0203",
      "style": 84,
      "name": "아니! 그건 아니지!"
    },
    {
      "$id": 46,
      "label": "kizunatalk076_msg0204",
      "style": 84,
      "name": "어떻게 단언할 수 있는데?"
    },
    {
      "$id": 47,
      "label": "kizunatalk076_msg0205",
      "style": 84,
      "name": "나는 아르스트 최강이 되려는\n블레이드잖아?"
    },
    {
      "$id": 48,
      "label": "kizunatalk076_msg0206",
      "style": 84,
      "name": "아르스트가 어떤 상황이건 간에\n분명 히카리에게 싸움을 걸었겠지"
    },
    {
      "$id": 49,
      "label": "kizunatalk076_msg0207",
      "style": 84,
      "name": "전쟁이 한창이었던 시기야\n그런 개인적인 이유는 상관없었을걸?"
    },
    {
      "$id": 50,
      "label": "kizunatalk076_msg0208",
      "style": 84,
      "name": "아니, 내가 싸우는 이유는 단 하나야!"
    },
    {
      "$id": 51,
      "label": "kizunatalk076_msg0209",
      "style": 84,
      "name": "드라이버가 누구든 간에\n그것은 절대로 변하지 않지"
    },
    {
      "$id": 52,
      "label": "kizunatalk076_msg0210",
      "style": 84,
      "name": "어느 시대든 간에\n강한 녀석을 찾아 싸우는 거야!"
    },
    {
      "$id": 53,
      "label": "kizunatalk076_msg0211",
      "style": 84,
      "name": "뭔가 이유가 나사 빠지긴 했는데\n그래도 야에기리답네"
    },
    {
      "$id": 54,
      "label": "kizunatalk076_msg0212",
      "style": 84,
      "name": "그만큼 히카리가 강하다는 걸\n인정하고 있다는 말이겠지?"
    },
    {
      "$id": 55,
      "label": "kizunatalk076_msg0213",
      "style": 84,
      "name": "그만큼 히카리가 강하다는 걸\n인정하고 있다는 말이겠지?"
    },
    {
      "$id": 56,
      "label": "kizunatalk076_msg0214",
      "style": 84,
      "name": "히카리가 강하다는 걸\n그만큼 인정하고 있다는 말이겠지?"
    },
    {
      "$id": 57,
      "label": "kizunatalk076_msg0215",
      "style": 84,
      "name": "그만큼 히카리가 강하다는 걸\n인정하고 있다는 기제?"
    },
    {
      "$id": 58,
      "label": "kizunatalk076_msg0216",
      "style": 84,
      "name": "뭐, 그런 고집은 싫지 않아"
    },
    {
      "$id": 59,
      "label": "kizunatalk076_msg0217",
      "style": 84,
      "name": "그런 마음만 있다면\n언젠가 꼭 최강의 이름을 얻을 테니까"
    },
    {
      "$id": 60,
      "label": "kizunatalk076_msg0301",
      "style": 84,
      "name": "음… 보아하니\n아직도 나와 싸워 줄 것 같진 않네"
    },
    {
      "$id": 61,
      "label": "kizunatalk076_msg0302",
      "style": 84,
      "name": "모의전이라도 좋으니까\n상대해 달라고는 하는데…"
    },
    {
      "$id": 62,
      "label": "kizunatalk076_msg0303",
      "style": 84,
      "name": "항상 적당하게만 대하거든"
    },
    {
      "$id": 63,
      "label": "kizunatalk076_msg0304",
      "style": 84,
      "name": "하긴 히카리가\n싸울 마음이 없는 것 같긴 해"
    },
    {
      "$id": 64,
      "label": "kizunatalk076_msg0305",
      "style": 84,
      "name": "하긴 히카리가\n싸울 마음이 없는 것 같긴 해"
    },
    {
      "$id": 65,
      "label": "kizunatalk076_msg0306",
      "style": 84,
      "name": "그러게\n히카리는 싸울 마음이 없는 것 같군"
    },
    {
      "$id": 66,
      "label": "kizunatalk076_msg0307",
      "style": 84,
      "name": "그러네\n히카리는 싸울 생각이 없는갑다"
    },
    {
      "$id": 67,
      "label": "kizunatalk076_msg0308",
      "style": 84,
      "name": "뭐, 지금은 그걸로 됐어!"
    },
    {
      "$id": 68,
      "label": "kizunatalk076_msg0309",
      "style": 84,
      "name": "나도 아직 수행 중이니까"
    },
    {
      "$id": 69,
      "label": "kizunatalk076_msg0310",
      "style": 84,
      "name": "드라이버와 함께 좀 더\n많은 녀석들과 싸워서 경험을 쌓고"
    },
    {
      "$id": 70,
      "label": "kizunatalk076_msg0311",
      "style": 84,
      "name": "언젠가 히카리를 넘어서 보이겠어!"
    }
  ]
}