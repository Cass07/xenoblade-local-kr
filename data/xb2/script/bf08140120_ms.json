  ],
  "rows": [
    {
      "$id": 1,
      "label": "bf08140120_0010",
      "style": 82,
      "name": "조용하네요모"
    },
    {
      "$id": 2,
      "label": "bf08140120_0020",
      "style": 82,
      "name": "그러게…"
    },
    {
      "$id": 3,
      "label": "bf08140120_0030",
      "style": 82,
      "name": "여기는 과학 기술이 엄청나게\n발달한 곳이었어요모"
    },
    {
      "$id": 4,
      "label": "bf08140120_0040",
      "style": 82,
      "name": "수많은 건물들과 사람들이 있고"
    },
    {
      "$id": 5,
      "label": "bf08140120_0050",
      "style": 82,
      "name": "아마 모두 행복하게 살았을 거예요모"
    },
    {
      "$id": 6,
      "label": "bf08140120_0060",
      "style": 82,
      "name": "하지만…"
    },
    {
      "$id": 7,
      "label": "bf08140120_0070",
      "style": 82,
      "name": "아무리 과학 기술로 삶이 풍족해져도\n사람의 본질은 변하지 않아"
    },
    {
      "$id": 8,
      "label": "bf08140120_0080",
      "style": 82,
      "name": "풍요를 가져다줄 거라고 생각했던 과학이\n세계를 파괴했지"
    },
    {
      "$id": 9,
      "label": "bf08140120_0090",
      "style": 82,
      "name": "지금은… 황량한 폐허야"
    },
    {
      "$id": 10,
      "label": "bf08140120_0100",
      "style": 82,
      "name": "하나도 과학 기술로 만들어졌어요모"
    },
    {
      "$id": 11,
      "label": "bf08140120_0110",
      "style": 82,
      "name": "하나도…"
    },
    {
      "$id": 12,
      "label": "bf08140120_0120",
      "style": 82,
      "name": "언젠가 세계를 파괴하는 걸까요모…"
    },
    {
      "$id": 13,
      "label": "bf08140120_0130",
      "style": 82,
      "name": "그건 나도 마찬가지야"
    },
    {
      "$id": 14,
      "label": "bf08140120_0140",
      "style": 82,
      "name": "이 세계가 \"아버지\"의 세계라면"
    },
    {
      "$id": 15,
      "label": "bf08140120_0150",
      "style": 82,
      "name": "그 \"아버지\"가 만든 나도 마찬가지…"
    },
    {
      "$id": 16,
      "label": "bf08140120_0160",
      "style": 82,
      "name": "히카리도 하나와 같아요모?"
    },
    {
      "$id": 17,
      "label": "bf08140120_0170",
      "style": 82,
      "name": "그래, 맞아"
    },
    {
      "$id": 18,
      "label": "bf08140120_0180",
      "style": 82,
      "name": "그럼…\n그럼 만약에 하나가 세계를…"
    },
    {
      "$id": 19,
      "label": "bf08140120_0190",
      "style": 82,
      "name": "주인을 부수려고 하면"
    },
    {
      "$id": 20,
      "label": "bf08140120_0200",
      "style": 82,
      "name": "하나를 부숴 줄래요모?"
    },
    {
      "$id": 21,
      "label": "bf08140120_0210",
      "style": 82,
      "name": "하나…"
    },
    {
      "$id": 22,
      "label": "bf08140120_0220",
      "style": 82,
      "name": "토라가 만든 너는 그런 짓 안 해\n절대로"
    },
    {
      "$id": 23,
      "label": "bf08140120_0230",
      "style": 82,
      "name": "하지만…"
    },
    {
      "$id": 24,
      "label": "bf08140120_0240",
      "style": 82,
      "name": "으음…"
    },
    {
      "$id": 25,
      "label": "bf08140120_0250",
      "style": 82,
      "name": "그럼 약속하자"
    },
    {
      "$id": 26,
      "label": "bf08140120_0260",
      "style": 82,
      "name": "약속…이요모?"
    },
    {
      "$id": 27,
      "label": "bf08140120_0270",
      "style": 82,
      "name": "그래, 약속"
    },
    {
      "$id": 28,
      "label": "bf08140120_0280",
      "style": 82,
      "name": "만약 정말로 하나가\n세계를 부수려고 하면"
    },
    {
      "$id": 29,
      "label": "bf08140120_0290",
      "style": 82,
      "name": "내가 하나의 소원을 들어줄게"
    },
    {
      "$id": 30,
      "label": "bf08140120_0300",
      "style": 82,
      "name": "그러니까 그 대신 하나도\n내 소원을 하나 들어줘"
    },
    {
      "$id": 31,
      "label": "bf08140120_0310",
      "style": 82,
      "name": "무슨 소원인데요모?"
    },
    {
      "$id": 32,
      "label": "bf08140120_0320",
      "style": 82,
      "name": "히카리도 세계를 부술 것 같아요모?"
    },
    {
      "$id": 33,
      "label": "bf08140120_0330",
      "style": 82,
      "name": "그렇게 안 되게 조심할 거니까\n그런 일은 없을 거야"
    },
    {
      "$id": 34,
      "label": "bf08140120_0340",
      "style": 82,
      "name": "하지만…"
    },
    {
      "$id": 35,
      "label": "bf08140120_0350",
      "style": 82,
      "name": "하지만?"
    },
    {
      "$id": 36,
      "label": "bf08140120_0360",
      "style": 82,
      "name": "아니, 아무것도 아냐"
    },
    {
      "$id": 37,
      "label": "bf08140120_0370",
      "style": 82,
      "name": "아직 이렇다 할 소원은 없어"
    },
    {
      "$id": 38,
      "label": "bf08140120_0380",
      "style": 82,
      "name": "정한 뒤에 얘기해도 돼?"
    },
    {
      "$id": 39,
      "label": "bf08140120_0390",
      "style": 82,
      "name": "알겠어요모"
    },
    {
      "$id": 40,
      "label": "bf08140120_0400",
      "style": 82,
      "name": "우리 둘만의 약속이에요모"
    },
    {
      "$id": 41,
      "label": "bf08140120_0410",
      "style": 82,
      "name": "뭐야?"
    },
    {
      "$id": 42,
      "label": "bf08140120_0420",
      "style": 82,
      "name": "주인이 가르쳐 준\n노폰식 약속의 의식이에요모"
    },
    {
      "$id": 43,
      "label": "bf08140120_0430",
      "style": 82,
      "name": "이렇게 손을 높이 들고 짝 하는 거예요모"
    },
    {
      "$id": 44,
      "label": "bf08140120_0440",
      "style": 82,
      "name": "그렇구나…"
    }
  ]
}