  "rows": [
    {
      "$id": 1,
      "label": "bf05410100_0010",
      "style": 82,
      "name": "끔찍해"
    },
    {
      "$id": 2,
      "label": "bf05410100_0020",
      "style": 82,
      "name": "저항할 틈도 없었는갑네"
    },
    {
      "$id": 3,
      "label": "bf05410100_0030",
      "style": 82,
      "name": "왔다! 저쪽이야!"
    },
    {
      "$id": 4,
      "label": "bf05410100_0040",
      "style": 82,
      "name": "위험해! 피해!"
    },
    {
      "$id": 5,
      "label": "bf05410100_0050",
      "style": 82,
      "name": "[ML:undisp ]"
    },
    {
      "$id": 6,
      "label": "bf05410100_0060",
      "style": 82,
      "name": "다들, 괜찮아?"
    },
    {
      "$id": 7,
      "label": "bf05410100_0070",
      "style": 82,
      "name": "그래, 일단 괜찮다"
    },
    {
      "$id": 8,
      "label": "bf05410100_0080",
      "style": 82,
      "name": "막아야 해"
    },
    {
      "$id": 9,
      "label": "bf05410100_0090",
      "style": 82,
      "name": "하지만 무슨 수로?"
    },
    {
      "$id": 10,
      "label": "bf05410100_0110",
      "style": 82,
      "name": "등 쪽에 수페르비아제 보조 구동기가 있다"
    },
    {
      "$id": 11,
      "label": "bf05410100_0120",
      "style": 82,
      "name": "거기서 각 다리로 연결된 케이블을 절단하면"
    },
    {
      "$id": 12,
      "label": "bf05410100_0130",
      "style": 82,
      "name": "에너지 공급이 끊겨서 멈출 거다"
    },
    {
      "$id": 13,
      "label": "bf05410100_0140",
      "style": 82,
      "name": "히카리 님의 힘으로 파괴할 수 없을까요?"
    },
    {
      "$id": 14,
      "label": "bf05410100_0150",
      "style": 82,
      "name": "그거야!"
    },
    {
      "$id": 15,
      "label": "bf05410100_0160",
      "style": 82,
      "name": "파괴할 수는 있겠지"
    },
    {
      "$id": 16,
      "label": "bf05410100_0170",
      "style": 82,
      "name": "대신 이 주변 일대가\n잿더미가 될 거다"
    },
    {
      "$id": 17,
      "label": "bf05410100_0180",
      "style": 82,
      "name": "무슨 말이고?"
    },
    {
      "$id": 18,
      "label": "bf05410100_0190",
      "style": 82,
      "name": "예전에 읽었던 보고서에 따르면\n저 병기의 주무기는"
    },
    {
      "$id": 19,
      "label": "bf05410100_0200",
      "style": 82,
      "name": "가스화한 아르스의 체액을\n이용한 거라더군"
    },
    {
      "$id": 20,
      "label": "bf05410100_0210",
      "style": 82,
      "name": "가스화한 체액은 매우 불안정해서\n작은 충격이나 열을 가하면 큰 폭발을 일으키지"
    },
    {
      "$id": 21,
      "label": "bf05410100_0220",
      "style": 82,
      "name": "실제로 3개월 전에\n여기서 대규모 폭발 사고가 있었다"
    },
    {
      "$id": 22,
      "label": "bf05410100_0230",
      "style": 82,
      "name": "모르고 공격했다면 엄청난 일이 벌어졌겠네"
    },
    {
      "$id": 23,
      "label": "bf05410100_0240",
      "style": 82,
      "name": "젠장"
    },
    {
      "$id": 24,
      "label": "bf05410100_0250",
      "style": 82,
      "name": "왜 그런 위험한 병기를?"
    },
    {
      "$id": 25,
      "label": "bf05410100_0260",
      "style": 82,
      "name": "만에 하나 적진에서 격퇴당한다 해도\n적도 함께 섬멸하겠다는"
    },
    {
      "$id": 26,
      "label": "bf05410100_0270",
      "style": 82,
      "name": "그런 설계 목표를 가지고 만들어졌다고 한다"
    },
    {
      "$id": 27,
      "label": "bf05410100_0280",
      "style": 82,
      "name": "그걸 생각해 낸 녀석은 제정신이 아니구마"
    },
    {
      "$id": 28,
      "label": "bf05410100_0290",
      "style": 82,
      "name": "그래서 폐하께선\n중지하라는 엄명을 내리셨지"
    },
    {
      "$id": 29,
      "label": "bf05410100_0300",
      "style": 82,
      "name": "저기에 탄 녀석을 끌어낼 방법은 없어?"
    },
    {
      "$id": 30,
      "label": "bf05410100_0310",
      "style": 82,
      "name": "10중 강화 장갑이 조종석을 보호하고 있다"
    },
    {
      "$id": 31,
      "label": "bf05410100_0320",
      "style": 82,
      "name": "밖에서 들어가거나 파괴하는 건 쉽지 않아"
    },
    {
      "$id": 32,
      "label": "bf05410100_0330",
      "style": 82,
      "name": "하지만 구동기와 케이블에는\n장갑으로 둘러싸이지 않은 부분이 있지"
    },
    {
      "$id": 33,
      "label": "bf05410100_0340",
      "style": 82,
      "name": "그럼 그걸 노리는 수밖에 없겠네"
    },
    {
      "$id": 34,
      "label": "bf05410100_0350",
      "style": 82,
      "name": "등에는 어떻게 올라탈 거야모?"
    },
    {
      "$id": 35,
      "label": "bf05410100_0360",
      "style": 82,
      "name": "여기서 조금만 더 가면 튀어나온 절벽이 있어"
    },
    {
      "$id": 36,
      "label": "bf05410100_0370",
      "style": 82,
      "name": "거기서라면 등에 올라탈 수 있을 거야"
    },
    {
      "$id": 37,
      "label": "bf05410100_0380",
      "style": 82,
      "name": "하지만 타이밍을 못 맞추면\n절벽 밑으로 떨어질 텐데?"
    },
    {
      "$id": 38,
      "label": "bf05410100_0390",
      "style": 82,
      "name": "게다가 공격을 피해야 하니\n보통 어려운 일이 아닙니다"
    },
    {
      "$id": 39,
      "label": "bf05410100_0400",
      "style": 82,
      "name": "제가 하겠습니다"
    },
    {
      "$id": 40,
      "label": "bf05410100_0410",
      "style": 82,
      "name": "판 씨?"
    },
    {
      "$id": 41,
      "label": "bf05410100_0420",
      "style": 82,
      "name": "제 능력으로 아르스의 움직임을 멈출 테니\n여러분은 그 틈에 아르스 등으로 가세요"
    },
    {
      "$id": 42,
      "label": "bf05410100_0430",
      "style": 82,
      "name": "판 씨…"
    },
    {
      "$id": 43,
      "label": "bf05410100_0440",
      "style": 82,
      "name": "알았어, 그래도 너무 무리하진 마"
    },
    {
      "$id": 44,
      "label": "bf05410100_0450",
      "style": 82,
      "name": "네, 알겠습니다"
    },
    {
      "$id": 45,
      "label": "bf05410100_0460",
      "style": 82,
      "name": "좋아, 서두르자!"
    }
  ]
}