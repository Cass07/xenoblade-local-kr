  "rows": [
    {
      "$id": 1,
      "label": "kizunatalk030_msg0001",
      "style": 84,
      "name": "하나"
    },
    {
      "$id": 2,
      "label": "kizunatalk030_msg0002",
      "style": 84,
      "name": "부르셨어요모?"
    },
    {
      "$id": 3,
      "label": "kizunatalk030_msg0003",
      "style": 84,
      "name": "왠지 신기하네요"
    },
    {
      "$id": 4,
      "label": "kizunatalk030_msg0004",
      "style": 84,
      "name": "이렇게나 겉모습이 다른데\n하나는 하나라니"
    },
    {
      "$id": 5,
      "label": "kizunatalk030_msg0005",
      "style": 84,
      "name": "확실히 느낌이 이상해모\n사실 토라도 그렇게 생각할 때가 있어모"
    },
    {
      "$id": 6,
      "label": "kizunatalk030_msg0006",
      "style": 84,
      "name": "토라가 만들었는데도 그래요?"
    },
    {
      "$id": 7,
      "label": "kizunatalk030_msg0007",
      "style": 84,
      "name": "그래모!"
    },
    {
      "$id": 8,
      "label": "kizunatalk030_msg0008",
      "style": 84,
      "name": "너무 빨리 성장했어모!"
    },
    {
      "$id": 9,
      "label": "kizunatalk030_msg0009",
      "style": 84,
      "name": "그럼 모드 명으로 부르면 될 것 같아요모"
    },
    {
      "$id": 10,
      "label": "kizunatalk030_msg0010",
      "style": 84,
      "name": "그거 좋은 생각이다모"
    },
    {
      "$id": 11,
      "label": "kizunatalk030_msg0011",
      "style": 84,
      "name": "모드 명이 뭔가요?"
    },
    {
      "$id": 12,
      "label": "kizunatalk030_msg0012",
      "style": 84,
      "name": "작은 하나는\n제트 스파크 모드가\n정식 명칭이야모"
    },
    {
      "$id": 13,
      "label": "kizunatalk030_msg0013",
      "style": 84,
      "name": "아~ 그랬군요"
    },
    {
      "$id": 14,
      "label": "kizunatalk030_msg0014",
      "style": 84,
      "name": "그리고 지금 모습은\n저스티스 나이트 모드예요모"
    },
    {
      "$id": 15,
      "label": "kizunatalk030_msg0015",
      "style": 84,
      "name": "아~멋진 이름이네요"
    },
    {
      "$id": 16,
      "label": "kizunatalk030_msg0016",
      "style": 84,
      "name": "이 모습일 때는\n저스티스 나이트 하나라고\n불러 줬으면 좋겠어요모"
    },
    {
      "$id": 17,
      "label": "kizunatalk030_msg0017",
      "style": 84,
      "name": "저스티스 나이트 하나…\n부르기 어렵지 않을까요?"
    },
    {
      "$id": 18,
      "label": "kizunatalk030_msg0018",
      "style": 84,
      "name": "저스티스 나이트 하나…"
    },
    {
      "$id": 19,
      "label": "kizunatalk030_msg0019",
      "style": 84,
      "name": "확실히 그렇네모\n생각해 보면 나눠서 부르는 게\n번거로워모"
    },
    {
      "$id": 20,
      "label": "kizunatalk030_msg0020",
      "style": 84,
      "name": "그런가요모?"
    },
    {
      "$id": 21,
      "label": "kizunatalk030_msg0021",
      "style": 84,
      "name": "토라는 비밀리에\n또 다른 모드의 추가를 생각하고 있어모"
    },
    {
      "$id": 22,
      "label": "kizunatalk030_msg0022",
      "style": 84,
      "name": "그게 완성되면\n3종류로 나눠서\n불러야 될지도 몰라모"
    },
    {
      "$id": 23,
      "label": "kizunatalk030_msg0023",
      "style": 84,
      "name": "엄청나게 번거로워모!"
    },
    {
      "$id": 24,
      "label": "kizunatalk030_msg0024",
      "style": 84,
      "name": "하나!"
    },
    {
      "$id": 25,
      "label": "kizunatalk030_msg0025",
      "style": 84,
      "name": "이 한마디로 부르는 게 가장 좋아모"
    },
    {
      "$id": 26,
      "label": "kizunatalk030_msg0026",
      "style": 165,
      "name": "뭐라고 할까요?"
    },
    {
      "$id": 27,
      "label": "kizunatalk030_msg0027",
      "style": 164,
      "name": "저도 찬성이에요"
    },
    {
      "$id": 28,
      "label": "kizunatalk030_msg0028",
      "style": 164,
      "name": "추가되는 모드는 어떤 건가요?"
    },
    {
      "$id": 29,
      "label": "kizunatalk030_msg0101",
      "style": 84,
      "name": "저도 하나라고 부르는 게\n제일 좋은 것 같아요"
    },
    {
      "$id": 30,
      "label": "kizunatalk030_msg0102",
      "style": 84,
      "name": "짧아서 알기 쉬운 데다 좋은 이름이고"
    },
    {
      "$id": 31,
      "label": "kizunatalk030_msg0103",
      "style": 84,
      "name": "게다가 모습이 바뀌어도\n하나는 하나니까요"
    },
    {
      "$id": 32,
      "label": "kizunatalk030_msg0104",
      "style": 84,
      "name": "애초에 두 사람이 꺼낸 이야기였어요모"
    },
    {
      "$id": 33,
      "label": "kizunatalk030_msg0105",
      "style": 84,
      "name": "하나는 하나라고 불러 주는 게\n가장 편해요모"
    },
    {
      "$id": 34,
      "label": "kizunatalk030_msg0106",
      "style": 84,
      "name": "그건 그래모"
    },
    {
      "$id": 35,
      "label": "kizunatalk030_msg0107",
      "style": 84,
      "name": "돌고 돌아 원점이네요"
    },
    {
      "$id": 36,
      "label": "kizunatalk030_msg0108",
      "style": 84,
      "name": "그보다 주인\n새로운 모드가 추가되는 건가요모?"
    },
    {
      "$id": 37,
      "label": "kizunatalk030_msg0201",
      "style": 84,
      "name": "아주 좋은 질문이야모!"
    },
    {
      "$id": 38,
      "label": "kizunatalk030_msg0202",
      "style": 84,
      "name": "그건 바로 저지먼트 데이 모드!!"
    },
    {
      "$id": 39,
      "label": "kizunatalk030_msg0203",
      "style": 84,
      "name": "그것도 멋진 이름이네요"
    },
    {
      "$id": 40,
      "label": "kizunatalk030_msg0204",
      "style": 84,
      "name": "강할 거 같아요모"
    },
    {
      "$id": 41,
      "label": "kizunatalk030_msg0205",
      "style": 84,
      "name": "수페르비아의 온천에\n왼발을 먼저 넣을까 오른발을 먼저 넣을까\n고민하다가 문득 떠오른 이름이야모"
    },
    {
      "$id": 42,
      "label": "kizunatalk030_msg0206",
      "style": 84,
      "name": "유래는 별거 아니었네요모"
    },
    {
      "$id": 43,
      "label": "kizunatalk030_msg0207",
      "style": 84,
      "name": "그래서 하나는\n어떤 식으로 변신하나요?"
    },
    {
      "$id": 44,
      "label": "kizunatalk030_msg0208",
      "style": 84,
      "name": "미정이야모"
    },
    {
      "$id": 45,
      "label": "kizunatalk030_msg0209",
      "style": 84,
      "name": "미정?"
    },
    {
      "$id": 46,
      "label": "kizunatalk030_msg0210",
      "style": 84,
      "name": "이름만 정했으니 지금부터 생각해 봐야지모"
    },
    {
      "$id": 47,
      "label": "kizunatalk030_msg0211",
      "style": 84,
      "name": "아직 먼 이야기인 것 같네요모"
    },
    {
      "$id": 48,
      "label": "kizunatalk030_msg0212",
      "style": 84,
      "name": "그렇지 않아모"
    },
    {
      "$id": 49,
      "label": "kizunatalk030_msg0213",
      "style": 84,
      "name": "평소에 아이디어를 미리 구상해 놓으면\n어느 날 갑자기 모모못~ 하며\n완성형이 떠오르는 거야모"
    },
    {
      "$id": 50,
      "label": "kizunatalk030_msg0214",
      "style": 84,
      "name": "발명가란 그런 거야모"
    },
    {
      "$id": 51,
      "label": "kizunatalk030_msg0215",
      "style": 84,
      "name": "정말 그런 건가요모?"
    },
    {
      "$id": 52,
      "label": "kizunatalk030_msg0216",
      "style": 84,
      "name": "아이디어가 떠오르면 바로 집으로 가서\n파워 업시켜 줄 테니, 믿고 기다려모"
    },
    {
      "$id": 53,
      "label": "kizunatalk030_msg0217",
      "style": 84,
      "name": "알겠어요모\n너무 기대하지 않고 기다릴게요모"
    },
    {
      "$id": 54,
      "label": "kizunatalk030_msg0218",
      "style": 84,
      "name": "전혀 믿지 않고 있잖아모?!"
    },
    {
      "$id": 55,
      "label": "kizunatalk030_msg0219",
      "style": 84,
      "name": "후후훗\n모습이 바뀌어도\n역시 하나는 하나로군요"
    }
  ]
}