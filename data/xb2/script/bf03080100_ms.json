  "rows": [
    {
      "$id": 1,
      "label": "bf03080100_0010",
      "style": 82,
      "name": "이쪽은 좀 밝네"
    },
    {
      "$id": 2,
      "label": "bf03080100_0020",
      "style": 82,
      "name": "인비디아의 아르스는\n등의 표면이 반투명하니"
    },
    {
      "$id": 3,
      "label": "bf03080100_0030",
      "style": 82,
      "name": "바깥의 빛이 여기까지 닿은 것일 게다"
    },
    {
      "$id": 4,
      "label": "bf03080100_0040",
      "style": 82,
      "name": "그럼 아르스는 지금 운해 위에 있는 건가?"
    },
    {
      "$id": 5,
      "label": "bf03080100_0050",
      "style": 82,
      "name": "그럴 게다"
    },
    {
      "$id": 6,
      "label": "bf03080100_0060",
      "style": 82,
      "name": "빛이 있는 쪽으로 가다 보면\n여기서 나갈 수 있을지도 모르겠네요"
    },
    {
      "$id": 7,
      "label": "bf03080100_0070",
      "style": 82,
      "name": "잠깐"
    },
    {
      "$id": 8,
      "label": "bf03080100_0080",
      "style": 82,
      "name": "이 근방에서는 못 보던 얼굴인데?"
    },
    {
      "$id": 9,
      "label": "bf03080100_0090",
      "style": 82,
      "name": "그렇다는 건 표류하던 도중에\n아르스에게 먹히기라도 한 건가?"
    },
    {
      "$id": 10,
      "label": "bf03080100_0100",
      "style": 82,
      "name": "음? 그 블레이드…"
    },
    {
      "$id": 11,
      "label": "bf03080100_0110",
      "style": 82,
      "name": "에메랄드색 코어 크리스털"
    },
    {
      "$id": 12,
      "label": "bf03080100_0120",
      "style": 82,
      "name": "[ML:undisp ]"
    },
    {
      "$id": 13,
      "label": "bf03080100_0130",
      "style": 82,
      "name": "하하하하, 소문이 사실이었군"
    },
    {
      "$id": 14,
      "label": "bf03080100_0140",
      "style": 82,
      "name": "소문이라니 무슨 소리야?"
    },
    {
      "$id": 15,
      "label": "bf03080100_0150",
      "style": 82,
      "name": "드라이버라면 누구나 한 번쯤은 듣게 되는\n전설의 블레이드, 하늘의 성배"
    },
    {
      "$id": 16,
      "label": "bf03080100_0160",
      "style": 82,
      "name": "그게 500년 만에 눈을 떴다는 소문이지"
    },
    {
      "$id": 17,
      "label": "bf03080100_0170",
      "style": 82,
      "name": "그건 그렇고 설마 너 같은 꼬맹이가\n드라이버일 줄이야"
    },
    {
      "$id": 18,
      "label": "bf03080100_0180",
      "style": 82,
      "name": "내가 드라이버면 안 되는 거냐?"
    },
    {
      "$id": 19,
      "label": "bf03080100_0190",
      "style": 82,
      "name": "안 될 건 없지\n그게 평범한 블레이드라면 말이다"
    },
    {
      "$id": 20,
      "label": "bf03080100_0200",
      "style": 82,
      "name": "하지만 그건 평범하지 않아\n너한테는 과분한 물건이지"
    },
    {
      "$id": 21,
      "label": "bf03080100_0210",
      "style": 82,
      "name": "꼬맹아, 하늘의 성배와 그 검을 넘겨라"
    },
    {
      "$id": 22,
      "label": "bf03080100_0220",
      "style": 82,
      "name": "설마 너도 호무라를?"
    },
    {
      "$id": 23,
      "label": "bf03080100_0230",
      "style": 82,
      "name": "누가 넘겨줄 것 같아?"
    },
    {
      "$id": 24,
      "label": "bf03080100_0240",
      "style": 82,
      "name": "기세만은 좋군, 꼬맹이"
    },
    {
      "$id": 25,
      "label": "bf03080100_0250",
      "style": 82,
      "name": "유우, 즈오! 너희는 다른 둘을 해치워라\n이 꼬맹이는 내가 맡지"
    },
    {
      "$id": 26,
      "label": "bf03080100_0260",
      "style": 82,
      "name": "넵! 맡겨 주십쇼, 단장님"
    },
    {
      "$id": 27,
      "label": "bf03080100_0270",
      "style": 82,
      "name": "우리가 잘해 줄게, 아가씨"
    }
  ]
}