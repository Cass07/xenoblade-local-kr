  "rows": [
    {
      "$id": 1,
      "label": "qst201102_msg0001",
      "style": 84,
      "name": "메모가 묶여 있는데"
    },
    {
      "$id": 2,
      "label": "qst201102_msg0002",
      "style": 84,
      "name": "좀 읽어 봐라, 니아"
    },
    {
      "$id": 3,
      "label": "qst201102_msg0003",
      "style": 84,
      "name": "…어디 보자"
    },
    {
      "$id": 4,
      "label": "qst201102_msg0004",
      "style": 84,
      "name": "인비디아의 채석장 터 근처에서\n금강 결정을 찾아내서…"
    },
    {
      "$id": 5,
      "label": "qst201102_msg0005",
      "style": 84,
      "name": "굴라의 갈레그로의 언덕으로 가지고 와라"
    },
    {
      "$id": 6,
      "label": "qst201102_msg0006",
      "style": 84,
      "name": "지시에 따르면\n기억을 되찾을 수 있을 것이다"
    },
    {
      "$id": 7,
      "label": "qst201102_msg0007",
      "style": 84,
      "name": "…이게 뭐야?"
    },
    {
      "$id": 8,
      "label": "qst201102_msg0008",
      "style": 84,
      "name": "누구의 기억 말이고…?"
    },
    {
      "$id": 9,
      "label": "qst201102_msg0009",
      "style": 84,
      "name": "그냥 장난일지도 몰라"
    },
    {
      "$id": 10,
      "label": "qst201102_msg0010",
      "style": 84,
      "name": "장난이라면 차라리 좋겠다만\n함정일 가능성도 있어…"
    },
    {
      "$id": 11,
      "label": "qst201102_msg0011",
      "style": 84,
      "name": "그렇긴 해…\n무시하는 게 좋겠어"
    },
    {
      "$id": 12,
      "label": "qst201102_msg0012",
      "style": 84,
      "name": "…………"
    },
    {
      "$id": 13,
      "label": "qst201102_msg0013",
      "style": 84,
      "name": "…잠깐만"
    },
    {
      "$id": 14,
      "label": "qst201102_msg0014",
      "style": 84,
      "name": "미쿠마리, 신경 쓰이나?"
    },
    {
      "$id": 15,
      "label": "qst201102_msg0015",
      "style": 84,
      "name": "응… 만약 우리에게\n위해를 가하려는 거라면\n지금 여기서 습격해도 되잖아?"
    },
    {
      "$id": 16,
      "label": "qst201102_msg0016",
      "style": 84,
      "name": "이 창도 일부러 빗나가게 하지 않고\n누군가를 맞힐 수도 있었을 거고…"
    },
    {
      "$id": 17,
      "label": "qst201102_msg0017",
      "style": 84,
      "name": "…그렇군"
    },
    {
      "$id": 18,
      "label": "qst201102_msg0018",
      "style": 84,
      "name": "이렇게 복잡한 일을 한 데는\n뭔가 의미가 있을 것 같아"
    },
    {
      "$id": 19,
      "label": "qst201102_msg0019",
      "style": 84,
      "name": "나는 범인의 목적과 정체가 궁금해…"
    },
    {
      "$id": 20,
      "label": "qst201102_msg0020",
      "style": 84,
      "name": "이 메모의 지시에 따라 보고 싶은데\n괜찮을까?"
    },
    {
      "$id": 21,
      "label": "qst201102_msg0021",
      "style": 84,
      "name": "함정의 위험성도 생각해 봤다만\n모험을 함으로써 얻을 수 있는 것이\n있을지도 모르겠군"
    },
    {
      "$id": 22,
      "label": "qst201102_msg0022",
      "style": 84,
      "name": "미쿠마리가 그렇다면\n같이 가자"
    },
    {
      "$id": 23,
      "label": "qst201102_msg0023",
      "style": 84,
      "name": "다들 괜찮지?"
    },
    {
      "$id": 24,
      "label": "qst201102_msg0024",
      "style": 84,
      "name": "좋아\n이 수수께끼를 같이 해결해 보자"
    },
    {
      "$id": 25,
      "label": "qst201102_msg0025",
      "style": 84,
      "name": "고마워!"
    }
  ]
}