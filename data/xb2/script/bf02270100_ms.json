  "rows": [
    {
      "$id": 1,
      "label": "bf02270100_0010",
      "style": 82,
      "name": "이건…"
    },
    {
      "$id": 2,
      "label": "bf02270100_0020",
      "style": 82,
      "name": "누구에게도 보여 준 적 없는 토라의 비밀…"
    },
    {
      "$id": 3,
      "label": "bf02270100_0030",
      "style": 82,
      "name": "인공 블레이드야모!"
    },
    {
      "$id": 4,
      "label": "bf02270100_0040",
      "style": 82,
      "name": "인공 블레이드?"
    },
    {
      "$id": 5,
      "label": "bf02270100_0050",
      "style": 82,
      "name": "이게…"
    },
    {
      "$id": 6,
      "label": "bf02270100_0060",
      "style": 82,
      "name": "토라는 드라이버가 되고 싶었어모"
    },
    {
      "$id": 7,
      "label": "bf02270100_0070",
      "style": 82,
      "name": "하지만 토라는…\n토라에게는 드라이버 적성이 없었어모…"
    },
    {
      "$id": 8,
      "label": "bf02270100_0080",
      "style": 82,
      "name": "왜 그렇게 단정 짓는 거야?\n적어도 시도는 해 봐야…"
    },
    {
      "$id": 9,
      "label": "bf02270100_0085",
      "style": 82,
      "name": "[ML:undisp ]"
    },
    {
      "$id": 10,
      "label": "bf02270100_0090",
      "style": 82,
      "name": "…해 봤어?"
    },
    {
      "$id": 11,
      "label": "bf02270100_0100",
      "style": 82,
      "name": "토라는 1년 전\n드라이버 스카우트에 지원했었어모…"
    },
    {
      "$id": 12,
      "label": "bf02270100_0110",
      "style": 82,
      "name": "안 됐나 보구먼"
    },
    {
      "$id": 13,
      "label": "bf02270100_0120",
      "style": 82,
      "name": "토라도 그 사람처럼…"
    },
    {
      "$id": 14,
      "label": "bf02270100_0130",
      "style": 82,
      "name": "맞아모… 3일 내내 코피가 멈추지 않아서\n죽는 줄 알았어모"
    },
    {
      "$id": 15,
      "label": "bf02270100_0140",
      "style": 82,
      "name": "겨, 겨우 그거…?"
    },
    {
      "$id": 16,
      "label": "bf02270100_0150",
      "style": 82,
      "name": "겨우 그거가 아니야모!\n코피라도 출혈량이 많으면 죽기도 한다고모!"
    },
    {
      "$id": 17,
      "label": "bf02270100_0160",
      "style": 82,
      "name": "들어 본 적이 없는데"
    },
    {
      "$id": 18,
      "label": "bf02270100_0170",
      "style": 82,
      "name": "뭐, 그건 그렇다 치고모"
    },
    {
      "$id": 19,
      "label": "bf02270100_0180",
      "style": 82,
      "name": "그렇다고 친다니…\n너무 대충 넘어가는 거 아냐?"
    },
    {
      "$id": 20,
      "label": "bf02270100_0190",
      "style": 82,
      "name": "아무튼 이 인공 블레이드가 완성되면"
    },
    {
      "$id": 21,
      "label": "bf02270100_0200",
      "style": 82,
      "name": "적성이 없는 토라라도\n드라이버가 될 수 있어모"
    },
    {
      "$id": 22,
      "label": "bf02270100_0210",
      "style": 82,
      "name": "헌데 참으로 대단하구먼\n토라, 전부 너 혼자 만든 게냐?"
    },
    {
      "$id": 23,
      "label": "bf02270100_0220",
      "style": 82,
      "name": "만들기 시작한 건\n할아버지와 아빠야모"
    },
    {
      "$id": 24,
      "label": "bf02270100_0230",
      "style": 82,
      "name": "하지만 할아버지는 돌아가셨고\n아빠도 어디론가 가 버렸어모…"
    },
    {
      "$id": 25,
      "label": "bf02270100_0240",
      "style": 82,
      "name": "토라…"
    },
    {
      "$id": 26,
      "label": "bf02270100_0250",
      "style": 82,
      "name": "그래서 토라는 이 녀석을 완성시켜서\n드라이버가 된 다음 대활약할 거야모"
    },
    {
      "$id": 27,
      "label": "bf02270100_0260",
      "style": 82,
      "name": "그럼 소문을 들은 아빠도\n집으로 돌아와 주겠지모"
    },
    {
      "$id": 28,
      "label": "bf02270100_0270",
      "style": 82,
      "name": "토라…"
    },
    {
      "$id": 29,
      "label": "bf02270100_0280",
      "style": 82,
      "name": "그런데 토라, 이 인공 블레이드…"
    },
    {
      "$id": 30,
      "label": "bf02270100_0290",
      "style": 82,
      "name": "보아하니 거의 완성된 것 같다마는\n이제 어떻게 하면 되는 게냐?"
    },
    {
      "$id": 31,
      "label": "bf02270100_0300",
      "style": 82,
      "name": "모후후~ 이제 부족한 파츠를\n몇 개 사 오면 돼모"
    },
    {
      "$id": 32,
      "label": "bf02270100_0310",
      "style": 82,
      "name": "하지만 토라 돈이 하나도 없어모"
    },
    {
      "$id": 33,
      "label": "bf02270100_0320",
      "style": 82,
      "name": "뭐~? 진짜?"
    },
    {
      "$id": 34,
      "label": "bf02270100_0330",
      "style": 82,
      "name": "진짜야모"
    },
    {
      "$id": 35,
      "label": "bf02270100_0340",
      "style": 82,
      "name": "하나도? 1골드도?"
    },
    {
      "$id": 36,
      "label": "bf02270100_0350",
      "style": 82,
      "name": "빈털터리야모"
    },
    {
      "$id": 37,
      "label": "bf02270100_0360",
      "style": 82,
      "name": "그러니까 돈을 빌려 달라고?"
    },
    {
      "$id": 38,
      "label": "bf02270100_0370",
      "style": 82,
      "name": "빌려주는 게 아니라\n그냥 주면 더 기쁠 것 같다모"
    },
    {
      "$id": 39,
      "label": "bf02270100_0380",
      "style": 82,
      "name": "역시 노폰족이군\n야무지다고 해야 할지, 약삭빠르다고 해야 할지…"
    },
    {
      "$id": 40,
      "label": "bf02270100_0390",
      "style": 82,
      "name": "그래도 인공 블레이드가 완성되면\n어마어마한 전력이 될 거야모!"
    },
    {
      "$id": 41,
      "label": "bf02270100_0400",
      "style": 82,
      "name": "뭐, 그야 그렇겠지만"
    },
    {
      "$id": 42,
      "label": "bf02270100_0410",
      "style": 82,
      "name": "그래서 얼마나 필요한데?"
    },
    {
      "$id": 43,
      "label": "bf02270100_0420",
      "style": 82,
      "name": "대충 6만 골드 정도야모"
    },
    {
      "$id": 44,
      "label": "bf02270100_0430",
      "style": 82,
      "name": "6만?!\n아무리 그래도 그건…"
    },
    {
      "$id": 45,
      "label": "bf02270100_0440",
      "style": 82,
      "name": "저기 렉스\n토라의 힘을 빌리는 게 어때요?"
    },
    {
      "$id": 46,
      "label": "bf02270100_0450",
      "style": 82,
      "name": "돈이라면 제가 구해 볼 테니까요"
    },
    {
      "$id": 47,
      "label": "bf02270100_0460",
      "style": 82,
      "name": "어떻게 마련할 건데?"
    },
    {
      "$id": 48,
      "label": "bf02270100_0470",
      "style": 82,
      "name": "설, 설마 불법적인…"
    },
    {
      "$id": 49,
      "label": "bf02270100_0480",
      "style": 82,
      "name": "지금이 농담할 때야, 할아버지?"
    },
    {
      "$id": 50,
      "label": "bf02270100_0490",
      "style": 82,
      "name": "천연 보석이니까\n팔면 6만 골드 정도는 될 거예요"
    },
    {
      "$id": 51,
      "label": "bf02270100_0500",
      "style": 82,
      "name": "안 돼! 그건 받을 수 없어"
    },
    {
      "$id": 52,
      "label": "bf02270100_0510",
      "style": 82,
      "name": "하지만…"
    },
    {
      "$id": 53,
      "label": "bf02270100_0520",
      "style": 82,
      "name": "에잇, 나도 남자라고!\n알았어, 파츠값은 전부 내가 낼게"
    },
    {
      "$id": 54,
      "label": "bf02270100_0530",
      "style": 82,
      "name": "통이 크구먼!"
    },
    {
      "$id": 55,
      "label": "bf02270100_0540",
      "style": 82,
      "name": "모모~!\n형님은 사나이 중의 사나이다모~!"
    },
    {
      "$id": 56,
      "label": "bf02270100_0550",
      "style": 82,
      "name": "그 대신 어설픈 블레이드면\n가만두지 않을 거야"
    },
    {
      "$id": 57,
      "label": "bf02270100_0560",
      "style": 82,
      "name": "그건 맡겨 줘모"
    },
    {
      "$id": 58,
      "label": "bf02270100_0570",
      "style": 82,
      "name": "좋아! 그럼 가자, 토라!"
    },
    {
      "$id": 59,
      "label": "bf02270100_0580",
      "style": 82,
      "name": "모~!"
    },
    {
      "$id": 60,
      "label": "bf02270100_0590",
      "style": 82,
      "name": "괜찮을까요…?"
    },
    {
      "$id": 61,
      "label": "bf02270100_0600",
      "style": 82,
      "name": "보아하니 자포자기한 것 같구먼"
    }
  ]
