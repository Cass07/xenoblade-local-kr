  "rows": [
    {
      "$id": 1,
      "label": "tlk502103_msg0001",
      "style": 84,
      "name": "저기, 최근에 이 마을에 왔지?"
    },
    {
      "$id": 2,
      "label": "tlk502103_msg0002",
      "style": 84,
      "name": "맞아…\n그게 왜?"
    },
    {
      "$id": 3,
      "label": "tlk502103_msg0003",
      "style": 84,
      "name": "용병으로 일한 지 오래됐어?"
    },
    {
      "$id": 4,
      "label": "tlk502103_msg0004",
      "style": 84,
      "name": "뭐 그렇지…"
    },
    {
      "$id": 5,
      "label": "tlk502103_msg0005",
      "style": 84,
      "name": "저기, 여기 오기 전에 겪은\n재미있는 경험담을 들려줘!"
    },
    {
      "$id": 6,
      "label": "tlk502103_msg0011",
      "style": 84,
      "name": "글쎄, 무슨 이야기를 하지…?"
    },
    {
      "$id": 7,
      "label": "tlk502103_msg0012",
      "style": 84,
      "name": "그럼… 가장 돈을 많이 벌었던\n임무에 대해 얘기해 줘"
    },
    {
      "$id": 8,
      "label": "tlk502103_msg0013",
      "style": 84,
      "name": "그래…\n그건 금방 얘기할 수 있지"
    },
    {
      "$id": 9,
      "label": "tlk502103_msg0014",
      "style": 84,
      "name": "폰스 마임 근처에 채굴장이 있잖아?"
    },
    {
      "$id": 10,
      "label": "tlk502103_msg0015",
      "style": 84,
      "name": "응, 있어"
    },
    {
      "$id": 11,
      "label": "tlk502103_msg0016",
      "style": 84,
      "name": "거기서 예전에 귀한 광석이 채굴됐었어"
    },
    {
      "$id": 12,
      "label": "tlk502103_msg0017",
      "style": 84,
      "name": "호오~"
    },
    {
      "$id": 13,
      "label": "tlk502103_msg0018",
      "style": 84,
      "name": "언제부턴가 몬스터가 정착하게 돼서\n난처해진 광부들은\n우리에게 토벌을 의뢰했지"
    },
    {
      "$id": 14,
      "label": "tlk502103_msg0019",
      "style": 84,
      "name": "광석으로 돈을 벌던 녀석들이 의뢰한 거라\n보수도 제법 짭짤했어"
    },
    {
      "$id": 15,
      "label": "tlk502103_msg0020",
      "style": 84,
      "name": "하기야 많이 버니까 씀씀이도 좋았겠네"
    },
    {
      "$id": 16,
      "label": "tlk502103_msg0021",
      "style": 84,
      "name": "그래서 한 번 소탕했는데\n또다시 몬스터가 정착하더라고"
    },
    {
      "$id": 17,
      "label": "tlk502103_msg0022",
      "style": 84,
      "name": "몬스터라는 게 끈질겨서\n아무리 해치워도 다시 돌아오거든"
    },
    {
      "$id": 18,
      "label": "tlk502103_msg0023",
      "style": 84,
      "name": "우리는 그때마다\n큰돈을 벌었어"
    },
    {
      "$id": 19,
      "label": "tlk502103_msg0024",
      "style": 84,
      "name": "좋았겠다~"
    },
    {
      "$id": 20,
      "label": "tlk502103_msg0025",
      "style": 84,
      "name": "그게 20번이나 반복돼서\n몬스터한테 참 고마웠는데…"
    },
    {
      "$id": 21,
      "label": "tlk502103_msg0026",
      "style": 84,
      "name": "21번째가 되니\n그 채석장을 폐기하게 됐어"
    },
    {
      "$id": 22,
      "label": "tlk502103_msg0027",
      "style": 84,
      "name": "그렇구나~\n그런 역사가 있었구나"
    },
    {
      "$id": 23,
      "label": "tlk502103_msg0028",
      "style": 84,
      "name": "오랜만에 나도 모르게 수다를 떨었군…"
    },
    {
      "$id": 24,
      "label": "tlk502103_msg0029",
      "style": 84,
      "name": "재미있었어!"
    },
    {
      "$id": 25,
      "label": "tlk502103_msg0030",
      "style": 84,
      "name": "저기, 그때 그 광석은 안 갖고 있어?\n몰래 가져온 거라든가"
    },
    {
      "$id": 26,
      "label": "tlk502103_msg0031",
      "style": 84,
      "name": "아, 갖고 있지\n줄까?"
    },
    {
      "$id": 27,
      "label": "tlk502103_msg0032",
      "style": 84,
      "name": "[ML:Feeling kind=Think ]훗…\n한 건 했네!"
    },
    {
      "$id": 28,
      "label": "tlk502103_msg0041",
      "style": 84,
      "name": "거절하지\n말할 기분이 아니야"
    },
    {
      "$id": 29,
      "label": "tlk502103_msg0042",
      "style": 84,
      "name": "[ML:Feeling kind=Think ]어?\n이상하네…"
    },
    {
      "$id": 30,
      "label": "tlk502103_msg0043",
      "style": 84,
      "name": "[ML:Feeling kind=Think ]내 매력이 안 통한 건가?"
    }
  ]
}