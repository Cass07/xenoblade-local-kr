  "rows": [
    {
      "$id": 1,
      "label": "kizunatalk079_msg0001",
      "style": 84,
      "name": "봉인된 블레이드를 깨웠구나…"
    },
    {
      "$id": 2,
      "label": "kizunatalk079_msg0002",
      "style": 84,
      "name": "폐하"
    },
    {
      "$id": 3,
      "label": "kizunatalk079_msg0003",
      "style": 84,
      "name": "폐하"
    },
    {
      "$id": 4,
      "label": "kizunatalk079_msg0004",
      "style": 84,
      "name": "제리히 폐하"
    },
    {
      "$id": 5,
      "label": "kizunatalk079_msg0005",
      "style": 84,
      "name": "아버지"
    },
    {
      "$id": 6,
      "label": "kizunatalk079_msg0006",
      "style": 84,
      "name": "여기는 꽤 오랜만에 오는구나"
    },
    {
      "$id": 7,
      "label": "kizunatalk079_msg0007",
      "style": 84,
      "name": "어릴 적에는 자주 놀러 오곤 했지만…"
    },
    {
      "$id": 8,
      "label": "kizunatalk079_msg0008",
      "style": 84,
      "name": "……"
    },
    {
      "$id": 9,
      "label": "kizunatalk079_msg0009",
      "style": 84,
      "name": "이 테오스피스티 신전이\n왜 파괴되었는지 알고 있나?"
    },
    {
      "$id": 10,
      "label": "kizunatalk079_msg0010",
      "style": 84,
      "name": "글쎄요, 잘 모르겠습니다"
    },
    {
      "$id": 11,
      "label": "kizunatalk079_msg0011",
      "style": 84,
      "name": "글쎄, 모르겠는데"
    },
    {
      "$id": 12,
      "label": "kizunatalk079_msg0012",
      "style": 84,
      "name": "잘 모르겠습니다"
    },
    {
      "$id": 13,
      "label": "kizunatalk079_msg0013",
      "style": 84,
      "name": "오래전에 무너진 건 아는데\n원인까지는 모르겠다"
    },
    {
      "$id": 14,
      "label": "kizunatalk079_msg0014",
      "style": 84,
      "name": "블레이드의 폭주다"
    },
    {
      "$id": 15,
      "label": "kizunatalk079_msg0015",
      "style": 84,
      "name": "너무나도 힘이 막강했기에\n드라이버도 제어하지 못했다고 한다"
    },
    {
      "$id": 16,
      "label": "kizunatalk079_msg0016",
      "style": 84,
      "name": "폭주한 블레이드는 아무도 제압하지 못했고\n결국 드라이버가 자결을 해\n겨우 수습되었다고 한다"
    },
    {
      "$id": 17,
      "label": "kizunatalk079_msg0017",
      "style": 84,
      "name": "그리고 코어 크리스털은\n봉인하게 되었지"
    },
    {
      "$id": 18,
      "label": "kizunatalk079_msg0018",
      "style": 84,
      "name": "경계하자는 차원에서\n재앙을 일으킨 이 신전의 터에 말이다"
    },
    {
      "$id": 19,
      "label": "kizunatalk079_msg0019",
      "style": 84,
      "name": "폭주했다는 블레이드가\n설마…"
    },
    {
      "$id": 20,
      "label": "kizunatalk079_msg0020",
      "style": 84,
      "name": "폭주했다는 블레이드가\n설마…"
    },
    {
      "$id": 21,
      "label": "kizunatalk079_msg0021",
      "style": 84,
      "name": "폭주한 블레이드라면\n설마…"
    },
    {
      "$id": 22,
      "label": "kizunatalk079_msg0022",
      "style": 84,
      "name": "폭주한 블레이드라니\n설마…"
    },
    {
      "$id": 23,
      "label": "kizunatalk079_msg0023",
      "style": 84,
      "name": "그래\n거기 있는 텐이다"
    },
    {
      "$id": 24,
      "label": "kizunatalk079_msg0024",
      "style": 84,
      "name": "내게는 기억이 없다…\n하지만 납득이 가는 이야기군"
    },
    {
      "$id": 25,
      "label": "kizunatalk079_msg0025",
      "style": 84,
      "name": "가끔 느껴지곤 한다…"
    },
    {
      "$id": 26,
      "label": "kizunatalk079_msg0026",
      "style": 84,
      "name": "내 끝없는 힘이 말이지…"
    },
    {
      "$id": 27,
      "label": "kizunatalk079_msg0027",
      "style": 84,
      "name": "왕이여…\n나는 벌을 달게 받겠다"
    },
    {
      "$id": 28,
      "label": "kizunatalk079_msg0028",
      "style": 84,
      "name": "코어 크리스털을 부숴\n영원히 매장해 버려라"
    },
    {
      "$id": 29,
      "label": "kizunatalk079_msg0029",
      "style": 165,
      "name": "텐이에게 뭐라고 할까?"
    },
    {
      "$id": 30,
      "label": "kizunatalk079_msg0030",
      "style": 164,
      "name": "그 판단을 존중할게"
    },
    {
      "$id": 31,
      "label": "kizunatalk079_msg0031",
      "style": 164,
      "name": "나라면 폭주를 막을 수 있어"
    },
    {
      "$id": 32,
      "label": "kizunatalk079_msg0032",
      "style": 165,
      "name": "텐이에게 뭐라고 할까?"
    },
    {
      "$id": 33,
      "label": "kizunatalk079_msg0033",
      "style": 164,
      "name": "그 판단을 존중할게"
    },
    {
      "$id": 34,
      "label": "kizunatalk079_msg0034",
      "style": 164,
      "name": "난 절대로 폭주하게 두지 않을 거야"
    },
    {
      "$id": 35,
      "label": "kizunatalk079_msg0035",
      "style": 165,
      "name": "텐이에게 뭐라고 하지?"
    },
    {
      "$id": 36,
      "label": "kizunatalk079_msg0036",
      "style": 164,
      "name": "너의 판단을 존중하지"
    },
    {
      "$id": 37,
      "label": "kizunatalk079_msg0037",
      "style": 164,
      "name": "내가 폭주를 막아 내겠다"
    },
    {
      "$id": 38,
      "label": "kizunatalk079_msg0038",
      "style": 165,
      "name": "텐이에게 뭐라 하지?"
    },
    {
      "$id": 39,
      "label": "kizunatalk079_msg0039",
      "style": 164,
      "name": "텐이의 판단을 존중할 기다"
    },
    {
      "$id": 40,
      "label": "kizunatalk079_msg0040",
      "style": 164,
      "name": "내라면 폭주하도록 안 놔둘 기다"
    },
    {
      "$id": 41,
      "label": "kizunatalk079_msg0101",
      "style": 84,
      "name": "고맙다…"
    },
    {
      "$id": 42,
      "label": "kizunatalk079_msg0102",
      "style": 84,
      "name": "그럼 왕이여…"
    },
    {
      "$id": 43,
      "label": "kizunatalk079_msg0103",
      "style": 84,
      "name": "잠깐, 그 판단은\n너무 성급하지 않나?"
    },
    {
      "$id": 44,
      "label": "kizunatalk079_msg0104",
      "style": 84,
      "name": "전승대로라면 텐이의 힘은\n앞으로 전투에서 큰 도움이 될 것이다"
    },
    {
      "$id": 45,
      "label": "kizunatalk079_msg0105",
      "style": 84,
      "name": "텐이여\n그대의 드라이버에게는\n소질과 힘이 있다"
    },
    {
      "$id": 46,
      "label": "kizunatalk079_msg0106",
      "style": 84,
      "name": "무엇보다 강한 의지를 지녔으며…"
    },
    {
      "$id": 47,
      "label": "kizunatalk079_msg0107",
      "style": 84,
      "name": "게다가 하늘의 성배도\n함께 있지 않나?"
    },
    {
      "$id": 48,
      "label": "kizunatalk079_msg0108",
      "style": 84,
      "name": "수백 년 전 드라이버와는 명백히 다르다\n그대는 폭주를 걱정하지 않아도 돼"
    },
    {
      "$id": 49,
      "label": "kizunatalk079_msg0109",
      "style": 84,
      "name": "내가 여기 온 건\n그대를 봉인하기 위해서가 아니라"
    },
    {
      "$id": 50,
      "label": "kizunatalk079_msg0110",
      "style": 84,
      "name": "그 힘을 마음껏 발휘해\n드라이버를 보좌하라고 말하러 온 것이다"
    },
    {
      "$id": 51,
      "label": "kizunatalk079_msg0111",
      "style": 84,
      "name": "……"
    },
    {
      "$id": 52,
      "label": "kizunatalk079_msg0112",
      "style": 84,
      "name": "텐이…"
    },
    {
      "$id": 53,
      "label": "kizunatalk079_msg0113",
      "style": 84,
      "name": "텐이…"
    },
    {
      "$id": 54,
      "label": "kizunatalk079_msg0114",
      "style": 84,
      "name": "텐이…"
    },
    {
      "$id": 55,
      "label": "kizunatalk079_msg0115",
      "style": 84,
      "name": "텐이…"
    },
    {
      "$id": 56,
      "label": "kizunatalk079_msg0116",
      "style": 84,
      "name": "따르겠다"
    },
    {
      "$id": 57,
      "label": "kizunatalk079_msg0117",
      "style": 84,
      "name": "그럼으로써 나의 죄를\n속죄할 수 있다면…"
    },
    {
      "$id": 58,
      "label": "kizunatalk079_msg0118",
      "style": 84,
      "name": "흠… 제대로 이해하려면\n시간이 필요하겠지만"
    },
    {
      "$id": 59,
      "label": "kizunatalk079_msg0119",
      "style": 84,
      "name": "그래도 부탁하지"
    },
    {
      "$id": 60,
      "label": "kizunatalk079_msg0120",
      "style": 84,
      "name": "응, 텐이는 나한테 맡겨 줘"
    },
    {
      "$id": 61,
      "label": "kizunatalk079_msg0121",
      "style": 84,
      "name": "응, 텐이는 내게 맡겨"
    },
    {
      "$id": 62,
      "label": "kizunatalk079_msg0122",
      "style": 84,
      "name": "알겠습니다\n텐이는 제게 맡겨 주십시오"
    },
    {
      "$id": 63,
      "label": "kizunatalk079_msg0123",
      "style": 84,
      "name": "그래\n텐이는 내한테 맡기라"
    },
    {
      "$id": 64,
      "label": "kizunatalk079_msg0201",
      "style": 84,
      "name": "렉스…"
    },
    {
      "$id": 65,
      "label": "kizunatalk079_msg0202",
      "style": 84,
      "name": "니아…"
    },
    {
      "$id": 66,
      "label": "kizunatalk079_msg0203",
      "style": 84,
      "name": "멜레프…"
    },
    {
      "$id": 67,
      "label": "kizunatalk079_msg0204",
      "style": 84,
      "name": "지크…"
    },
    {
      "$id": 68,
      "label": "kizunatalk079_msg0205",
      "style": 84,
      "name": "하하하\n내 생각이 맞았군"
    },
    {
      "$id": 69,
      "label": "kizunatalk079_msg0206",
      "style": 84,
      "name": "텐이여"
    },
    {
      "$id": 70,
      "label": "kizunatalk079_msg0207",
      "style": 84,
      "name": "그대의 드라이버는\n그대를 믿고 있나 보구나"
    },
    {
      "$id": 71,
      "label": "kizunatalk079_msg0208",
      "style": 84,
      "name": "그리고 나는\n드라이버를 믿고 있지"
    },
    {
      "$id": 72,
      "label": "kizunatalk079_msg0209",
      "style": 84,
      "name": "비록 텐이가 과거를 안다고 해도\n괘념할 것 없다"
    },
    {
      "$id": 73,
      "label": "kizunatalk079_msg0210",
      "style": 84,
      "name": "과거에 얽매이지 말고\n드라이버의 힘이 되어 주거라"
    },
    {
      "$id": 74,
      "label": "kizunatalk079_msg0211",
      "style": 84,
      "name": "나는 이 말을 전하러 왔다"
    },
    {
      "$id": 75,
      "label": "kizunatalk079_msg0212",
      "style": 84,
      "name": "하지만 나는 죗값을 치러야 해"
    },
    {
      "$id": 76,
      "label": "kizunatalk079_msg0213",
      "style": 84,
      "name": "그리 생각한다면 어쩔 수 없는 일이지만\n속죄하는 방법은 여러 가지가 있다"
    },
    {
      "$id": 77,
      "label": "kizunatalk079_msg0214",
      "style": 84,
      "name": "앞으로 더욱 격렬해질 전투에서\n드라이버의 힘이 되어 주는 것도 방법이지"
    },
    {
      "$id": 78,
      "label": "kizunatalk079_msg0215",
      "style": 84,
      "name": "그럴까…"
    },
    {
      "$id": 79,
      "label": "kizunatalk079_msg0216",
      "style": 84,
      "name": "응, 텐이의 힘이 필요해"
    },
    {
      "$id": 80,
      "label": "kizunatalk079_msg0217",
      "style": 84,
      "name": "그러니까 과거의 일에 얽매이지 말고\n함께 싸워 줬으면 좋겠어"
    },
    {
      "$id": 81,
      "label": "kizunatalk079_msg0218",
      "style": 84,
      "name": "맞아, 텐이의 힘이 필요해"
    },
    {
      "$id": 82,
      "label": "kizunatalk079_msg0219",
      "style": 84,
      "name": "옛날 일에 얽매이지 말고\n같이 싸워 줘!"
    },
    {
      "$id": 83,
      "label": "kizunatalk079_msg0220",
      "style": 84,
      "name": "그래, 텐이의 힘이 필요하다"
    },
    {
      "$id": 84,
      "label": "kizunatalk079_msg0221",
      "style": 84,
      "name": "그러니 과거에 얽매이지 말고\n우리와 함께 싸워 줬으면 좋겠군"
    },
    {
      "$id": 85,
      "label": "kizunatalk079_msg0222",
      "style": 84,
      "name": "그럼, 텐이 힘이 필요하다"
    },
    {
      "$id": 86,
      "label": "kizunatalk079_msg0223",
      "style": 84,
      "name": "옛날 일에 얽매이지 말고\n우리 같이 싸우자"
    },
    {
      "$id": 87,
      "label": "kizunatalk079_msg0224",
      "style": 84,
      "name": "알겠다…"
    },
    {
      "$id": 88,
      "label": "kizunatalk079_msg0225",
      "style": 84,
      "name": "노력하지…"
    },
    {
      "$id": 89,
      "label": "kizunatalk079_msg0226",
      "style": 84,
      "name": "걱정 말거라, 네가 어떻게 되더라도\n훌륭한 드라이버가 함께하고 있지 않느냐"
    },
    {
      "$id": 90,
      "label": "kizunatalk079_msg0227",
      "style": 84,
      "name": "왕이여… 고맙다"
    },
    {
      "$id": 91,
      "label": "kizunatalk079_msg0228",
      "style": 84,
      "name": "그래\n기대하마"
    }
  ]
}