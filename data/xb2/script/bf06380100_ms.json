  "rows": [
    {
      "$id": 1,
      "label": "bf06380100_0010",
      "style": 82,
      "name": "살아갈 자격…"
    },
    {
      "$id": 2,
      "label": "bf06380100_0020",
      "style": 82,
      "name": "무슨 의도로 한 말인지는\n저도 모르겠어요"
    },
    {
      "$id": 3,
      "label": "bf06380100_0030",
      "style": 82,
      "name": "단순히 인간과 블레이드의\n공존을 말한 건지, 아니면…"
    },
    {
      "$id": 4,
      "label": "bf06380100_0040",
      "style": 82,
      "name": "그래도 그건 일종의 각오였을 거예요"
    },
    {
      "$id": 5,
      "label": "bf06380100_0050",
      "style": 82,
      "name": "인간이 살아남기 위한…"
    },
    {
      "$id": 6,
      "label": "bf06380100_0060",
      "style": 82,
      "name": "[ML:undisp ]"
    },
    {
      "$id": 7,
      "label": "bf06380100_0070",
      "style": 82,
      "name": "폐하!"
    },
    {
      "$id": 8,
      "label": "bf06380100_0080",
      "style": 82,
      "name": "무슨 일이냐!"
    },
    {
      "$id": 9,
      "label": "bf06380100_0090",
      "style": 82,
      "name": "겐부가"
    },
    {
      "$id": 10,
      "label": "bf06380100_0100",
      "style": 82,
      "name": "저희의 제어를 벗어나\n운해로 가라앉고 있습니다"
    },
    {
      "$id": 11,
      "label": "bf06380100_0110",
      "style": 82,
      "name": "뭐라? 정말인가!"
    },
    {
      "$id": 12,
      "label": "bf06380100_0120",
      "style": 82,
      "name": "[ML:undisp ]"
    },
    {
      "$id": 13,
      "label": "bf06380100_0130",
      "style": 82,
      "name": "사이카!"
    },
    {
      "$id": 14,
      "label": "bf06380100_0140",
      "style": 82,
      "name": "아, 안 된다…"
    },
    {
      "$id": 15,
      "label": "bf06380100_0150",
      "style": 82,
      "name": "무슨 일이 일어난 겁니까?"
    },
    {
      "$id": 16,
      "label": "bf06380100_0160",
      "style": 82,
      "name": "설마… 아까 그 일 때문에"
    },
    {
      "$id": 17,
      "label": "bf06380100_0170",
      "style": 82,
      "name": "아버지"
    },
    {
      "$id": 18,
      "label": "bf06380100_0175",
      "style": 82,
      "name": "무슨 말이야?"
    },
    {
      "$id": 19,
      "label": "bf06380100_0180",
      "style": 82,
      "name": "겐부는 상투스 체인을 써가 제어하고 있그든"
    },
    {
      "$id": 20,
      "label": "bf06380100_0190",
      "style": 82,
      "name": "아까 에테르 가속기를 썼제?"
    },
    {
      "$id": 21,
      "label": "bf06380100_0200",
      "style": 82,
      "name": "아마 겐부에 흐르는 에너지가 역류해서"
    },
    {
      "$id": 22,
      "label": "bf06380100_0210",
      "style": 82,
      "name": "과부하를 못 버텼는갑다"
    },
    {
      "$id": 23,
      "label": "bf06380100_0220",
      "style": 82,
      "name": "망가졌다는 거야?"
    },
    {
      "$id": 24,
      "label": "bf06380100_0230",
      "style": 82,
      "name": "이제 내 말도 안 듣는데이"
    },
    {
      "$id": 25,
      "label": "bf06380100_0240",
      "style": 82,
      "name": "이대로 가면 점점 깊이 가라앉을 테고\n그라믄…"
    },
    {
      "$id": 26,
      "label": "bf06380100_0250",
      "style": 82,
      "name": "멍청하게 저런 골동품을 쓰니까 그런 기야!"
    },
    {
      "$id": 27,
      "label": "bf06380100_0260",
      "style": 82,
      "name": "운해의 밀도는 물과 거의 똑같아"
    },
    {
      "$id": 28,
      "label": "bf06380100_0270",
      "style": 82,
      "name": "깊이 가라앉으면 그만큼 압력이 높아져"
    },
    {
      "$id": 29,
      "label": "bf06380100_0280",
      "style": 82,
      "name": "인간은 500만 내려가도 못 견딘다고"
    },
    {
      "$id": 30,
      "label": "bf06380100_0290",
      "style": 82,
      "name": "그런 압력이 겐부에게…"
    },
    {
      "$id": 31,
      "label": "bf06380100_0300",
      "style": 82,
      "name": "심도는 지금 몇이고?"
    },
    {
      "$id": 32,
      "label": "bf06380100_0310",
      "style": 82,
      "name": "약 2200멜트입니다"
    },
    {
      "$id": 33,
      "label": "bf06380100_0320",
      "style": 82,
      "name": "1분에 120멜트의 속도로\n가라앉고 있습니다"
    },
    {
      "$id": 34,
      "label": "bf06380100_0330",
      "style": 82,
      "name": "외곽의 한계 심도는?"
    },
    {
      "$id": 35,
      "label": "bf06380100_0340",
      "style": 82,
      "name": "25000이 한계일 겁니다…"
    },
    {
      "$id": 36,
      "label": "bf06380100_0350",
      "style": 82,
      "name": "그럼 길어도 3시간 정도겠네"
    },
    {
      "$id": 37,
      "label": "bf06380100_0360",
      "style": 82,
      "name": "거북이 주제에 계산이 빠르네"
    },
    {
      "$id": 38,
      "label": "bf06380100_0370",
      "style": 82,
      "name": "멍청아!"
    },
    {
      "$id": 39,
      "label": "bf06380100_0380",
      "style": 82,
      "name": "진짜 위험하데이"
    },
    {
      "$id": 40,
      "label": "bf06380100_0390",
      "style": 82,
      "name": "꼬맹아, 왕궁의 남쪽 탑에 잠수정이 있다"
    },
    {
      "$id": 41,
      "label": "bf06380100_0400",
      "style": 82,
      "name": "니들만이라도 그걸 타고 도망치라"
    },
    {
      "$id": 42,
      "label": "bf06380100_0410",
      "style": 82,
      "name": "지크랑 이곳 사람들은?"
    },
    {
      "$id": 43,
      "label": "bf06380100_0420",
      "style": 82,
      "name": "우리는 됐다"
    },
    {
      "$id": 44,
      "label": "bf06380100_0430",
      "style": 82,
      "name": "됐다니, 어떻게 우리만 가겠어?"
    },
    {
      "$id": 45,
      "label": "bf06380100_0440",
      "style": 82,
      "name": "무슨 방법 없어?\n모두를 구할 수 있는 방법 말이야"
    },
    {
      "$id": 46,
      "label": "bf06380100_0450",
      "style": 82,
      "name": "겐부는 사이카만 제어할 수 있다"
    },
    {
      "$id": 47,
      "label": "bf06380100_0460",
      "style": 82,
      "name": "사이카가 몬 한다면 방법이 없데이"
    },
    {
      "$id": 48,
      "label": "bf06380100_0470",
      "style": 82,
      "name": "남은 건 되도록 많은 사람을\n탈출시키는 긴데…"
    },
    {
      "$id": 49,
      "label": "bf06380100_0480",
      "style": 82,
      "name": "3시간밖에 없으니…"
    },
    {
      "$id": 50,
      "label": "bf06380100_0490",
      "style": 82,
      "name": "겐부는 상투스 체인으로 제어했던 거죠?"
    },
    {
      "$id": 51,
      "label": "bf06380100_0500",
      "style": 82,
      "name": "그래, 맞다"
    },
    {
      "$id": 52,
      "label": "bf06380100_0510",
      "style": 82,
      "name": "어디에 있죠?"
    },
    {
      "$id": 53,
      "label": "bf06380100_0520",
      "style": 82,
      "name": "겐부의 머리다"
    },
    {
      "$id": 54,
      "label": "bf06380100_0530",
      "style": 82,
      "name": "왕궁 남쪽에서 척추를 타고 가면 나온데이"
    },
    {
      "$id": 55,
      "label": "bf06380100_0540",
      "style": 82,
      "name": "제가 갈게요"
    },
    {
      "$id": 56,
      "label": "bf06380100_0550",
      "style": 82,
      "name": "간다니…\n호무라, 니…"
    },
    {
      "$id": 57,
      "label": "bf06380100_0560",
      "style": 82,
      "name": "상투스 체인은\n원래 서펀트를 제어하는 코어예요"
    },
    {
      "$id": 58,
      "label": "bf06380100_0570",
      "style": 82,
      "name": "그리고 서펀트는 제 디바이스죠"
    },
    {
      "$id": 59,
      "label": "bf06380100_0580",
      "style": 82,
      "name": "단언할 수는 없지만\n저라면 고칠 수 있을지도 몰라요"
    },
    {
      "$id": 60,
      "label": "bf06380100_0590",
      "style": 82,
      "name": "호무라…"
    },
    {
      "$id": 61,
      "label": "bf06380100_0600",
      "style": 82,
      "name": "호무라, 진심이야모?"
    },
    {
      "$id": 62,
      "label": "bf06380100_0610",
      "style": 82,
      "name": "아까 죽을 뻔했는데 도와주는 거야모?"
    },
    {
      "$id": 63,
      "label": "bf06380100_0620",
      "style": 82,
      "name": "그건 그거고 이건 이거\n렉스라면 그렇게 말할 거죠?"
    },
    {
      "$id": 64,
      "label": "bf06380100_0630",
      "style": 82,
      "name": "어? 아, 응… 그렇지"
    },
    {
      "$id": 65,
      "label": "bf06380100_0640",
      "style": 82,
      "name": "그런 이유예요"
    },
    {
      "$id": 66,
      "label": "bf06380100_0650",
      "style": 82,
      "name": "호무라…"
    },
    {
      "$id": 67,
      "label": "bf06380100_0660",
      "style": 82,
      "name": "알았어모, 그런 거라면\n토라도 하나도 도와줄게모"
    },
    {
      "$id": 68,
      "label": "bf06380100_0670",
      "style": 82,
      "name": "할 수 없지, 뱌코"
    },
    {
      "$id": 69,
      "label": "bf06380100_0680",
      "style": 82,
      "name": "처음부터 그럴 생각이었습니다"
    },
    {
      "$id": 70,
      "label": "bf06380100_0690",
      "style": 82,
      "name": "그렇게 됐으니까 방해하지 마\n알았지?"
    },
    {
      "$id": 71,
      "label": "bf06380100_0700",
      "style": 82,
      "name": "그대들은 목숨을 걸겠다는 건가?"
    },
    {
      "$id": 72,
      "label": "bf06380100_0710",
      "style": 82,
      "name": "하늘의 성배를 말살하려 한\n룩수리아를 위해?"
    },
    {
      "$id": 73,
      "label": "bf06380100_0720",
      "style": 82,
      "name": "나라를 위한 게 아니야"
    },
    {
      "$id": 74,
      "label": "bf06380100_0730",
      "style": 82,
      "name": "그럼 무엇을 위해서…"
    },
    {
      "$id": 75,
      "label": "bf06380100_0740",
      "style": 82,
      "name": "왕이잖아?\n그럼 말하지 않아도 알 텐데"
    },
    {
      "$id": 76,
      "label": "bf06380100_0750",
      "style": 82,
      "name": "렉스, 그대는…"
    },
    {
      "$id": 77,
      "label": "bf06380100_0760",
      "style": 82,
      "name": "멜레프?"
    },
    {
      "$id": 78,
      "label": "bf06380100_0770",
      "style": 82,
      "name": "하는 수 없군"
    },
    {
      "$id": 79,
      "label": "bf06380100_0780",
      "style": 82,
      "name": "지크, 상투스 체인이 어디에 있는지\n자세히 알려 줘"
    },
    {
      "$id": 80,
      "label": "bf06380100_0790",
      "style": 82,
      "name": "그래, 따라온나\n안내해 주께"
    },
    {
      "$id": 81,
      "label": "bf06380100_0800",
      "style": 82,
      "name": "지크"
    },
    {
      "$id": 82,
      "label": "bf06380100_0810",
      "style": 82,
      "name": "아버지\n만일의 사태에 대비해가 주민들을 대피시키라"
    },
    {
      "$id": 83,
      "label": "bf06380100_0820",
      "style": 82,
      "name": "뭐, 우리가 나서는 거니까"
    },
    {
      "$id": 84,
      "label": "bf06380100_0830",
      "style": 82,
      "name": "만일의 사태는 안 일어날 거라 생각하지만"
    },
    {
      "$id": 85,
      "label": "bf06380100_0840",
      "style": 82,
      "name": "알겠다"
    },
    {
      "$id": 86,
      "label": "bf06380100_0850",
      "style": 82,
      "name": "시간이 없데이\n서두르자"
    },
    {
      "$id": 87,
      "label": "bf06380100_0860",
      "style": 82,
      "name": "그래!"
    }
  ]
}