  "rows": [
    {
      "$id": 1,
      "label": "bf09250100_0010",
      "style": 82,
      "name": "…자네"
    },
    {
      "$id": 2,
      "label": "bf09250100_0020",
      "style": 82,
      "name": "혹시 오르넬라 씨의\n블레이드였던 사람 아닌가?"
    },
    {
      "$id": 3,
      "label": "bf09250100_0030",
      "style": 82,
      "name": "오르넬라?\n아니, 나는…"
    },
    {
      "$id": 4,
      "label": "bf09250100_0040",
      "style": 82,
      "name": "맞지? 그 근위군의"
    },
    {
      "$id": 5,
      "label": "bf09250100_0050",
      "style": 82,
      "name": "근위…"
    },
    {
      "$id": 6,
      "label": "bf09250100_0060",
      "style": 82,
      "name": "그립구먼\n그때 나는 아직 요만한 꼬마였지"
    },
    {
      "$id": 7,
      "label": "bf09250100_0070",
      "style": 82,
      "name": "그 장검을 짊어진 모습을 동경했었다네"
    },
    {
      "$id": 8,
      "label": "bf09250100_0080",
      "style": 82,
      "name": "자네들이 와 주지 않았더라면\n지금쯤 이 마을은…"
    },
    {
      "$id": 9,
      "label": "bf09250100_0090",
      "style": 82,
      "name": "그렇지"
    },
    {
      "$id": 10,
      "label": "bf09250100_0100",
      "style": 82,
      "name": "자네들이 살던 집이\n마을 외곽에 남아 있다네"
    },
    {
      "$id": 11,
      "label": "bf09250100_0110",
      "style": 82,
      "name": "집?"
    },
    {
      "$id": 12,
      "label": "bf09250100_0120",
      "style": 82,
      "name": "[ML:undisp ]"
    },
    {
      "$id": 13,
      "label": "bf09250100_0130",
      "style": 82,
      "name": "기나긴 독립 전쟁도\n머지않아 종지부를 찍을 것이다"
    },
    {
      "$id": 14,
      "label": "bf09250100_0140",
      "style": 82,
      "name": "하지만 끝나는 건 그뿐만이 아니다"
    },
    {
      "$id": 15,
      "label": "bf09250100_0150",
      "style": 82,
      "name": "그녀의 수명은\n그리 오래 남지 않았을 것이다"
    },
    {
      "$id": 16,
      "label": "bf09250100_0160",
      "style": 82,
      "name": "그건 동시에 나의 끝을 의미한다"
    },
    {
      "$id": 17,
      "label": "bf09250100_0170",
      "style": 82,
      "name": "그녀의 죽음과 무관하게\n나는 당연한 듯 코어로 돌아가서"
    },
    {
      "$id": 18,
      "label": "bf09250100_0180",
      "style": 82,
      "name": "다시 눈뜰 날을 기다리겠지"
    },
    {
      "$id": 19,
      "label": "bf09250100_0190",
      "style": 82,
      "name": "지금까지 수천의 밤을 그래 왔듯이"
    },
    {
      "$id": 20,
      "label": "bf09250100_0200",
      "style": 82,
      "name": "그 후에 다시 눈뜬 나는 이전의 나일까"
    },
    {
      "$id": 21,
      "label": "bf09250100_0210",
      "style": 82,
      "name": "\"신\"이라는 자가 정말로 존재한다면\n나는 묻고 싶다"
    },
    {
      "$id": 22,
      "label": "bf09250100_0220",
      "style": 82,
      "name": "나는 누구인가"
    },
    {
      "$id": 23,
      "label": "bf09250100_0230",
      "style": 82,
      "name": "어디서 와서 어디로 가는 것인가"
    },
    {
      "$id": 24,
      "label": "bf09250100_0240",
      "style": 82,
      "name": "아무쪼록\n이 일기가 \"네\" 손에 전해지길 바란다"
    },
    {
      "$id": 25,
      "label": "bf09250100_0250",
      "style": 82,
      "name": "그걸 믿고 마지막으로\n내가 나로 있기 위한 방법을"
    },
    {
      "$id": 26,
      "label": "bf09250100_0260",
      "style": 82,
      "name": "내 반려와의 인연을\n이 몸에 새길 방법을 남긴다"
    },
    {
      "$id": 27,
      "label": "bf09250100_0270",
      "style": 82,
      "name": "그 방법은…"
    },
    {
      "$id": 28,
      "label": "bf09250100_0275",
      "style": 82,
      "name": "[ML:undisp ]"
    },
    {
      "$id": 29,
      "label": "bf09250100_0280",
      "style": 82,
      "name": "우와, 엄청나게 낡았네"
    },
    {
      "$id": 30,
      "label": "bf09250100_0290",
      "style": 82,
      "name": "90년쯤 지났다고 들었는데…"
    },
    {
      "$id": 31,
      "label": "bf09250100_0300",
      "style": 82,
      "name": "왜 그래?\n추억이 담긴 물건이라도 찾았어?"
    },
    {
      "$id": 32,
      "label": "bf09250100_0310",
      "style": 82,
      "name": "아, 아니… 아무것도 없었어"
    },
    {
      "$id": 33,
      "label": "bf09250100_0320",
      "style": 82,
      "name": "그렇구나, 아쉽네\n옛날의 너도 알고 싶었는데"
    },
    {
      "$id": 34,
      "label": "bf09250100_0330",
      "style": 82,
      "name": "나는… 나야\n언제든 그 사실은 변함없어"
    }
  ]
}