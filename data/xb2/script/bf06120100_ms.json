      "$id": 1,
      "label": "bf06120100_0002",
      "style": 82,
      "name": "미안하군\n오래 기다렸나?"
    },
    {
      "$id": 2,
      "label": "bf06120100_0004",
      "style": 82,
      "name": "아뇨, 괜찮아요"
    },
    {
      "$id": 3,
      "label": "bf06120100_0006",
      "style": 82,
      "name": "그런데… 방금 그건?"
    },
    {
      "$id": 4,
      "label": "bf06120100_0008",
      "style": 82,
      "name": "코어 크리스털의 세례다"
    },
    {
      "$id": 5,
      "label": "bf06120100_0010",
      "style": 82,
      "name": "이렇게 하면 동조할 확률이 훨씬 올라가지"
    },
    {
      "$id": 6,
      "label": "bf06120100_0012",
      "style": 82,
      "name": "동조에는 아무래도 위험이 따르니까"
    },
    {
      "$id": 7,
      "label": "bf06120100_0014",
      "style": 82,
      "name": "하긴 그렇죠"
    },
    {
      "$id": 8,
      "label": "bf06120100_0016",
      "style": 82,
      "name": "하늘의 성배의 드라이버가 되고 나서\n나는 이 힘을 얻었지"
    },
    {
      "$id": 9,
      "label": "bf06120100_0018",
      "style": 82,
      "name": "그 말은 어쩌면 저도?"
    },
    {
      "$id": 10,
      "label": "bf06120100_0020",
      "style": 82,
      "name": "글쎄?\n누구나 적성이라는 게 있으니까"
    },
    {
      "$id": 11,
      "label": "bf06120100_0022",
      "style": 82,
      "name": "그렇죠"
    },
    {
      "$id": 12,
      "label": "bf06120100_0024",
      "style": 82,
      "name": "그럼…\n오늘 할 일은 이걸로 끝이군"
    },
    {
      "$id": 13,
      "label": "bf06120100_0026",
      "style": 82,
      "name": "따라오게"
    },
    {
      "$id": 14,
      "label": "bf06120100_0028",
      "style": 82,
      "name": "룩수리아로 보내는 특사… 말인가요?\n제가요?"
    },
    {
      "$id": 15,
      "label": "bf06120100_0030",
      "style": 82,
      "name": "그래, 이걸 보거라"
    },
    {
      "$id": 16,
      "label": "bf06120100_0035",
      "style": 82,
      "name": "그대는 샐비저니까 잘 알겠지만"
    },
    {
      "$id": 17,
      "label": "bf06120100_0037",
      "style": 82,
      "name": "이게 현재 우리가 사는 아르스트의 세계다"
    },
    {
      "$id": 18,
      "label": "bf06120100_0040",
      "style": 82,
      "name": "세계수를 중심으로 아르스가 그 주위를 돌고 있지"
    },
    {
      "$id": 19,
      "label": "bf06120100_0050",
      "style": 82,
      "name": "그리고 여기가 대공동이다"
    },
    {
      "$id": 20,
      "label": "bf06120100_0060",
      "style": 82,
      "name": "대공동은 500년 전에 생겨났다\n그전에는 없었지"
    },
    {
      "$id": 21,
      "label": "bf06120100_0070",
      "style": 82,
      "name": "저도 알아요\n이것 때문에 우리는 세계수로 갈 수 없죠"
    },
    {
      "$id": 22,
      "label": "bf06120100_0080",
      "style": 82,
      "name": "이 대공동은 서펀트라고 불리는\n거대한 몬스터가 만들어 내지"
    },
    {
      "$id": 23,
      "label": "bf06120100_0090",
      "style": 82,
      "name": "그건 그때 봤던…"
    }
  ]
