  "rows": [
    {
      "$id": 1,
      "label": "bf02400100_0010",
      "style": 82,
      "name": "이 불꽃은!"
    },
    {
      "$id": 2,
      "label": "bf02400100_0020",
      "style": 82,
      "name": "그 녀석인가"
    },
    {
      "$id": 3,
      "label": "bf02400100_0030",
      "style": 82,
      "name": "이번에는 드라이버도 같이 온 건가"
    },
    {
      "$id": 4,
      "label": "bf02400100_0040",
      "style": 82,
      "name": "불꽃의 인도자… 멜레프"
    },
    {
      "$id": 5,
      "label": "bf02400100_0050",
      "style": 82,
      "name": "멜레프?"
    },
    {
      "$id": 6,
      "label": "bf02400100_0060",
      "style": 82,
      "name": "수페르비아 제국 특별 집권관 멜레프"
    },
    {
      "$id": 7,
      "label": "bf02400100_0070",
      "style": 82,
      "name": "제국 최강의 드라이버이면서\n제국 최강의 블레이드인 카구츠치를 사용하는 자"
    },
    {
      "$id": 8,
      "label": "bf02400100_0080",
      "style": 82,
      "name": "최강에 최강을 얹은 완전 최강이라는 건가"
    },
    {
      "$id": 9,
      "label": "bf02400100_0090",
      "style": 82,
      "name": "매복하고 있었던 모양이네요"
    },
    {
      "$id": 10,
      "label": "bf02400100_0100",
      "style": 82,
      "name": "그래서 이상할 정도로 쉽게\n탈출할 수 있었던 게로군"
    },
    {
      "$id": 11,
      "label": "bf02400100_0110",
      "style": 82,
      "name": "역시 모프가 막을 수 있는 상대는 아니었나"
    },
    {
      "$id": 12,
      "label": "bf02400100_0120",
      "style": 82,
      "name": "너지?"
    },
    {
      "$id": 13,
      "label": "bf02400100_0130",
      "style": 82,
      "name": "내가 처형된다는 거짓 정보를\n렉스 일행에게 들어가도록 조작한 게!"
    },
    {
      "$id": 14,
      "label": "bf02400100_0140",
      "style": 82,
      "name": "감이 좋은걸\n그래, 너는 너대로 이용 가치가 있다"
    },
    {
      "$id": 15,
      "label": "bf02400100_0150",
      "style": 82,
      "name": "하지만…"
    },
    {
      "$id": 16,
      "label": "bf02400100_0160",
      "style": 82,
      "name": "렉스 일행은 더 이용 가치가 크다?"
    },
    {
      "$id": 17,
      "label": "bf02400100_0170",
      "style": 82,
      "name": "조금 다르군"
    },
    {
      "$id": 18,
      "label": "bf02400100_0180",
      "style": 82,
      "name": "에메랄드색 코어 크리스털은\n하늘의 성배라는 증거"
    },
    {
      "$id": 19,
      "label": "bf02400100_0190",
      "style": 82,
      "name": "그 블레이드가 진짜 하늘의 성배라면\n내게는 해야만 하는 일이 있다"
    },
    {
      "$id": 20,
      "label": "bf02400100_0200",
      "style": 82,
      "name": "해야만 하는 일?"
    },
    {
      "$id": 21,
      "label": "bf02400100_0210",
      "style": 82,
      "name": "하늘을 찢고 대지를 가르는 그 힘…"
    },
    {
      "$id": 22,
      "label": "bf02400100_0220",
      "style": 82,
      "name": "또다시 세계를 불태우게 둘 수는 없지"
    },
    {
      "$id": 23,
      "label": "bf02400100_0230",
      "style": 82,
      "name": "호무라가 세계를 불태웠다고?\n말도 안 되는 소리 하지 마!"
    },
    {
      "$id": 24,
      "label": "bf02400100_0240",
      "style": 82,
      "name": "모르는 거냐?\n500년 전의 성배 대전에서 일어난 일"
    },
    {
      "$id": 25,
      "label": "bf02400100_0250",
      "style": 82,
      "name": "3개의 아르스를\n운해 깊은 곳에 가라앉힌 전설의 힘을"
    },
    {
      "$id": 26,
      "label": "bf02400100_0260",
      "style": 82,
      "name": "아르스를 3개나…?"
    },
    {
      "$id": 27,
      "label": "bf02400100_0270",
      "style": 82,
      "name": "전부 역사에 기록된 사실이다"
    },
    {
      "$id": 28,
      "label": "bf02400100_0280",
      "style": 82,
      "name": "알겠다\n호무라를 전쟁의 도구로 쓰려는 거지?"
    },
    {
      "$id": 29,
      "label": "bf02400100_0290",
      "style": 82,
      "name": "너희 뜻대로는 안 될 거다!"
    },
    {
      "$id": 30,
      "label": "bf02400100_0300",
      "style": 82,
      "name": "그런 힘을\n방치할 수 없다는 거다"
    },
    {
      "$id": 31,
      "label": "bf02400100_0310",
      "style": 82,
      "name": "싫다면?"
    },
    {
      "$id": 32,
      "label": "bf02400100_0320",
      "style": 82,
      "name": "강제로라도 너희를 구속하겠다"
    },
    {
      "$id": 33,
      "label": "bf02400100_0330",
      "style": 82,
      "name": "그렇다면 진심을 담아 말해 주지"
    },
    {
      "$id": 34,
      "label": "bf02400100_0332",
      "style": 82,
      "name": "절~대로"
    },
    {
      "$id": 35,
      "label": "bf02400100_0334",
      "style": 82,
      "name": "안! 돼!"
    },
    {
      "$id": 36,
      "label": "bf02400100_0340",
      "style": 82,
      "name": "훗… 그렇다면 그 의지를\n힘으로 증명해 봐라, 소년!"
    }
  ]
}