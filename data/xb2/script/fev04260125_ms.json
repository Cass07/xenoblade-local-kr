  "rows": [
    {
      "$id": 1,
      "label": "fev04_260_125_msg0001",
      "style": 84,
      "name": "이제…\n키쿠의 행방은 어떻게 찾지?"
    },
    {
      "$id": 2,
      "label": "fev04_260_125_msg0002",
      "style": 84,
      "name": "늘 그랬던 것처럼 마을 사람들에게\n물어보는 건… 소용없겠지"
    },
    {
      "$id": 3,
      "label": "fev04_260_125_msg0003",
      "style": 84,
      "name": "그러게… 한밤중이었으니\n본 사람도 거의 없을 거야"
    },
    {
      "$id": 4,
      "label": "fev04_260_125_msg0004",
      "style": 84,
      "name": "후후후… 아무래도 토라가\n나서야겠군모"
    },
    {
      "$id": 5,
      "label": "fev04_260_125_msg0005",
      "style": 84,
      "name": "토라, 뭔가 좋은 생각이라도 있어?"
    },
    {
      "$id": 6,
      "label": "fev04_260_125_msg0006",
      "style": 84,
      "name": "키쿠를 완성한 건\n우리 아빠일 거야모"
    },
    {
      "$id": 7,
      "label": "fev04_260_125_msg0007",
      "style": 84,
      "name": "어젯밤에 그렇게 말씀하셨죠"
    },
    {
      "$id": 8,
      "label": "fev04_260_125_msg0008",
      "style": 84,
      "name": "그리고 하나는 아빠가 준\n설계도를 토대로 만들었다고\n했잖아모?"
    },
    {
      "$id": 9,
      "label": "fev04_260_125_msg0009",
      "style": 84,
      "name": "한마디로 키쿠와 하나는\n거의 똑같이 설계돼 있어모"
    },
    {
      "$id": 10,
      "label": "fev04_260_125_msg0010",
      "style": 84,
      "name": "그 말은… 하나와 키쿠를\n만드는 데 필요한 소재는\n비슷하다는 게로군?"
    },
    {
      "$id": 11,
      "label": "fev04_260_125_msg0011",
      "style": 84,
      "name": "그거야모!"
    },
    {
      "$id": 12,
      "label": "fev04_260_125_msg0012",
      "style": 84,
      "name": "오…"
    },
    {
      "$id": 13,
      "label": "fev04_260_125_msg0013",
      "style": 84,
      "name": "수페르비아에서 소재의 경로를 추적하면\n키쿠를 만든 사람을 만날 수 있다는 거네"
    },
    {
      "$id": 14,
      "label": "fev04_260_125_msg0014",
      "style": 84,
      "name": "주인, 웬일로 오랜만에 명안을 내셨네요모"
    },
    {
      "$id": 15,
      "label": "fev04_260_125_msg0015",
      "style": 84,
      "name": "토라, 인공 블레이드를 만드는 덴\n어떤 소재가 필요해?"
    },
    {
      "$id": 16,
      "label": "fev04_260_125_msg0016",
      "style": 84,
      "name": "뭐였더라모…\n에테르 노심은 꼭 있어야 해모\n그리고 다른 건…"
    },
    {
      "$id": 17,
      "label": "fev04_260_125_msg0017",
      "style": 84,
      "name": "테트라 서클"
    },
    {
      "$id": 18,
      "label": "fev04_260_125_msg0018",
      "style": 84,
      "name": "스네이크 조인트"
    },
    {
      "$id": 19,
      "label": "fev04_260_125_msg0019",
      "style": 84,
      "name": "블랙 애시"
    },
    {
      "$id": 20,
      "label": "fev04_260_125_msg0020",
      "style": 84,
      "name": "델타 박스"
    },
    {
      "$id": 21,
      "label": "fev04_260_125_msg0021",
      "style": 84,
      "name": "래빗 다이오드"
    },
    {
      "$id": 22,
      "label": "fev04_260_125_msg0022",
      "style": 84,
      "name": "메타모르 오브"
    },
    {
      "$id": 23,
      "label": "fev04_260_125_msg0023",
      "style": 84,
      "name": "포톤 서클"
    },
    {
      "$id": 24,
      "label": "fev04_260_125_msg0024",
      "style": 84,
      "name": "메르헨 벨트 풀리"
    },
    {
      "$id": 25,
      "label": "fev04_260_125_msg0025",
      "style": 84,
      "name": "아크 인디케이터"
    },
    {
      "$id": 26,
      "label": "fev04_260_125_msg0026",
      "style": 84,
      "name": "인섹트 칩"
    },
    {
      "$id": 27,
      "label": "fev04_260_125_msg0027",
      "style": 84,
      "name": "완벽 측거 센서"
    },
    {
      "$id": 28,
      "label": "fev04_260_125_msg0028",
      "style": 84,
      "name": "비욘 커넥터"
    },
    {
      "$id": 29,
      "label": "fev04_260_125_msg0029",
      "style": 84,
      "name": "잠깐, 잠깐!"
    },
    {
      "$id": 30,
      "label": "fev04_260_125_msg0030",
      "style": 84,
      "name": "그걸 어떻게 다 외워!"
    },
    {
      "$id": 31,
      "label": "fev04_260_125_msg0031",
      "style": 84,
      "name": "아직 30분의 1도 말 안 했는데모…"
    },
    {
      "$id": 32,
      "label": "fev04_260_125_msg0032",
      "style": 84,
      "name": "하지만 익숙한 이름이 있구먼"
    },
    {
      "$id": 33,
      "label": "fev04_260_125_msg0033",
      "style": 84,
      "name": "비욘 커넥터 말인데\n굴라에서 샐비지하면\n흔히 나오는 물건 맞지?"
    },
    {
      "$id": 34,
      "label": "fev04_260_125_msg0034",
      "style": 84,
      "name": "지금은 굴라에서만 구할 수 있을 거야모"
    },
    {
      "$id": 35,
      "label": "fev04_260_125_msg0035",
      "style": 84,
      "name": "그 말은 굴라에서\n수입할 수밖에 없다는 거지?"
    },
    {
      "$id": 36,
      "label": "fev04_260_125_msg0036",
      "style": 84,
      "name": "아난얌 항구에 가면\n단서가 있을지도 모르겠군요"
    },
    {
      "$id": 37,
      "label": "fev04_260_125_msg0037",
      "style": 84,
      "name": "달리 유력한 단서도 없으니\n일단 가 볼까"
    }
  ]
