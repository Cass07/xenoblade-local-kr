    {
      "$id": 12,
      "label": "qst006002_msg0012",
      "style": 84,
      "name": "힘내, 쿠비라"
    },
    {
      "$id": 13,
      "label": "qst006002_msg0013",
      "style": 84,
      "name": "흥, 걱정은 필요 없다"
