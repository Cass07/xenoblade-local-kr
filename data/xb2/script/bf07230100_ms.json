  "rows": [
    {
      "$id": 1,
      "label": "bf07230100_0010",
      "style": 82,
      "name": "이 앞이…"
    },
    {
      "$id": 2,
      "label": "bf07230100_0020",
      "style": 82,
      "name": "음, 여기부턴 정신 똑바로 차리거라"
    },
    {
      "$id": 3,
      "label": "bf07230100_0030",
      "style": 82,
      "name": "안에 들어가면\n블레이드는 힘의 대부분을 잃을 게다"
    },
    {
      "$id": 4,
      "label": "bf07230100_0040",
      "style": 82,
      "name": "경우에 따라서는 에너지가 다해\n코어로 환원되어"
    },
    {
      "$id": 5,
      "label": "bf07230100_0050",
      "style": 82,
      "name": "다시는 블레이드가 되지 못할 수도 있으니"
    },
    {
      "$id": 6,
      "label": "bf07230100_0060",
      "style": 82,
      "name": "사이카, 니는 여기 남아라"
    },
    {
      "$id": 7,
      "label": "bf07230100_0070",
      "style": 82,
      "name": "와? 와 내만 남는데?"
    },
    {
      "$id": 8,
      "label": "bf07230100_0080",
      "style": 82,
      "name": "와 그카는지는\n니가 제일 잘 알 긴데?"
    },
    {
      "$id": 9,
      "label": "bf07230100_0090",
      "style": 82,
      "name": "그래 따지면 왕자도…"
    },
    {
      "$id": 10,
      "label": "bf07230100_0100",
      "style": 82,
      "name": "내는 근성으로 해결하께"
    },
    {
      "$id": 11,
      "label": "bf07230100_0110",
      "style": 82,
      "name": "그라믄 내도…"
    },
    {
      "$id": 12,
      "label": "bf07230100_0120",
      "style": 82,
      "name": "안 된다 안 카나"
    },
    {
      "$id": 13,
      "label": "bf07230100_0130",
      "style": 82,
      "name": "뭔가 사정이 있나 보군"
    },
    {
      "$id": 14,
      "label": "bf07230100_0140",
      "style": 82,
      "name": "별일 아이다, 이쪽 얘기다"
    },
    {
      "$id": 15,
      "label": "bf07230100_0150",
      "style": 82,
      "name": "싫데이… 죽어도 싫다"
    },
    {
      "$id": 16,
      "label": "bf07230100_0160",
      "style": 82,
      "name": "그때 약속했다 아이가\n까먹은 건 아닐 긴데?"
    },
    {
      "$id": 17,
      "label": "bf07230100_0170",
      "style": 82,
      "name": "[ML:undisp ]"
    },
    {
      "$id": 18,
      "label": "bf07230100_0180",
      "style": 82,
      "name": "왕자가 가면 내도 간다\n알겠제?"
    },
    {
      "$id": 19,
      "label": "bf07230100_0190",
      "style": 82,
      "name": "맘대로 해라"
    },
    {
      "$id": 20,
      "label": "bf07230100_0200",
      "style": 82,
      "name": "아가씨…"
    },
    {
      "$id": 21,
      "label": "bf07230100_0210",
      "style": 82,
      "name": "괜찮아\n여기서 멈출 수는 없어"
    },
    {
      "$id": 22,
      "label": "bf07230100_0220",
      "style": 82,
      "name": "뱌코, 그 얘기는 비밀인 거 알지?"
    },
    {
      "$id": 23,
      "label": "bf07230100_0230",
      "style": 82,
      "name": "아… 알겠습니다"
    },
    {
      "$id": 24,
      "label": "bf07230100_0240",
      "style": 82,
      "name": "안에 뭐가 있을지 모른다"
    },
    {
      "$id": 25,
      "label": "bf07230100_0250",
      "style": 82,
      "name": "게다가 토라와 하나를 제외한\n우리의 아츠는 아주 약해질 거다"
    },
    {
      "$id": 26,
      "label": "bf07230100_0260",
      "style": 82,
      "name": "조심해라, 렉스"
    },
    {
      "$id": 27,
      "label": "bf07230100_0270",
      "style": 82,
      "name": "맞다, 회복 아츠도 효과가 약해지니까"
    },
    {
      "$id": 28,
      "label": "bf07230100_0280",
      "style": 82,
      "name": "크게 다치면 끝장이데이"
    },
    {
      "$id": 29,
      "label": "bf07230100_0290",
      "style": 82,
      "name": "그래, 알았어\n토라, 하나, 방패 역할을 맡아 줘"
    },
    {
      "$id": 30,
      "label": "bf07230100_0300",
      "style": 82,
      "name": "형님, 맡겨 줘모"
    },
    {
      "$id": 31,
      "label": "bf07230100_0310",
      "style": 82,
      "name": "하나는 인공 블레이드니까 조금도 요만큼도\n눈곱만큼도 걱정할 것 없어모"
    },
    {
      "$id": 32,
      "label": "bf07230100_0320",
      "style": 82,
      "name": "기술의 승리야모!\n공업 문명 만세다모~~~"
    },
    {
      "$id": 33,
      "label": "bf07230100_0330",
      "style": 82,
      "name": "엣헴이에요모!"
    },
    {
      "$id": 34,
      "label": "bf07230100_0340",
      "style": 82,
      "name": "하하하, 너희들만 믿을게"
    }
  ]
}