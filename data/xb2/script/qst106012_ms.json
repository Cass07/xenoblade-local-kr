  "rows": [
    {
      "$id": 1,
      "label": "qst106012_msg0001",
      "style": 84,
      "name": "다녀왔어요모"
    },
    {
      "$id": 2,
      "label": "qst106012_msg0002",
      "style": 84,
      "name": "어서 오세요모, 하나\n왠지 조금 어른스러운 기운이\n느껴지네요모"
    },
    {
      "$id": 3,
      "label": "qst106012_msg0003",
      "style": 84,
      "name": "맞아요모\n하나는 숙녀가 되었어요모"
    },
    {
      "$id": 4,
      "label": "qst106012_msg0004",
      "style": 84,
      "name": "키쿠가 의상을 완성해서\n우리도 준비가 다 됐단다모"
    },
    {
      "$id": 5,
      "label": "qst106012_msg0005",
      "style": 84,
      "name": "이제 장치가 완전히 충전되는 걸\n기다리기만 하면 돼모"
    },
    {
      "$id": 6,
      "label": "qst106012_msg0006",
      "style": 84,
      "name": "드디어 하나가\n다시 태어나는 거네요!"
    },
    {
      "$id": 7,
      "label": "qst106012_msg0007",
      "style": 84,
      "name": "조금 긴장되는걸모…"
    },
    {
      "$id": 8,
      "label": "qst106012_msg0008",
      "style": 84,
      "name": "그래서 개조는 언제 시작해?"
    },
    {
      "$id": 9,
      "label": "qst106012_msg0009",
      "style": 84,
      "name": "모모모…\n충전이 완료되려면\n앞으로 1주일은 걸린단다모"
    },
    {
      "$id": 10,
      "label": "qst106012_msg0010",
      "style": 84,
      "name": "뭐어?!\n충전되는 데 그렇게 오래 걸려?"
    },
    {
      "$id": 11,
      "label": "qst106012_msg0011",
      "style": 84,
      "name": "당연하지모!\n하나를 개조하려면\n막대한 전력이 필요해모"
    },
    {
      "$id": 12,
      "label": "qst106012_msg0012",
      "style": 84,
      "name": "쉬운 일이 아니야모"
    },
    {
      "$id": 13,
      "label": "qst106012_msg0013",
      "style": 84,
      "name": "하지만 이대로 1주일이나 기다릴 순 없어요\n너무 두근거려서 진이 빠질 거예요"
    },
    {
      "$id": 14,
      "label": "qst106012_msg0014",
      "style": 84,
      "name": "충전 시간을 앞당길 방법은 없어?"
    },
    {
      "$id": 15,
      "label": "qst106012_msg0015",
      "style": 84,
      "name": "모모…"
    },
    {
      "$id": 16,
      "label": "qst106012_msg0016",
      "style": 84,
      "name": "거기 있는 충전 장치에\n에너지가 될 만한 소재를 쏟아부으면\n충전 시간을 앞당길 수 있지모"
    },
    {
      "$id": 17,
      "label": "qst106012_msg0017",
      "style": 84,
      "name": "E팩이 가장 효율이 높지만\n다른 소재여도 상관없어모"
    },
    {
      "$id": 18,
      "label": "qst106012_msg0018",
      "style": 84,
      "name": "용병단에 E팩을 모아 달라고 하는 게\n가장 손쉬운 방법이야모"
    },
    {
      "$id": 19,
      "label": "qst106012_msg0019",
      "style": 84,
      "name": "그렇구나\n그럼 해 볼까?"
    },
    {
      "$id": 20,
      "label": "qst106012_msg0020",
      "style": 84,
      "name": "하나, 곧 개조할 수 있을 테니까\n기다려 주세요"
    },
    {
      "$id": 21,
      "label": "qst106012_msg0021",
      "style": 84,
      "name": "알았어요모\n다들 잘 부탁드려요모"
    }
  ]
}