  "rows": [
    {
      "$id": 1,
      "label": "bf03420100_0010",
      "style": 82,
      "name": "할아범, 무슨 일이야?"
    },
    {
      "$id": 2,
      "label": "bf03420100_0020",
      "style": 82,
      "name": "이온이… 사라졌다\n아마 녀석들 짓일 테지"
    },
    {
      "$id": 3,
      "label": "bf03420100_0030",
      "style": 82,
      "name": "녀석들?"
    },
    {
      "$id": 4,
      "label": "bf03420100_0040",
      "style": 82,
      "name": "너희가 돌아간 뒤 메츠와…\n어떤 남자 하나가 날 찾아왔었다"
    },
    {
      "$id": 5,
      "label": "bf03420100_0050",
      "style": 82,
      "name": "메츠라고?"
    },
    {
      "$id": 6,
      "label": "bf03420100_0060",
      "style": 82,
      "name": "다른 남자는 누구야?"
    },
    {
      "$id": 7,
      "label": "bf03420100_0070",
      "style": 82,
      "name": "이름은 몰라\n붉은 안경을 낀 남자다"
    },
    {
      "$id": 8,
      "label": "bf03420100_0080",
      "style": 82,
      "name": "요시츠네구나\n하지만 그 녀석들이 무슨 목적으로 이온을?"
    },
    {
      "$id": 9,
      "label": "bf03420100_0090",
      "style": 82,
      "name": "…설마\n니아, 호무라는?"
    },
    {
      "$id": 10,
      "label": "bf03420100_0100",
      "style": 82,
      "name": "내, 내가 일어났을 땐 없었어"
    },
    {
      "$id": 11,
      "label": "bf03420100_0110",
      "style": 82,
      "name": "어? 아무도 못 봤어?"
    },
    {
      "$id": 12,
      "label": "bf03420100_0120",
      "style": 82,
      "name": "호무라, 설마 이온이 잡혀간 걸\n알고 있었던 거 아니야?"
    },
    {
      "$id": 13,
      "label": "bf03420100_0130",
      "style": 82,
      "name": "가능한 이야기다\n\"그 두 사람이라면\""
    },
    {
      "$id": 14,
      "label": "bf03420100_0140",
      "style": 82,
      "name": "바보 아냐?\n혼자서 구하러 간 거야?"
    },
    {
      "$id": 15,
      "label": "bf03420100_0150",
      "style": 82,
      "name": "하지만 어디로 간 거지?\n쪽지 같은 건 없었어?"
    },
    {
      "$id": 16,
      "label": "bf03420100_0160",
      "style": 82,
      "name": "없어, 있으면 봤겠지"
    },
    {
      "$id": 17,
      "label": "bf03420100_0170",
      "style": 82,
      "name": "장소는… 알 것 같구나\n아마 카람의 유적이겠지"
    },
    {
      "$id": 18,
      "label": "bf03420100_0180",
      "style": 82,
      "name": "카람의 유적?"
    },
    {
      "$id": 19,
      "label": "bf03420100_0190",
      "style": 82,
      "name": "500년 전 영웅 아델이 이끌었던\n저항군이 궐기한 땅"
    },
    {
      "$id": 20,
      "label": "bf03420100_0200",
      "style": 82,
      "name": "하늘의 성배가 깨어난 곳이다"
    },
    {
      "$id": 21,
      "label": "bf03420100_0210",
      "style": 82,
      "name": "[ML:undisp ]"
    },
    {
      "$id": 22,
      "label": "bf03420100_0220",
      "style": 82,
      "name": "메츠가 불렀을 테니\n하늘의 성배가 갈 곳이라면 거기밖에 없지"
    },
    {
      "$id": 23,
      "label": "bf03420100_0230",
      "style": 82,
      "name": "콜 씨\n카람의 유적은 어디야?"
    },
    {
      "$id": 24,
      "label": "bf03420100_0240",
      "style": 82,
      "name": "대계단을 올라가면 돼"
    },
    {
      "$id": 25,
      "label": "bf03420100_0250",
      "style": 82,
      "name": "반담 씨!"
    },
    {
      "$id": 26,
      "label": "bf03420100_0260",
      "style": 82,
      "name": "그래\n형씨, 이 할아범을 부탁한다!"
    },
    {
      "$id": 27,
      "label": "bf03420100_0270",
      "style": 82,
      "name": "아, 네!"
    },
    {
      "$id": 28,
      "label": "bf03420100_0280",
      "style": 82,
      "name": "서두르자, 바로 쫓아가면\n따라잡을 수 있을지도 몰라"
    }
  ]
}