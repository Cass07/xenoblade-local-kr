  "rows": [
    {
      "$id": 1,
      "label": "bf04040100_0010",
      "style": 82,
      "name": "아까는 죄송했어요\n히카리가 말이 심했죠"
    },
    {
      "$id": 2,
      "label": "bf04040100_0020",
      "style": 82,
      "name": "히카리는 렉스를 지키고 싶다는\n간절한 마음 하나로 나온 거예요"
    },
    {
      "$id": 3,
      "label": "bf04040100_0030",
      "style": 82,
      "name": "자신에게 건 봉인을 풀고\n하늘의 성배의 힘까지 쓰면서…"
    },
    {
      "$id": 4,
      "label": "bf04040100_0032",
      "style": 82,
      "name": "제가 쓰는 힘은\n그런 히카리의…"
    },
    {
      "$id": 5,
      "label": "bf04040100_0034",
      "style": 82,
      "name": "하늘의 성배의 잉여 에너지일 뿐이에요"
    },
    {
      "$id": 6,
      "label": "bf04040100_0040",
      "style": 82,
      "name": "신경 쓰지 마\n다 내가 힘이 부족해서 그런 거니까"
    },
    {
      "$id": 7,
      "label": "bf04040100_0050",
      "style": 82,
      "name": "그보다…"
    },
    {
      "$id": 8,
      "label": "bf04040100_0060",
      "style": 82,
      "name": "[ML:undisp ]"
    },
    {
      "$id": 9,
      "label": "bf04040100_0070",
      "style": 82,
      "name": "사실대로 말해 줘서 기뻐"
    },
    {
      "$id": 10,
      "label": "bf04040100_0080",
      "style": 82,
      "name": "그리고 고마워\n우리 모두를 지켜 줘서"
    },
    {
      "$id": 11,
      "label": "bf04040100_0090",
      "style": 82,
      "name": "지켰다고요? 제가?"
    },
    {
      "$id": 12,
      "label": "bf04040100_0100",
      "style": 82,
      "name": "호무라의 힘은 굉장해\n전설이 진짜라는 걸 실감했어"
    },
    {
      "$id": 13,
      "label": "bf04040100_0110",
      "style": 82,
      "name": "죄송해요… 전…"
    },
    {
      "$id": 14,
      "label": "bf04040100_0120",
      "style": 82,
      "name": "호무라는 언제나 사과만 하네"
    },
    {
      "$id": 15,
      "label": "bf04040100_0130",
      "style": 82,
      "name": "그건 히카리가 지닌…"
    },
    {
      "$id": 16,
      "label": "bf04040100_0135",
      "style": 82,
      "name": "성배의 힘이 마음에 걸려서 그런 거지?"
    },
    {
      "$id": 17,
      "label": "bf04040100_0140",
      "style": 82,
      "name": "난 호무라가 그런 식으로\n사과할 일이 없도록"
    },
    {
      "$id": 18,
      "label": "bf04040100_0150",
      "style": 82,
      "name": "슬픈 표정을 짓지 않게 해 주고 싶어"
    },
    {
      "$id": 19,
      "label": "bf04040100_0160",
      "style": 82,
      "name": "아니, 반드시 그렇게 해 줄 거야"
    },
    {
      "$id": 20,
      "label": "bf04040100_0170",
      "style": 82,
      "name": "렉스…"
    },
    {
      "$id": 21,
      "label": "bf04040100_0180",
      "style": 82,
      "name": "힘은 그걸 쓰는 사람의 마음에 달렸다…"
    },
    {
      "$id": 22,
      "label": "bf04040100_0185",
      "style": 82,
      "name": "반담 씨가 그랬어"
    },
    {
      "$id": 23,
      "label": "bf04040100_0190",
      "style": 82,
      "name": "호무라가 지닌 힘은 모두를 지키는 힘이야"
    },
    {
      "$id": 24,
      "label": "bf04040100_0195",
      "style": 82,
      "name": "호무라는 사람들의 미소를 되찾아 주지"
    },
    {
      "$id": 25,
      "label": "bf04040100_0200",
      "style": 82,
      "name": "난 호무라와 다른 사람들의\n미소를 지키는 드라이버가 되겠어"
    },
    {
      "$id": 26,
      "label": "bf04040100_0210",
      "style": 82,
      "name": "여자를 울리는 녀석은 최악의 남자고"
    },
    {
      "$id": 27,
      "label": "bf04040100_0215",
      "style": 82,
      "name": "여자를 웃게 하는 녀석은 최고의 남자다"
    },
    {
      "$id": 28,
      "label": "bf04040100_0220",
      "style": 82,
      "name": "샐비저 코드 3번이야"
    },
    {
      "$id": 29,
      "label": "bf04040100_0230",
      "style": 82,
      "name": "형님~\n하나 수리하는 것 좀 도와줘모~"
    },
    {
      "$id": 30,
      "label": "bf04040100_0240",
      "style": 82,
      "name": "알았어, 지금 갈게"
    },
    {
      "$id": 31,
      "label": "bf04040100_0250",
      "style": 82,
      "name": "아 참, 오늘 저녁밥은 뭐야?"
    },
    {
      "$id": 32,
      "label": "bf04040100_0260",
      "style": 82,
      "name": "미트볼 포토푀 어때요?"
    },
    {
      "$id": 33,
      "label": "bf04040100_0270",
      "style": 82,
      "name": "오오~ 그거 기대되는걸~"
    },
    {
      "$id": 34,
      "label": "bf04040100_0280",
      "style": 82,
      "name": "저게 바로 렉스란다\n슬픔을 미소로 바꿔 내일을 위한 힘으로 삼지"
    },
    {
      "$id": 35,
      "label": "bf04040100_0290",
      "style": 82,
      "name": "…아델과 닮은 것 같지 않으냐?"
    }
  ]
