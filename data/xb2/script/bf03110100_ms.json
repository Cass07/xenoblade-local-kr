  "rows": [
    {
      "$id": 1,
      "label": "bf03110100_0010",
      "style": 82,
      "name": "…그래"
    },
    {
      "$id": 2,
      "label": "bf03110100_0020",
      "style": 82,
      "name": "낙원에 가려고 세계수에 접근했다가\n그대로 잡아먹혔다는 거로군"
    },
    {
      "$id": 3,
      "label": "bf03110100_0030",
      "style": 82,
      "name": "그거 딱하게 됐구나"
    },
    {
      "$id": 4,
      "label": "bf03110100_0040",
      "style": 82,
      "name": "반담 씨는 왜 그런 곳에?"
    },
    {
      "$id": 5,
      "label": "bf03110100_0050",
      "style": 82,
      "name": "이 아르스는 말이야\n정기적으로 바깥에 있는 걸 삼키지"
    },
    {
      "$id": 6,
      "label": "bf03110100_0060",
      "style": 82,
      "name": "그래서 마을에서 쓸 만한 물자가\n있을까 싶어서 가 봤다가"
    },
    {
      "$id": 7,
      "label": "bf03110100_0070",
      "style": 82,
      "name": "너희와 맞닥뜨린 거다"
    },
    {
      "$id": 8,
      "label": "bf03110100_0080",
      "style": 82,
      "name": "그랬구나"
    },
    {
      "$id": 9,
      "label": "bf03110100_0090",
      "style": 82,
      "name": "도착했다\n여기가 우리 마을인 흐레스벨그다"
    },
    {
      "$id": 10,
      "label": "bf03110100_0100",
      "style": 82,
      "name": "이 마을에는 아이가 많네요"
    },
    {
      "$id": 11,
      "label": "bf03110100_0110",
      "style": 82,
      "name": "드라이버와 블레이드도 많아모"
    },
    {
      "$id": 12,
      "label": "bf03110100_0120",
      "style": 82,
      "name": "여기서는 세계 여기저기로\n드라이버와 블레이드를 파견하고 있다"
    },
    {
      "$id": 13,
      "label": "bf03110100_0130",
      "style": 82,
      "name": "아이들은 그때 데리고 온 전쟁고아지"
    },
    {
      "$id": 14,
      "label": "bf03110100_0140",
      "style": 82,
      "name": "부모를 잃은 아이들을\n거둬들인 거군요"
    },
    {
      "$id": 15,
      "label": "bf03110100_0150",
      "style": 82,
      "name": "어른이 될 때까지만 돌보는 검다만"
    },
    {
      "$id": 16,
      "label": "bf03110100_0160",
      "style": 82,
      "name": "독립한 다음에는\n드라이버가 되는 녀석도 있고"
    },
    {
      "$id": 17,
      "label": "bf03110100_0170",
      "style": 82,
      "name": "옷을 만들어 파는 녀석도 있어"
    },
    {
      "$id": 18,
      "label": "bf03110100_0180",
      "style": 82,
      "name": "자기 인생을 어떻게 살지는\n아이들의 자유 의지에 맡기고 있지"
    },
    {
      "$id": 19,
      "label": "bf03110100_0190",
      "style": 82,
      "name": "뭐, \"정의의 용병단\"이라고 불러 주십쇼"
    },
    {
      "$id": 20,
      "label": "bf03110100_0200",
      "style": 82,
      "name": "자선가네"
    },
    {
      "$id": 21,
      "label": "bf03110100_0210",
      "style": 82,
      "name": "괜찮은데?\n그런 표현 나쁘지 않아"
    },
    {
      "$id": 22,
      "label": "bf03110100_0220",
      "style": 82,
      "name": "그나저나 배가 고프군\n빨리 가서 밥이나 먹자고"
    }
  ]
}