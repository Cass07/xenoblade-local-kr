    {
      "name": "style",
      "type": 2
    },
    {
      "name": "name",
      "type": 7
    }
  ],
  "rows": [
    {
      "$id": 1,
      "label": "bf01140100_0010",
      "style": 82,
      "name": "[ML:undisp ]"
    },
    {
      "$id": 2,
      "label": "bf01140100_0020",
      "style": 82,
      "name": "항구에 있던 검은 배잖아…\n따라오고 있는 건가…?"
    },
    {
      "$id": 3,
      "label": "bf01140100_0030",
      "style": 82,
      "name": "뭐야, 엄청 춥네"
    },
    {
      "$id": 4,
      "label": "bf01140100_0040",
      "style": 82,
      "name": "너…"
    },
    {
      "$id": 5,
      "label": "bf01140100_0050",
      "style": 82,
      "name": "내 이름은 너가 아니고 니아야"
    },
    {
      "$id": 6,
      "label": "bf01140100_0060",
      "style": 82,
      "name": "아래는 지금 난장판이니까\n잠깐만 나랑 어울려 줘"
    },
    {
      "$id": 7,
      "label": "bf01140100_0070",
      "style": 82,
      "name": "니아는 그런 거 싫어해?"
    },
    {
      "$id": 8,
      "label": "bf01140100_0080",
      "style": 82,
      "name": "왁자지껄한 건 싫지 않아\n하지만 내가 끼는 건 싫어"
    },
    {
      "$id": 9,
      "label": "bf01140100_0090",
      "style": 82,
      "name": "흐음\n그럼 샐비저는 못 되겠네"
    },
    {
      "$id": 10,
      "label": "bf01140100_0100",
      "style": 82,
      "name": "왜?"
    },
    {
      "$id": 11,
      "label": "bf01140100_0110",
      "style": 82,
      "name": "배에 취하지 말고 흥에 취해라\n샐비저끼리 쓰는 코드 같은 거야"
    },
    {
      "$id": 12,
      "label": "bf01140100_0120",
      "style": 82,
      "name": "시시해\n전직할 생각조차 안 들어"
    },
    {
      "$id": 13,
      "label": "bf01140100_0130",
      "style": 82,
      "name": "뭐, 그렇겠지"
    },
    {
      "$id": 14,
      "label": "bf01140100_0140",
      "style": 82,
      "name": "드라이버면 샐비저보다\n더 많이 벌 수 있잖아?"
    },
    {
      "$id": 15,
      "label": "bf01140100_0150",
      "style": 82,
      "name": "너는…"
    },
    {
      "$id": 16,
      "label": "bf01140100_0160",
      "style": 82,
      "name": "렉스라고 불러"
    },
    {
      "$id": 17,
      "label": "bf01140100_0170",
      "style": 82,
      "name": "…렉스는\n왜 샐비저 일을 하는 거야?"
    },
    {
      "$id": 18,
      "label": "bf01140100_0180",
      "style": 82,
      "name": "저거 때문에"
    },
    {
      "$id": 19,
      "label": "bf01140100_0190",
      "style": 82,
      "name": "세계수…\n저게 뭘 어쨌는데?"
    },
    {
      "$id": 20,
      "label": "bf01140100_0200",
      "style": 82,
      "name": "샐비저를 하다 보면 말이야\n다양한 것들을 줍게 돼"
    },
    {
      "$id": 21,
      "label": "bf01140100_0210",
      "style": 82,
      "name": "누군가가 떨어뜨린 것, 누군가가 남긴 것…\n전부 다 한 번은 죽은 것들이지"
    },
    {
      "$id": 22,
      "label": "bf01140100_0220",
      "style": 82,
      "name": "요즘 아르스 수도 눈에 띄게 줄었잖아?"
    },
    {
      "$id": 23,
      "label": "bf01140100_0230",
      "style": 82,
      "name": "얼마 전에도\n대형 아르스 하나가 가라앉는 걸 봤어"
    },
    {
      "$id": 24,
      "label": "bf01140100_0240",
      "style": 82,
      "name": "수많은 동물들을 태운 채로…"
    },
    {
      "$id": 25,
      "label": "bf01140100_0250",
      "style": 82,
      "name": "우리가 살 수 있는 장소는\n계속해서 줄어들고 있어"
    },
    {
      "$id": 26,
      "label": "bf01140100_0260",
      "style": 82,
      "name": "우리에게도 언젠가\n한 번은 죽음이 찾아올 거야"
    },
    {
      "$id": 27,
      "label": "bf01140100_0270",
      "style": 82,
      "name": "하지만…\n저기에는 낙원이 있어"
    },
    {
      "$id": 28,
      "label": "bf01140100_0280",
      "style": 82,
      "name": "[ML:undisp ]"
    },
    {
      "$id": 29,
      "label": "bf01140100_0290",
      "style": 82,
      "name": "너 진심이야?\n낙원 전설이라니"
    },
    {
      "$id": 30,
      "label": "bf01140100_0300",
      "style": 82,
      "name": "그런 게 어딨어\n저건 그냥 커다란 나무일 뿐이야"
    },
    {
      "$id": 31,
      "label": "bf01140100_0310",
      "style": 82,
      "name": "그래서 샐비저가 된 거야?"
    },
    {
      "$id": 32,
      "label": "bf01140100_0320",
      "style": 82,
      "name": "저기로 갈 수 있는 도구든 뭐든\n찾을 수 있을지도 모르잖아?"
    },
    {
      "$id": 33,
      "label": "bf01140100_0330",
      "style": 82,
      "name": "그게 뭐 어때서"
    },
    {
      "$id": 34,
      "label": "bf01140100_0340",
      "style": 82,
      "name": "그래도 그렇지"
    },
    {
      "$id": 35,
      "label": "bf01140100_0350",
      "style": 82,
      "name": "얼마 남지 않은 아르스나\n자원 때문에 싸울 필요도 없고"
    },
    {
      "$id": 36,
      "label": "bf01140100_0360",
      "style": 82,
      "name": "대지가 가라앉을 거라는\n불안에 떨 필요도 없이"
    },
    {
      "$id": 37,
      "label": "bf01140100_0370",
      "style": 82,
      "name": "모두가 안심하고 살아갈 수 있는 곳"
    },
    {
      "$id": 38,
      "label": "bf01140100_0380",
      "style": 82,
      "name": "그런 곳이 있으면 좋겠다는\n꿈 정도는 꿔도 되잖아?"
    },
    {
      "$id": 39,
      "label": "bf01140100_0390",
      "style": 82,
      "name": "꿈꾸는 거야 네 마음이지만…"
    },
    {
      "$id": 40,
      "label": "bf01140100_0400",
      "style": 82,
      "name": "흐응, 모두가 말이지…"
    },
    {
      "$id": 41,
      "label": "bf01140100_0410",
      "style": 82,
      "name": "뭐야?"
    },
    {
      "$id": 42,
      "label": "bf01140100_0420",
      "style": 82,
      "name": "인간은 좀 더 제멋대로 구는\n생물이라고 생각했는데 말이야"
    },
    {
      "$id": 43,
      "label": "bf01140100_0430",
      "style": 82,
      "name": "[ML:undisp ]"
    },
    {
      "$id": 44,
      "label": "bf01140100_0440",
      "style": 82,
      "name": "렉스, 부모님은?"
    },
    {
      "$id": 45,
      "label": "bf01140100_0450",
      "style": 82,
      "name": "안 계셔"
    },
    {
      "$id": 46,
      "label": "bf01140100_0460",
      "style": 82,
      "name": "내가 어렸을 때 돌아가셨다고\n할아버지가 그랬어"
    },
    {
      "$id": 47,
      "label": "bf01140100_0470",
      "style": 82,
      "name": "할아버지라면\n아츠를 가르쳐 줬다는 그 할아버지?"
    },
    {
      "$id": 48,
      "label": "bf01140100_0480",
      "style": 82,
      "name": "응, 날 계속 키워 주셨지"
    },
    {
      "$id": 49,
      "label": "bf01140100_0490",
      "style": 82,
      "name": "인간은 아니지만"
    },
    {
      "$id": 50,
      "label": "bf01140100_0500",
      "style": 82,
      "name": "인간이 아니라니…"
    },
    {
      "$id": 51,
      "label": "bf01140100_0510",
      "style": 82,
      "name": "잘 이해는 안 되지만\n그 할아버지한테 감사하도록 해"
    },
    {
      "$id": 52,
      "label": "bf01140100_0520",
      "style": 82,
      "name": "[ML:undisp ]"
    },
    {
      "$id": 53,
      "label": "bf01140100_0530",
      "style": 82,
      "name": "너, 나쁘지 않네"
    },
    {
      "$id": 54,
      "label": "bf01140100_0540",
      "style": 82,
      "name": "나랑 같아…"
    },
    {
      "$id": 55,
      "label": "bf01140100_0550",
      "style": 82,
      "name": "[ML:undisp ]"
    }
  ]
}