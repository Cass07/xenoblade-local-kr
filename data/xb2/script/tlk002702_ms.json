  "rows": [
    {
      "$id": 1,
      "label": "qst002703_msg0001",
      "style": 84,
      "name": "…하아"
    },
    {
      "$id": 2,
      "label": "qst002703_msg0002",
      "style": 84,
      "name": "…어머\n너 언제 왔어?"
    },
    {
      "$id": 3,
      "label": "qst002703_msg0003",
      "style": 84,
      "name": "…저기\n내 말 좀 들어 줄래?"
    },
    {
      "$id": 4,
      "label": "qst002703_msg0004",
      "style": 84,
      "name": "고마워…"
    },
    {
      "$id": 5,
      "label": "qst002703_msg0005",
      "style": 84,
      "name": "그러고 난 뒤에…\n결국 난 실패했어…"
    },
    {
      "$id": 6,
      "label": "qst002703_msg0006",
      "style": 84,
      "name": "반하는 약을 만드는 건 성공했거든?\n그 사람한테 약도 먹였고…"
    },
    {
      "$id": 7,
      "label": "qst002703_msg0007",
      "style": 84,
      "name": "…그런데\n타이밍이 안 좋았어"
    },
    {
      "$id": 8,
      "label": "qst002703_msg0008",
      "style": 84,
      "name": "때마침 그 사람 옆에 온 여자랑\n눈이 마주치고 만 거야…"
    },
    {
      "$id": 9,
      "label": "qst002703_msg0009",
      "style": 84,
      "name": "…반하는 약을 마신 그 사람은\n여자를 힘껏 안아 올리더니\n어디론가 달려가 버렸어…"
    },
    {
      "$id": 10,
      "label": "qst002703_msg0010",
      "style": 84,
      "name": "내가 만든 반하는 약은 효과가 강하거든\n그 사람은 이제 나를 거들떠보지도 않겠지"
    },
    {
      "$id": 11,
      "label": "qst002703_msg0011",
      "style": 84,
      "name": "…내가 오랜 세월 동안\n연구해 온 건 대체 뭐였을까…"
    },
    {
      "$id": 12,
      "label": "qst002703_msg0012",
      "style": 84,
      "name": "어머, 걱정해 주는 거니?\n…후훗, 착하네"
    },
    {
      "$id": 13,
      "label": "qst002703_msg0013",
      "style": 84,
      "name": "…그래"
    },
    {
      "$id": 14,
      "label": "qst002703_msg0014",
      "style": 84,
      "name": "그때는 널 이용하려고\n한 말이었지만…"
    },
    {
      "$id": 15,
      "label": "qst002703_msg0015",
      "style": 84,
      "name": "모든 사람을 구하는 약이나\n진짜로 만들어 볼까?"
    },
    {
      "$id": 16,
      "label": "qst002703_msg0016",
      "style": 84,
      "name": "…후훗"
    },
    {
      "$id": 17,
      "label": "qst002703_msg0017",
      "style": 84,
      "name": "내 얘기를 들어 줘서 고마워\n덕분에 마음이 한결 편해졌어"
    },
    {
      "$id": 18,
      "label": "qst002703_msg0018",
      "style": 84,
      "name": "그럼 난 진정한 의미에서 모든 사람을\n구할 수 있는 약을 연구할게"
    },
    {
      "$id": 19,
      "label": "qst002703_msg0019",
      "style": 84,
      "name": "너도 이제 나 같은 사람한테\n속으면 안 된다?"
    }
  ]
}