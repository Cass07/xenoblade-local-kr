    {
      "$id": 1,
      "label": "bf07370100_0010",
      "style": 82,
      "name": "세상 사람 모두가 적으로 보여서…"
    },
    {
      "$id": 2,
      "label": "bf07370100_0020",
      "style": 82,
      "name": "우리는 계속 떠돌 수밖에 없었어"
    },
    {
      "$id": 3,
      "label": "bf07370100_0030",
      "style": 82,
      "name": "하루하루 살아가는 것도 벅차서"
    },
    {
      "$id": 4,
      "label": "bf07370100_0040",
      "style": 82,
      "name": "내일 내가 살아남을 수 있을지도\n확신할 수 없었어"
    },
    {
      "$id": 5,
      "label": "bf07370100_0050",
      "style": 82,
      "name": "[ML:undisp ]"
    },
    {
      "$id": 6,
      "label": "bf07370100_0060",
      "style": 82,
      "name": "그때 만난 게 신이었어"
    },
    {
      "$id": 7,
      "label": "bf07370100_0070",
      "style": 82,
      "name": "신이 나를 거두어 준 거야"
    },
    {
      "$id": 8,
      "label": "bf07370100_0080",
      "style": 82,
      "name": "그래서 신이랑 함께…"
    },
    {
      "$id": 9,
      "label": "bf07370100_0090",
      "style": 82,
      "name": "나는 신에게서 미래를 봤어"
    },
    {
      "$id": 10,
      "label": "bf07370100_0100",
      "style": 82,
      "name": "우리에겐 미래가 없었으니까\n함께 미래를 찾으려 했던 거야"
    },
    {
      "$id": 11,
      "label": "bf07370100_0110",
      "style": 82,
      "name": "하지만…"
    },
    {
      "$id": 12,
      "label": "bf07370100_0120",
      "style": 82,
      "name": "안 어울려"
    },
    {
      "$id": 13,
      "label": "bf07370100_0130",
      "style": 82,
      "name": "뭐?"
    },
    {
      "$id": 14,
      "label": "bf07370100_0140",
      "style": 82,
      "name": "니아가 그런 걸로 고민하는 성격이었어?"
    },
    {
      "$id": 15,
      "label": "bf07370100_0150",
      "style": 82,
      "name": "딱히 고민한 건…"
    },
    {
      "$id": 16,
      "label": "bf07370100_0160",
      "style": 82,
      "name": "그럼 옛날이야기는 왜 했어?"
    },
    {
      "$id": 17,
      "label": "bf07370100_0170",
      "style": 82,
      "name": "그건…\n그냥 어쩌다 보니까…"
    },
    {
      "$id": 18,
      "label": "bf07370100_0180",
      "style": 82,
      "name": "지금 니아를 속박하는 게\n뭔지는 모르겠지만 말이야"
    },
    {
      "$id": 19,
      "label": "bf07370100_0190",
      "style": 82,
      "name": "좀 자유롭게 살아도 되지 않을까?"
    },
    {
      "$id": 20,
      "label": "bf07370100_0200",
      "style": 82,
      "name": "자유…"
    },
    {
      "$id": 21,
      "label": "bf07370100_0210",
      "style": 82,
      "name": "자유롭게 자기 뜻대로"
    },
    {
      "$id": 22,
      "label": "bf07370100_0220",
      "style": 82,
      "name": "한번 결심하면 주먹질을 해서라도\n자기 생각을 밀어붙이는 녀석"
    },
    {
      "$id": 23,
      "label": "bf07370100_0230",
      "style": 82,
      "name": "그게 니아의 본모습이잖아?"
    },
    {
      "$id": 24,
      "label": "bf07370100_0240",
      "style": 82,
      "name": "뭐야, 그게…"
    },
    {
      "$id": 25,
      "label": "bf07370100_0250",
      "style": 82,
      "name": "그건 그냥 폭력적이란 거잖아"
    },
    {
      "$id": 26,
      "label": "bf07370100_0260",
      "style": 82,
      "name": "잘 아네"
    },
    {
      "$id": 27,
      "label": "bf07370100_0270",
      "style": 82,
      "name": "뭐가 어째?"
    },
    {
      "$id": 28,
      "label": "bf07370100_0280",
      "style": 82,
      "name": "그래, 바로 그거야\n평소의 니아로 돌아왔네"
    },
    {
      "$id": 29,
      "label": "bf07370100_0290",
      "style": 82,
      "name": "평소의… 나?"
    },
    {
      "$id": 30,
      "label": "bf07370100_0300",
      "style": 82,
      "name": "조금만 더 힘내자"
    },
    {
      "$id": 31,
      "label": "bf07370100_0310",
      "style": 82,
      "name": "렉스…"
    }
  ]
