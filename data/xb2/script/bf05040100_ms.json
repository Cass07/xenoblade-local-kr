  "rows": [
    {
      "$id": 1,
      "label": "bf05040100_0010",
      "style": 82,
      "name": "카스미…"
    },
    {
      "$id": 2,
      "label": "bf05040100_0020",
      "style": 82,
      "name": "히카리 님?\n이런 늦은 시간에 어쩐 일이세요?"
    },
    {
      "$id": 3,
      "label": "bf05040100_0030",
      "style": 82,
      "name": "이맘때 운해의 밤은 무척 쌀쌀해요\n감기에 걸리실지도 모른답니다?"
    },
    {
      "$id": 4,
      "label": "bf05040100_0040",
      "style": 82,
      "name": "난 블레이드야, 감기엔 안 걸려"
    },
    {
      "$id": 5,
      "label": "bf05040100_0050",
      "style": 82,
      "name": "그렇네요, 제가 실례를 범했군요"
    },
    {
      "$id": 6,
      "label": "bf05040100_0060",
      "style": 82,
      "name": "카스미… 기억 못 하는구나"
    },
    {
      "$id": 7,
      "label": "bf05040100_0070",
      "style": 82,
      "name": "…왜 저를 카스미라고 부르시죠?"
    },
    {
      "$id": 8,
      "label": "bf05040100_0080",
      "style": 82,
      "name": "저는 판 레 노른\n마르베니 성하의 블레이드입니다"
    },
    {
      "$id": 9,
      "label": "bf05040100_0090",
      "style": 82,
      "name": "그래… 마르베니 말이지"
    },
    {
      "$id": 10,
      "label": "bf05040100_0100",
      "style": 82,
      "name": "미안, 잊어버려\n아는 사람하고 많이 닮아서 그만"
    },
    {
      "$id": 11,
      "label": "bf05040100_0110",
      "style": 82,
      "name": "확실히 밖이 춥긴 하네\n네 충고대로 방으로 돌아갈게"
    },
    {
      "$id": 12,
      "label": "bf05040100_0120",
      "style": 82,
      "name": "네, 안녕히 주무세요, 히카리 님"
    },
    {
      "$id": 13,
      "label": "bf05040100_0130",
      "style": 82,
      "name": "잘 자, 판"
    },
    {
      "$id": 14,
      "label": "bf05040100_0140",
      "style": 82,
      "name": "『히카리\n저 코어 크리스털의 형태는…』"
    },
    {
      "$id": 15,
      "label": "bf05040100_0150",
      "style": 82,
      "name": "『나도 알아\n단순한 동조가 아니야』"
    },
    {
      "$id": 16,
      "label": "bf05040100_0160",
      "style": 82,
      "name": "『우리나 렉스와도 달라』"
    },
    {
      "$id": 17,
      "label": "bf05040100_0170",
      "style": 82,
      "name": "『물어볼 게 하나 더 늘었네』"
    },
    {
      "$id": 18,
      "label": "bf05040100_0180",
      "style": 82,
      "name": "『미안, 잠시 생각 좀 하고 싶으니까 교대해 줘』"
    },
    {
      "$id": 19,
      "label": "bf05040100_0190",
      "style": 82,
      "name": "『응, 알았어』"
    }
  ]
}