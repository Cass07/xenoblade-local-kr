  "rows": [
    {
      "$id": 1,
      "label": "qst102714_msg001",
      "style": 62,
      "name": "どうしたイオン？(仮)"
    },
    {
      "$id": 2,
      "label": "qst102714_msg002",
      "style": 62,
      "name": "おじいちゃん　これあげる(仮)"
    },
    {
      "$id": 3,
      "label": "qst102714_msg003",
      "style": 62,
      "name": "ありがとう(仮)"
    },
    {
      "$id": 4,
      "label": "qst102714_msg0001",
      "style": 84,
      "name": "약…\n고마워!"
    },
    {
      "$id": 5,
      "label": "qst102714_msg0002",
      "style": 84,
      "name": "그럼\n할아버지에게…"
    },
    {
      "$id": 6,
      "label": "qst102714_msg0050",
      "style": 84,
      "name": "콜록, 콜록"
    },
    {
      "$id": 7,
      "label": "qst102714_msg0051",
      "style": 84,
      "name": "…미안하군\n무슨 일이지? 다 모여서"
    },
    {
      "$id": 8,
      "label": "qst102714_msg0052",
      "style": 84,
      "name": "날이 갈수록 약해지는 게 느껴져…"
    },
    {
      "$id": 9,
      "label": "qst102714_msg0053",
      "style": 84,
      "name": "마르베니가 죽은 영향…\n그럴지도 모르지"
    },
    {
      "$id": 10,
      "label": "qst102714_msg0054",
      "style": 84,
      "name": "반담의 이야기를\n마지막까지 쓸 수 있을지…"
    },
    {
      "$id": 11,
      "label": "qst102714_msg0055",
      "style": 84,
      "name": "있잖아\n모두가 할아버지에게\n약을 가져다줬어"
    },
    {
      "$id": 12,
      "label": "qst102714_msg0056",
      "style": 84,
      "name": "그리고 히카리가\n에테르의 흐름을 고쳐 준대…"
    },
    {
      "$id": 13,
      "label": "qst102714_msg0057",
      "style": 84,
      "name": "미노치, 각오는 됐어?\n약을 먹으면 시작할게"
    },
    {
      "$id": 14,
      "label": "qst102714_msg0058",
      "style": 84,
      "name": "히카리…\n그렇군, 드디어…"
    },
    {
      "$id": 15,
      "label": "qst102714_msg0059",
      "style": 84,
      "name": "이온…\n약을 줘"
    },
    {
      "$id": 16,
      "label": "qst102714_msg0060",
      "style": 84,
      "name": "응!"
    },
    {
      "$id": 17,
      "label": "qst102714_msg0061",
      "style": 84,
      "name": "자, 끝났어"
    },
    {
      "$id": 18,
      "label": "qst102714_msg0101",
      "style": 84,
      "name": "할아버지…\n어때?"
    },
    {
      "$id": 19,
      "label": "qst102714_msg0102",
      "style": 84,
      "name": "몸이 따뜻해지고…\n호흡이 편해진 것 같군"
    },
    {
      "$id": 20,
      "label": "qst102714_msg0103",
      "style": 84,
      "name": "정말?\n다행이다…"
    },
    {
      "$id": 21,
      "label": "qst102714_msg0104",
      "style": 84,
      "name": "고맙다…"
    },
    {
      "$id": 22,
      "label": "qst102714_msg0105",
      "style": 84,
      "name": "죽을 운명에서는\n결코 도망칠 수 없겠지만"
    },
    {
      "$id": 23,
      "label": "qst102714_msg0106",
      "style": 84,
      "name": "집필 중인 이야기가\n공연되는 날까지는\n버틸 거다"
    },
    {
      "$id": 24,
      "label": "qst102714_msg0107",
      "style": 84,
      "name": "게다가…\n반담의 이야기에는\n너희를 빼놓을 수 없지"
    },
    {
      "$id": 25,
      "label": "qst102714_msg0108",
      "style": 84,
      "name": "목적이 달성되는 걸\n지켜봐야겠지"
    },
    {
      "$id": 26,
      "label": "qst102714_msg0109",
      "style": 84,
      "name": "답례라긴 좀 그렇지만\n이걸…"
    },
    {
      "$id": 27,
      "label": "qst102714_msg0110",
      "style": 84,
      "name": "나는 못 쓰는 거지만\n너희라면"
    },
    {
      "$id": 28,
      "label": "qst102714_msg0111",
      "style": 84,
      "name": "모두 고마워요!"
    },
    {
      "$id": 29,
      "label": "qst102714_msg0112",
      "style": 84,
      "name": "네!"
    }
  ]
}