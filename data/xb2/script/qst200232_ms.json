    {
      "$id": 25,
      "label": "qst200232_msg0025",
      "style": 84,
      "name": "정의라고 하면 말은 쉬운데\n그걸 실현하는 건 굉장히 어렵구나"
    },
    {
      "$id": 26,
      "label": "qst200232_msg0026",
      "style": 84,
      "name": "정의…\n무거운 단어군"
