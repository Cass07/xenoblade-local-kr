  "rows": [
    {
      "$id": 1,
      "label": "bf08030100_0010",
      "style": 82,
      "name": "그래…\n아델을 만났구나"
    },
    {
      "$id": 2,
      "label": "bf08030100_0020",
      "style": 82,
      "name": "응, 아델 씨가 남긴\n기억의 단편 같은 거였지만"
    },
    {
      "$id": 3,
      "label": "bf08030100_0030",
      "style": 82,
      "name": "아델 씨가 그랬어"
    },
    {
      "$id": 4,
      "label": "bf08030100_0040",
      "style": 82,
      "name": "진정으로 지키고 싶은 것을 위해 싸우라고…"
    },
    {
      "$id": 5,
      "label": "bf08030100_0050",
      "style": 82,
      "name": "그래서 결심했어\n히카리와 호무라를 위해 싸우겠다고"
    },
    {
      "$id": 6,
      "label": "bf08030100_0060",
      "style": 82,
      "name": "아직 좀 부족해 보일지도 모르지만…"
    },
    {
      "$id": 7,
      "label": "bf08030100_0070",
      "style": 82,
      "name": "그렇지 않아"
    },
    {
      "$id": 8,
      "label": "bf08030100_0080",
      "style": 82,
      "name": "나도 결심했어\n이제… 두렵지 않아"
    },
    {
      "$id": 9,
      "label": "bf08030100_0090",
      "style": 82,
      "name": "그나저나 아까 그 모습 말인데…"
    },
    {
      "$id": 10,
      "label": "bf08030100_0100",
      "style": 82,
      "name": "그게 우리의 본래 모습"
    },
    {
      "$id": 11,
      "label": "bf08030100_0110",
      "style": 82,
      "name": "검이 변했었잖아?"
    },
    {
      "$id": 12,
      "label": "bf08030100_0120",
      "style": 82,
      "name": "아, 응"
    },
    {
      "$id": 13,
      "label": "bf08030100_0130",
      "style": 82,
      "name": "단순히 힘만 강해진 게 아냐"
    },
    {
      "$id": 14,
      "label": "bf08030100_0140",
      "style": 82,
      "name": "디바이스도 자유로이 다룰 수 있으니까\n조심해"
    },
    {
      "$id": 15,
      "label": "bf08030100_0150",
      "style": 82,
      "name": "조심?"
    },
    {
      "$id": 16,
      "label": "bf08030100_0160",
      "style": 82,
      "name": "우쭐대지 말라는 거야"
    },
    {
      "$id": 17,
      "label": "bf08030100_0170",
      "style": 82,
      "name": "아, 알았어…"
    },
    {
      "$id": 18,
      "label": "bf08030100_0180",
      "style": 82,
      "name": "그런데, 그때는 어느 쪽이야?"
    },
    {
      "$id": 19,
      "label": "bf08030100_0190",
      "style": 82,
      "name": "어느 쪽?"
    },
    {
      "$id": 20,
      "label": "bf08030100_0200",
      "style": 82,
      "name": "안에"
    },
    {
      "$id": 21,
      "label": "bf08030100_0210",
      "style": 82,
      "name": "어느 쪽도 아냐\n둘이서 하나지"
    },
    {
      "$id": 22,
      "label": "bf08030100_0220",
      "style": 82,
      "name": "카페오레 같은 거야?"
    },
    {
      "$id": 23,
      "label": "bf08030100_0230",
      "style": 82,
      "name": "무슨 비유가 그래?"
    },
    {
      "$id": 24,
      "label": "bf08030100_0240",
      "style": 82,
      "name": "뭐, 대충 맞긴 하네"
    },
    {
      "$id": 25,
      "label": "bf08030100_0250",
      "style": 82,
      "name": "이름은?\n호무리? 히카라?"
    },
    {
      "$id": 26,
      "label": "bf08030100_0260",
      "style": 82,
      "name": "뭐야, 그게?"
    },
    {
      "$id": 27,
      "label": "bf08030100_0270",
      "style": 82,
      "name": "호무라든 히카리든\n부르고 싶은 대로 불러"
    },
    {
      "$id": 28,
      "label": "bf08030100_0280",
      "style": 82,
      "name": "알았어, 그렇게 할게"
    },
    {
      "$id": 29,
      "label": "bf08030100_0290",
      "style": 82,
      "name": "그런데, 언제든 될 수 있는 거야?"
    },
    {
      "$id": 30,
      "label": "bf08030100_0300",
      "style": 82,
      "name": "필요하다면"
    },
    {
      "$id": 31,
      "label": "bf08030100_0310",
      "style": 82,
      "name": "그렇구나…"
    },
    {
      "$id": 32,
      "label": "bf08030100_0320",
      "style": 82,
      "name": "뭐야, 갑자기 왜 웃어?"
    },
    {
      "$id": 33,
      "label": "bf08030100_0330",
      "style": 82,
      "name": "아니…"
    },
    {
      "$id": 34,
      "label": "bf08030100_0340",
      "style": 82,
      "name": "나, 그 헤어스타일 꽤 맘에 들었는데…"
    },
    {
      "$id": 35,
      "label": "bf08030100_0350",
      "style": 82,
      "name": "바, 바보\n뜬금없이 뭐야"
    },
    {
      "$id": 36,
      "label": "bf08030100_0360",
      "style": 82,
      "name": "이 소리는…"
    },
    {
      "$id": 37,
      "label": "bf08030100_0370",
      "style": 82,
      "name": "동료들일 거야\n서두르자"
    },
    {
      "$id": 38,
      "label": "bf08030100_0380",
      "style": 82,
      "name": "응"
    }
  ]
}