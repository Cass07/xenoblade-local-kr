  "rows": [
    {
      "$id": 1,
      "label": "bf06140100_0010",
      "style": 82,
      "name": "카스미를 동조시킨 이유가 뭐야?"
    },
    {
      "$id": 2,
      "label": "bf06140100_0020",
      "style": 82,
      "name": "그녀의 힘이 필요했기 때문이다"
    },
    {
      "$id": 3,
      "label": "bf06140100_0030",
      "style": 82,
      "name": "이곳 아케디아는 지금까지\n몇 번이나 이라의 습격을 받았지"
    },
    {
      "$id": 4,
      "label": "bf06140100_0040",
      "style": 82,
      "name": "그들을 물리치려면 그 힘이 꼭 필요했다"
    },
    {
      "$id": 5,
      "label": "bf06140100_0050",
      "style": 82,
      "name": "그래?"
    },
    {
      "$id": 6,
      "label": "bf06140100_0060",
      "style": 82,
      "name": "그런 것치고 당신에겐\n다른 블레이드가 없는 것 같은데?"
    },
    {
      "$id": 7,
      "label": "bf06140100_0070",
      "style": 82,
      "name": "나는 드라이버지만 전사가 아니다"
    },
    {
      "$id": 8,
      "label": "bf06140100_0080",
      "style": 82,
      "name": "그리고 이곳의 승병은 믿음직스럽거든\n전투는 병사에게 맡겨야 하는 법이지"
    },
    {
      "$id": 9,
      "label": "bf06140100_0090",
      "style": 82,
      "name": "그래… 그럼 다행이지만"
    },
    {
      "$id": 10,
      "label": "bf06140100_0100",
      "style": 82,
      "name": "왜 그들은 망국인 이라의 이름을 사용하는 거지?"
    },
    {
      "$id": 11,
      "label": "bf06140100_0110",
      "style": 82,
      "name": "신은 이라에 소속되어 있던 블레이드다"
    },
    {
      "$id": 12,
      "label": "bf06140100_0120",
      "style": 82,
      "name": "향수나 충성…\n아마 그런 것 때문이겠지"
    },
    {
      "$id": 13,
      "label": "bf06140100_0130",
      "style": 82,
      "name": "정말 그것뿐일까?"
    },
    {
      "$id": 14,
      "label": "bf06140100_0140",
      "style": 82,
      "name": "무슨 뜻이지?"
    },
    {
      "$id": 15,
      "label": "bf06140100_0150",
      "style": 82,
      "name": "난 내가 잠든 후에 일어난 역사를 몰라"
    },
    {
      "$id": 16,
      "label": "bf06140100_0160",
      "style": 82,
      "name": "그걸 기록한 문서도 없어\n막연히 전설로만 남아 있을 뿐이야"
    },
    {
      "$id": 17,
      "label": "bf06140100_0170",
      "style": 82,
      "name": "그걸로 충분하지 않나?"
    },
    {
      "$id": 18,
      "label": "bf06140100_0180",
      "style": 82,
      "name": "역사란 막연하고 불확실한 것이다\n진상은 당사자만 알 수 있지"
    },
    {
      "$id": 19,
      "label": "bf06140100_0190",
      "style": 82,
      "name": "당신은 당사자가 아니라는 거야?"
    },
    {
      "$id": 20,
      "label": "bf06140100_0200",
      "style": 82,
      "name": "그러니 이렇게 도움을 청한 거다"
    }
  ]
}