  "rows": [
    {
      "$id": 1,
      "label": "qst200803_0010",
      "style": 82,
      "name": "호타루?\n그 나무가 뭐 어쨌는데?"
    },
    {
      "$id": 2,
      "label": "qst200803_0020",
      "style": 82,
      "name": "이 아이, 다쳤어…"
    },
    {
      "$id": 3,
      "label": "qst200803_0030",
      "style": 82,
      "name": "다쳤다고?"
    },
    {
      "$id": 4,
      "label": "qst200803_0040",
      "style": 82,
      "name": "응, 아까 그 사람들과 싸울 때\n다쳤나 봐…"
    },
    {
      "$id": 5,
      "label": "qst200803_0050",
      "style": 82,
      "name": "그렇게 걱정할 정도는 아니지 않아?\n저절로 나을 거 아냐?"
    },
    {
      "$id": 6,
      "label": "qst200803_0060",
      "style": 82,
      "name": "이 아이들도 살아 있어"
    },
    {
      "$id": 7,
      "label": "qst200803_0070",
      "style": 82,
      "name": "그냥 내버려 두면\n시들지도 모르는데"
    },
    {
      "$id": 8,
      "label": "qst200803_0080",
      "style": 82,
      "name": "그런 식으로 말하면 어떡해…"
    },
    {
      "$id": 9,
      "label": "qst200803_0090",
      "style": 82,
      "name": "…아, 저기, 미안"
    },
    {
      "$id": 10,
      "label": "qst200803_0100",
      "style": 82,
      "name": "그런 뜻으로 한 말은 아닌데…"
    },
    {
      "$id": 11,
      "label": "qst200803_0110",
      "style": 82,
      "name": "그런데…\n다친 건 알겠는데 어떻게 하면 될까모?"
    },
    {
      "$id": 12,
      "label": "qst200803_0120",
      "style": 82,
      "name": "치료할 수는 있는 거야?"
    },
    {
      "$id": 13,
      "label": "qst200803_0130",
      "style": 82,
      "name": "음…\n조사해 봐야 알겠지만"
    },
    {
      "$id": 14,
      "label": "qst200803_0140",
      "style": 82,
      "name": "약이 있으면 괜찮을 거야"
    },
    {
      "$id": 15,
      "label": "qst200803_0150",
      "style": 82,
      "name": "약이라…"
    },
    {
      "$id": 16,
      "label": "qst200803_0160",
      "style": 82,
      "name": "그거라면\n우리가 마련할 수 있을 것 같네"
    },
    {
      "$id": 17,
      "label": "qst200803_0170",
      "style": 82,
      "name": "좋아, 그럼\n그 거목을 구해 주자"
    },
    {
      "$id": 18,
      "label": "qst200803_0180",
      "style": 82,
      "name": "정말?\n구해 줄 거야?"
    },
    {
      "$id": 19,
      "label": "qst200803_0190",
      "style": 82,
      "name": "그래, 이 나무가 시들면\n슬퍼할 사람도 있을 테니까"
    },
    {
      "$id": 20,
      "label": "qst200803_0200",
      "style": 82,
      "name": "이것도 남을 돕는 일이겠지"
    },
    {
      "$id": 21,
      "label": "qst200803_0210",
      "style": 82,
      "name": "호타루도 그러면 되겠지?"
    },
    {
      "$id": 22,
      "label": "qst200803_0220",
      "style": 82,
      "name": "응\n모두 고마워"
    },
    {
      "$id": 23,
      "label": "qst200803_0230",
      "style": 82,
      "name": "그럼 이 아이에 대해\n조사해 볼게"
    }
  ]
}