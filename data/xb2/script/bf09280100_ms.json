  "rows": [
    {
      "$id": 1,
      "label": "bf09280100_0010",
      "style": 82,
      "name": "…그 힘인가!"
    },
    {
      "$id": 2,
      "label": "bf09280100_0012",
      "style": 82,
      "name": "그럼 우리도…"
    },
    {
      "$id": 3,
      "label": "bf09280100_0020",
      "style": 82,
      "name": "[ML:undisp ]"
    },
    {
      "$id": 4,
      "label": "bf09280100_0030",
      "style": 82,
      "name": "얼리게 두지 않아!"
    },
    {
      "$id": 5,
      "label": "bf09280100_0040",
      "style": 82,
      "name": "자신이 생각한 사상을 발생시키는 힘인가"
    },
    {
      "$id": 6,
      "label": "bf09280100_0050",
      "style": 82,
      "name": "근사하군"
    },
    {
      "$id": 7,
      "label": "bf09280100_0060",
      "style": 82,
      "name": "\"신\"에게 받은 그 힘을\n왜 인간을 위해 휘두르지?"
    },
    {
      "$id": 8,
      "label": "bf09280100_0070",
      "style": 82,
      "name": "낙원에 가기 위해서다!"
    },
    {
      "$id": 9,
      "label": "bf09280100_0080",
      "style": 82,
      "name": "가서 어쩔 셈이지?"
    },
    {
      "$id": 10,
      "label": "bf09280100_0090",
      "style": 82,
      "name": "사람들에게 개방하려는 건가?"
    },
    {
      "$id": 11,
      "label": "bf09280100_0095",
      "style": 82,
      "name": "나눠 줄 셈인가?"
    },
    {
      "$id": 12,
      "label": "bf09280100_0100",
      "style": 82,
      "name": "뻔한 걸 물어보지 마"
    },
    {
      "$id": 13,
      "label": "bf09280100_0110",
      "style": 82,
      "name": "나눠 준 다음엔 어쩔 셈이지?"
    },
    {
      "$id": 14,
      "label": "bf09280100_0115",
      "style": 82,
      "name": "결과는 불 보듯 뻔한데"
    },
    {
      "$id": 15,
      "label": "bf09280100_0120",
      "style": 82,
      "name": "무슨 결과를 말하는 거야!"
    },
    {
      "$id": 16,
      "label": "bf09280100_0130",
      "style": 82,
      "name": "사람들은 낙원을 모르스처럼"
    },
    {
      "$id": 17,
      "label": "bf09280100_0135",
      "style": 82,
      "name": "500년 전의 이라처럼 만들 뿐이다"
    },
    {
      "$id": 18,
      "label": "bf09280100_0140",
      "style": 82,
      "name": "그건 내가 막을 거야"
    },
    {
      "$id": 19,
      "label": "bf09280100_0150",
      "style": 82,
      "name": "네가 죽은 뒤에는 어쩔 거지?"
    },
    {
      "$id": 20,
      "label": "bf09280100_0155",
      "style": 82,
      "name": "누가 그것을 막지?"
    },
    {
      "$id": 21,
      "label": "bf09280100_0160",
      "style": 82,
      "name": "그러기 위해…"
    },
    {
      "$id": 22,
      "label": "bf09280100_0170",
      "style": 82,
      "name": "그러기 위해 너희들이 있는 거잖아!"
    },
    {
      "$id": 23,
      "label": "bf09280100_0180",
      "style": 82,
      "name": "[ML:undisp ]"
    },
    {
      "$id": 24,
      "label": "bf09280100_0190",
      "style": 82,
      "name": "그리고 그걸 같이 해내는 건 내가 아냐"
    },
    {
      "$id": 25,
      "label": "bf09280100_0195",
      "style": 82,
      "name": "다른 누군가야!"
    },
    {
      "$id": 26,
      "label": "bf09280100_0200",
      "style": 82,
      "name": "맞아\n인간은 죽으면 끝이야"
    },
    {
      "$id": 27,
      "label": "bf09280100_0210",
      "style": 82,
      "name": "하지만 그 마음과 기억은\n다른 누군가가 반드시 물려받아"
    },
    {
      "$id": 28,
      "label": "bf09280100_0220",
      "style": 82,
      "name": "그건 너희 블레이드들도 마찬가지 아냐?"
    },
    {
      "$id": 29,
      "label": "bf09280100_0230",
      "style": 82,
      "name": "다른 이와 함께 과거의 자신을 이어받아\n새로운 자신을 이루는"
    },
    {
      "$id": 30,
      "label": "bf09280100_0240",
      "style": 82,
      "name": "그런 식으로 살아왔잖아?"
    },
    {
      "$id": 31,
      "label": "bf09280100_0250",
      "style": 82,
      "name": "그럼 다를 게 없잖아"
    },
    {
      "$id": 32,
      "label": "bf09280100_0260",
      "style": 82,
      "name": "이제부터는 내가 곁에 있을게"
    },
    {
      "$id": 33,
      "label": "bf09280100_0270",
      "style": 82,
      "name": "너는 내가 지키겠다"
    }
  ]
}